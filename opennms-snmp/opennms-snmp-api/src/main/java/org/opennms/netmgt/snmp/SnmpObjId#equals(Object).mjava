    @Override
    public boolean equals(Object obj) {
        if (obj == null) { return false; }
        if (obj == this) { return true; }
        if (obj.getClass() != getClass()) {
          return false;
        }
        
        return new EqualsBuilder()
            .appendSuper(super.equals(obj))
            .append(getIds(), ((SnmpObjId)obj).getIds())
            .isEquals();
    }

