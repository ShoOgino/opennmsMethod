    private boolean hasRow() {
        if (isFinished()) {
            if (!m_pendingData.isEmpty()) {
                return true;
            }
            return false;
        } else {
            for (SnmpRowResult rr : m_pendingData.values()) {
                if (rr.isComplete()) {
                    return true;
                }
            }
            return false;
        }
    }

