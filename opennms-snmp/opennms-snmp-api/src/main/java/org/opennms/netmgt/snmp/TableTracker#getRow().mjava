    public List<SnmpResult> getRow() {
        List<SnmpResult> l = new ArrayList<SnmpResult>(m_ids.length);
        for (Queue<SnmpResult> q : m_pendingData) {
            if (q.isEmpty()) {
                l.add(null);
            } else {
                l.add(q.poll());
            }
        }
        return l;
    }

