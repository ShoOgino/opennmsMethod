    private boolean buildPdu(PDU pdu, SnmpObjId[] oids) {
        
        VariableBinding vb;
        
        for (int i=0; i<oids.length; i++) {
            vb = new VariableBinding(new OID(oids[i].toString()));
            pdu.add(vb);
        }
        
        if (pdu.getVariableBindings().size() == oids.length) {
            return true;
        } else {
            return false;
        }
        
    }

