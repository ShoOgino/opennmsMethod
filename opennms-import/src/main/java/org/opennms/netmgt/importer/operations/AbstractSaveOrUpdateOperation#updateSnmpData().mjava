	protected void updateSnmpData() {
		if (m_collector != null) {
            m_collector.run();
        }
		
		updateSnmpDataForNode();
		
		updateSnmpDataForSnmpInterfaces();
		
		for (OnmsIpInterface ipIf : m_node.getIpInterfaces()) {
            resolveIpHostname(ipIf);
            updateSnmpDataForInterface(ipIf);
		}
	}

