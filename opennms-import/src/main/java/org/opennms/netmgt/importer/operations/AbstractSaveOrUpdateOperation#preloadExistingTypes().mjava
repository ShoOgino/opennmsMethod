    private void preloadExistingTypes() {
        
        if (getTypes() == null) {
            setTypes(new HashMap());
            for (Iterator it = m_svcTypeDao.findAll().iterator(); it.hasNext();) {
                OnmsServiceType svcType = (OnmsServiceType) it.next();
                getTypes().put(svcType.getName(), svcType);
            }
        }
    }

