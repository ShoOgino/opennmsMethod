	/** {@inheritDoc} */
        @Override
	public void onEvent(Event e) {
		String oldPrefix = ThreadCategory.getPrefix();
		try {
			ThreadCategory.setPrefix(NAME);
			if (!EventConstants.RELOAD_IMPORT_UEI.equals(e.getUei())) {
				return;
			}
			doImport(e);
		} finally {
			ThreadCategory.setPrefix(oldPrefix);
		}
	}

