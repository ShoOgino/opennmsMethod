	/** {@inheritDoc} */
        @Override
	public void onEvent(Event e) {
        	
        Map mdc = MDC.getCopyOfContextMap();
		try {
			MDC.put("prefix", NAME);
			
			if (!EventConstants.RELOAD_IMPORT_UEI.equals(e.getUei())) {
				return;
			}
			doImport(e);
		} finally {
			
			MDC.setContextMap(mdc);
		}
	}

