	/** {@inheritDoc} */
        @Override
	public void onEvent(Event e) {
        	
            Map mdc = Logging.getCopyOfContextMap();
            try {
                Logging.putPrefix(NAME);

                if (!EventConstants.RELOAD_IMPORT_UEI.equals(e.getUei())) {
                    return;
                }
                doImport(e);
            } finally {
                Logging.setContextMap(mdc);
            }
        }

