    private ZoneId getZoneId(){
        ZoneId timeZoneId = (ZoneId) this.getJspContext().getAttribute(CentralizedDateTimeFormat.SESSION_PROPERTY_TIMEZONE_ID, PageContext.SESSION_SCOPE);
        if(timeZoneId == null){
            timeZoneId = ZoneId.systemDefault();
        }
        return timeZoneId;
    }

