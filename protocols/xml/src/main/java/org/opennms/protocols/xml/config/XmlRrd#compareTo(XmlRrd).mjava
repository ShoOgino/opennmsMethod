    /* (non-Javadoc)
     * @see java.lang.Comparable#compareTo(java.lang.Object)
     */
    public int compareTo(XmlRrd obj) {
        return new CompareToBuilder()
        .append(getStep(), obj.getStep())
        .append(getXmlRras().toArray(), obj.getXmlRras().toArray())
        .toComparison();
    }

