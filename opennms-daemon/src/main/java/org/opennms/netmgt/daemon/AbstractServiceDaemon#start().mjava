    /**
     * <p>start</p>
     */
    final public synchronized void start() {
        final String prefix = ThreadCategory.getPrefix();
        try {
            ThreadCategory.setPrefix(getName());
            log().info(getName()+" starting.");

            setStatus(STARTING);
            onStart();
            setStatus(RUNNING);

            log().info(getName()+" started.");
        } finally {
            ThreadCategory.setPrefix(prefix);
        }

    }

