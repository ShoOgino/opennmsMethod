	public static String cleanText(List list) {
		StringBuffer s = new StringBuffer();
		Iterator i = list.iterator();

		while (i.hasNext()) {
			String l = (String) i.next();

			s.append(l.replaceAll("\\s+", " "));
			if (l.indexOf(';') != -1) {
				s.append('\n');
			}
		}

		return s.toString();
	}

