    public boolean isDBTestEnabled() {
        String property = System.getProperty(s_runProperty);
        boolean enabled = "true".equals(property);
        if (!enabled) {
            System.out.println("Test '" + getName() + "' disabled.  Set '"
                               + s_runProperty
                               + "' property to 'true' to enable.");
        }
        return enabled;
    }

