    private void scheduleReads() {
        if (m_timer != null) {
            m_status = FINISHING;
            m_timer.cancel();
        }
        m_timer = new Timer("Ackd.JavaMailReader", true);
        TimerTask task = new TimerTask() {
            @Override
            public void run() {
                findAcks();  //should be something else, place holder for now
            }

        };
        m_timer.scheduleAtFixedRate(task, 3000, 3000);

    }

