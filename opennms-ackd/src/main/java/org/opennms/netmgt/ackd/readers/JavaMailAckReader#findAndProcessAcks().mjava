    private void findAndProcessAcks() {
        
        Collection<Acknowledgment> acks;

        try {
            List<Message> msgs = readMessages();
            acks = detectAcks(msgs);
            m_ackService.proccessAck(acks);
        } catch (JavaMailerException e) {
            // TODO Auto-generated catch block
            e.printStackTrace();
        }
        
        
        
    }

