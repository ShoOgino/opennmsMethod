    @Override
    public void connect(InetAddress address, int port, int timeout) throws IOException, Exception {
        URIBuilder ub = new URIBuilder();
        ub.setScheme(schema);
        ub.setHost(InetAddressUtils.str(address));
        ub.setPort(port);
        ub.setPath(path);
        m_httpMethod = new HttpGet(ub.build());
        setTimeout(timeout);
    }

