    /**
     * <p>sysContact</p>
     *
     * @return a Storer object.
     */
    public static Storer sysContact() {
        return new Storer() {
            @Override
            public void storeResult(ScanContext scanContext, SnmpResult res) {
                scanContext.updateSysContact(res.getValue().toDisplayString());
            }
        };
    }

