    /**
     * <p>sysObjectId</p>
     *
     * @return a Storer object.
     */
    public static Storer sysObjectId() {
        return new Storer() {
            @Override
            public void storeResult(ScanContext scanContext, SnmpResult res) {
                scanContext.updateSysObjectId(res.getValue().toDisplayString());
            }
        };
    }

