    /**
     * <p>sysLocation</p>
     *
     * @return a Storer object.
     */
    public static Storer sysLocation() {
        return new Storer() {
            @Override
            public void storeResult(ScanContext scanContext, SnmpResult res) {
                scanContext.updateSysLocation(res.getValue().toDisplayString());
            }
        };
    }

