    /**
     * <p>sysDescription</p>
     *
     * @return a Storer object.
     */
    public static Storer sysDescription() {
        return new Storer() {
            @Override
            public void storeResult(ScanContext scanContext, SnmpResult res) {
                scanContext.updateSysDescription(res.getValue().toDisplayString());
            }
        };
    }

