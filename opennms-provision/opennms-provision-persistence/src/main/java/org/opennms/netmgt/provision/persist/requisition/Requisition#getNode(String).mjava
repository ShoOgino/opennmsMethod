    public RequisitionNode getNode(String foreignId) {
        if (m_nodes != null) {
            for (RequisitionNode n : m_nodes) {
                if (n.getForeignId().equals(foreignId)) {
                    System.err.println("returning node '" + n + "' for foreign id '" + foreignId + "'");
                    return n;
                }
            }
        }
        return null;
    }

