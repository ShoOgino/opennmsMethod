    public void removeInterface(RequisitionInterface iface) {
        if (m_interfaces != null) {
            Iterator<RequisitionInterface> i = m_interfaces.iterator();
            while (i.hasNext()) {
                RequisitionInterface ri = i.next();
                if (ri.getIpAddr().equals(iface.getIpAddr())) {
                    i.remove();
                    break;
                }
            }
        }
    }

