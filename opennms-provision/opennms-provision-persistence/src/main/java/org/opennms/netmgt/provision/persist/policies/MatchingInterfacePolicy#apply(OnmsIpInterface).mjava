    public OnmsIpInterface apply(OnmsIpInterface iface) {
        if (m_ipAddress != null) {
            if (!match(iface.getIpAddress(), m_ipAddress)) {
                return null;
            }
        }
        if (m_hostName != null) {
            if (!match(iface.getIpHostName(), m_hostName)) {
                return null;
            }
        }
        return iface;
    }

