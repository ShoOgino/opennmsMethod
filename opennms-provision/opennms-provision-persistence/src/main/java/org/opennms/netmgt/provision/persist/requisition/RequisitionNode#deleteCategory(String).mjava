    /**
     * <p>deleteCategory</p>
     *
     * @param category a {@link java.lang.String} object.
     */
    public void deleteCategory(String category) {
        Iterator<RequisitionCategory> i = m_categories.iterator();
        while (i.hasNext()) {
            RequisitionCategory cat = i.next();
            if (cat.getName().equals(category)) {
                i.remove();
                break;
            }
        }
    }

