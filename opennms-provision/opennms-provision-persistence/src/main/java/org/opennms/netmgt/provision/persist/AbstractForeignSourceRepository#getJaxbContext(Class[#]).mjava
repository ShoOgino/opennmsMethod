    /**
     * <p>getJaxbContext</p>
     *
     * @param objectType a {@link java.lang.Class} object.
     * @return a {@link javax.xml.bind.JAXBContext} object.
     */
    protected JAXBContext getJaxbContext(Class<?> objectType) {
        return m_jaxbContextResolver.getContext(objectType);
    }

