    public void deleteInterface(String ipAddress) {
        if (m_interfaces != null) {
            Iterator<RequisitionInterface> i = m_interfaces.iterator();
            while (i.hasNext()) {
                RequisitionInterface iface = i.next();
                if (iface.getIpAddr().equals(ipAddress)) {
                    i.remove();
                    break;
                }
            }
        }
    }

