    /** {@inheritDoc} */
    @Override
    public int hashCode() {
        return new HashCodeBuilder(443, 1061)
            .append(getName())
            .append(getScanInterval())
            .append(getDetectors().toArray(OF_PLUGIN_CONFIGS))
            .append(getPolicies().toArray(OF_PLUGIN_CONFIGS))
            .toHashCode();
      }

