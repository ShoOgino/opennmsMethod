    @Override
    public OnmsIpInterface act(OnmsIpInterface iface) {
        switch (m_action) {
        case DO_NOT_PERSIST: 
            return null;
        case MANAGE:
            iface.setIsManaged("M");
            return iface;
        case UNMANAGE:
            iface.setIsManaged("U");
            return iface;
        default:
            return iface;    
        }
    }

