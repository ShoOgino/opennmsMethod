    /**
     * <p>validateIPMatch</p>
     *
     * @param ip a {@link java.lang.String} object.
     * @return a boolean.
     */
    public boolean validateIPMatch(String ip){
      if(IPLike.matches(m_address, ip)){
          return m_isSupported;
      }else{
        return false;  
      }
        
    }

