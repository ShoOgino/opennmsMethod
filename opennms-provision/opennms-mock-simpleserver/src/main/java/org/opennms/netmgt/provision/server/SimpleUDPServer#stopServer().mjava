    /**
     * <p>stopServer</p>
     *
     * @throws java.io.IOException if any.
     */
    public void stopServer() throws IOException {
        if(getServerSocket() != null ){
            getServerSocket().close();
        }
        if(m_serverThread != null && m_serverThread.isAlive()) { 
            
            if(m_socket != null && !m_socket.isClosed()) {
                m_socket.close();  
            }
            
        }
    }

