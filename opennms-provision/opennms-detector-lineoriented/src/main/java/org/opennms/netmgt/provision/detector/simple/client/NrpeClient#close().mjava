    /**
     * <p>close</p>
     */
    public void close() {
        Socket socket = m_socket;
        m_socket = null;
        try {
            if(socket != null) {
                socket.close();
            }
            
        } catch (final IOException e) {
            LogUtils.debugf(this, e, "failed to close socket");
        }
        
    }

