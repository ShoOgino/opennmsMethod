    @Override
    public void close() {
        try {
            if (isTcp() && tcpSocket != null) {
                tcpSocket.close();
            }
            if (isUdp() && udpSocket != null) {
                udpSocket.close();
            }
        } catch (Exception e) {
            LOG.error("Can't close detector sockets.", e);
        }
    }

