    @Test(timeout=20000)
    public void testDetectorFailWrongPort() throws Exception {
        m_detector.setPort(2000);
        m_detector.init();

        m_wireMockRule.stubFor(get(urlEqualTo("/")).willReturn(getOKResponse()));
