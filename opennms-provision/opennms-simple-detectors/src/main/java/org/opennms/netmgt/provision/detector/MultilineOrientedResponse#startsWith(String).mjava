    public boolean startsWith(String pattern) {
        String line = null;
        do {
           try {
               line = getLine();
            } catch (IOException e) {
                e.printStackTrace();
                return false;
            }
           
           if(!line.startsWith(pattern)) { return false; };
           
       }while(line !=null && line.length() > 0 && "-".equals(line.substring(3,4)));            
        
        return true;
    }

