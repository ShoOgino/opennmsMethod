    /**
     * @param node
     * @return
     */
    private Set<String> getImportedIpAddresses(OnmsNode node) {
        Set<String> ipAddrs = new LinkedHashSet<String>();
        for (OnmsIpInterface ipIf : node.getIpInterfaces()) {
            String ipAddr = ipIf.getIpAddress();
            if (ipAddr != null) {
                ipAddrs.add(ipAddr);
            }
        }
        return ipAddrs;
    }

