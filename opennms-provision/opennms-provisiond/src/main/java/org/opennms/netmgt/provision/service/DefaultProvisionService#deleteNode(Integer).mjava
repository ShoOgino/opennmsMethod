    /** {@inheritDoc} */
    @Transactional
    @Override
    public void deleteNode(final Integer nodeId) {
        final OnmsNode node = m_nodeDao.get(nodeId);

        if (node != null && shouldDelete(node)) {
            m_nodeDao.delete(node);
            m_nodeDao.flush();
            node.visit(new DeleteEventVisitor(m_eventForwarder));
        }


    }

