    /** {@inheritDoc} */
    @Transactional
    @Override
    public void insertNode(final OnmsNode node) {

        node.setDistPoller(createDistPollerIfNecessary("localhost", "127.0.0.1"));
        m_nodeDao.save(node);
        m_nodeDao.flush();

        final EntityVisitor visitor = new AddEventVisitor(m_eventForwarder);
        node.visit(visitor);
    }

