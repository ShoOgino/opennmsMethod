        public void run() {
            try {
                while(true) {
                    Runnable r = m_queue.take().get();
                    if (r != null) {
                        r.run();
                    }
                }
            } catch (InterruptedException e) {
                // TODO Auto-generated catch block
                e.printStackTrace();
            } catch (ExecutionException e) {
                // TODO Auto-generated catch block
                e.printStackTrace();
            }
        }

