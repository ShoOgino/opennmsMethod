        public void run() {
            try {
                while(true) {
                    Runnable r = m_queue.take().get();
                    //System.err.printf("Processing %s\n", r);
                    //System.err.printf("Processing %s, queue is %s\n", r, m_queue);
                    if (r != null) {
                        r.run();
                    }
                    if (m_loopDelay != null) {
                        sleep(m_loopDelay);
                    }
                }
            } catch (InterruptedException e) {
                // TODO Auto-generated catch block
                e.printStackTrace();
            } catch (ExecutionException e) {
                // TODO Auto-generated catch block
                e.printStackTrace();
            }
        }

