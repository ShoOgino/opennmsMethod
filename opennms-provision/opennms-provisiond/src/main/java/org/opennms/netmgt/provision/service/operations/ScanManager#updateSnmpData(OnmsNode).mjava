    void updateSnmpData(OnmsNode node) {
        if (getCollector() != null) 
        	getCollector().run();
        
        updateSnmpDataForNode(node);
        
        for (OnmsIpInterface ipIf : node.getIpInterfaces()) {
            resolveIpHostname(ipIf);
            updateSnmpDataforInterface(ipIf);
        }
    }

