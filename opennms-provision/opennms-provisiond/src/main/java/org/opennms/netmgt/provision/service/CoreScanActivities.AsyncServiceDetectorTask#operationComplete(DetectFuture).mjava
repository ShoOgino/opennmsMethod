        public void operationComplete(DetectFuture future) {
            try {
                boolean serviceDetected = future.isServiceDetected();
                info("Attempted to detect service %s on address %s: %s", m_detector.getServiceName(), m_address, serviceDetected);
                if (serviceDetected) {
                    serviceDetected();
                }
            } finally {
                markTaskAsCompleted();
            }
        }

