    private OnmsNode createNode(final String foreignSource) {
        OnmsNode node = new OnmsNode(m_locationDao.getDefaultLocation(), "default");
        //node.setId(nodeId);
        node.setLastCapsdPoll(new Date());
        node.setForeignSource(foreignSource);

        m_nodeDao.save(node);
        m_nodeDao.flush();
        return node;
    }

