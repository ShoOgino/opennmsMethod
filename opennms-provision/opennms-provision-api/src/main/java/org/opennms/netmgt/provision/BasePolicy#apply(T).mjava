    /**
     * <p>apply</p>
     *
     * @param iface a T object.
     * @return a T object.
     */
    public T apply(final T iface) {
        if (iface == null) {
            return null;
        }
        
        if (matches(iface)) {
            LogUtils.debugf(this, "Found Match %s for %s", iface, this);
            return act(iface);
        }
        
        LogUtils.debugf(this, "No Match Found: %s for %s", iface, this);
        return iface;
    }

