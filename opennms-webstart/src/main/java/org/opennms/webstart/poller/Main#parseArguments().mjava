    private void parseArguments() {
        if (m_args.length < 1) {
            usage();
        }
        
        String arg = m_args[0];
        arg = arg.toLowerCase();
        
        if (arg.startsWith("http")) {
            try {
                URL url = new URL(arg);
                m_url = "rmi://"+url.getHost();
                
            } catch (MalformedURLException e) {
                usage();
            }
            
        } else {
            m_url = arg;
        }
    }

