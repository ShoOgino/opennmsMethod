	/**
	 * Constructor.
	 *
	 * @param host
	 *            sets the host name to connect to.
	 * @param user
	 *            sets the username to connect with
	 * @param pass
	 *            sets the password to connect with.
	 */
	public WmiManager(final String host, final String user, final String pass) {
		m_HostName = host;
		m_Username = user;
		m_Password = pass;
		m_Domain = host;
        m_MatchType = "all";
    }

