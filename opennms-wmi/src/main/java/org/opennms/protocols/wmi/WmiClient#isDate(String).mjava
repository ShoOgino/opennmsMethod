    private static boolean isDate(String str) {
        DateFormat df = DateFormat.getDateInstance();
        Date dtm;

        // Parse the date.
        try {
            dtm = df.parse(str);
        } catch (ParseException e) {
            return false;
        }

        // If we caught an exception, we reset to null.
        if (dtm == null) {
            return false;
        }
        // If not null, then we parsed successfully.
        else {
            return true;
        }
    }

