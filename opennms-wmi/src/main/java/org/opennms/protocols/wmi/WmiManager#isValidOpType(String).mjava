	/**
	 * <p>isValidOpType</p>
	 *
	 * @param opType a {@link java.lang.String} object.
	 * @return a boolean.
	 */
	public static boolean isValidOpType(final String opType) {
		try {
		    WmiMgrOperation.valueOf(opType);
			return true;
		} catch(IllegalArgumentException e) {
			return false;
		}
	}

