    /**
     * <p>connect</p>
     */
    public void connect() {
        try {
            m_wmiClient.connect(m_agentConfig.getDomain(), m_agentConfig.getUsername(), m_agentConfig.getPassword());
        } catch(final WmiException e) {
            LogUtils.errorf(this, e, "Failed to connect to host.");
        }
    }

