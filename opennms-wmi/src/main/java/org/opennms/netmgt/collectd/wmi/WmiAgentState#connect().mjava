    /**
     * <p>connect</p>
     */
    public void connect() {
        try {
            m_wmiClient.connect(m_agentConfig.getDomain(), m_agentConfig.getUsername(), m_agentConfig.getPassword());
        } catch(WmiException e) {
            log().error("Failed to connect to host: "+ e.getMessage(), e);
        }
    }

