    /* (non-Javadoc)
     * @see org.opennms.secret.dao.ServiceDao#getService(java.lang.Long)
     */
    public InterfaceService getService(Long id) {
        
        InterfaceService svc = new InterfaceService();
        svc.setId(id);
        svc.setIpAddr("192.168.0.1");
        svc.setServiceName("HTTP-"+id);
        return svc;
        
    }

