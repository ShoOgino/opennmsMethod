    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        PingSweepResultDTO other = (PingSweepResultDTO) obj;
        return Objects.equals(this.address, other.address) && Objects.equals(this.rtt, other.rtt);
    }

