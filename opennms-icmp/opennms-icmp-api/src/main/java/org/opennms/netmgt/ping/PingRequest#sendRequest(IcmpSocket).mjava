    /**
     * Send this PingRequest through the given icmpSocket
     * @param icmpSocket
     */
    public void sendRequest(IcmpSocket icmpSocket) {
        try {
            m_expiration = System.currentTimeMillis() + m_timeout;
            icmpSocket.send(getDatagram());
        } catch (Throwable t) {
            m_callback.handleError(this, t);
        }
    }

