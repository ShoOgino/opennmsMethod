    public Reply getNextReply() throws InterruptedException {
        long waitTime = getNextExpirationTime();
        log().info("minimum wait time: " + waitTime);
        if (waitTime > 0) {
            return m_queue.remove(waitTime);
        } else if (waitTime == -1L) {
            Thread.sleep(Pinger.DEFAULT_WAIT_TIME);
        }
        return null;
    }

