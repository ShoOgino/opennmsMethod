    public PingRequest processTimeout() {
        PingRequest returnval = null;
        if (isExpired()) {
            if (getRetries() > 0) {
                returnval = new PingRequest(getAddress(), getTimeout(), getRetries() - 1, getSequenceId(), m_callback);
            } else {
                m_callback.handleTimeout(getRequest());
            }
        }
        return returnval;
    }

