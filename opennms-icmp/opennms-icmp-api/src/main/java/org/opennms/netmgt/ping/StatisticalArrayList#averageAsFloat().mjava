	public float averageAsFloat() {
		Long total = new Long(0);
		int count = 0;

		for (int i = 0; i < this.size(); i++) {
			Long value = (Long)this.get(i);
			if (value != null) {
				total += value;
				count++;
			}
		}
		
		if (total > 0 && count > 1) {
			Float average = (float)total / (float)count;
			return average.floatValue();
		}
	
		return total.floatValue();
	}

