    /**
     * Duplicates the array of bytes.
     * 
     * @param src
     *            The source bytes to duplicate.
     * 
     * @return The duplicated array of bytes.
     */
    private byte[] dup(byte[] src) {
        byte[] cpy = null;
        if (src != null) {
            cpy = new byte[src.length];
            for (int x = 0; x < src.length; x++) {
                cpy[x] = src[x];
            }
        }
        return cpy;
    }

