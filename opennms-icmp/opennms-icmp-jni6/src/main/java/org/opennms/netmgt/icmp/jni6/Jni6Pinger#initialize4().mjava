    private synchronized void initialize4() throws IOException {
        if (m_jniPinger != null) return;
        try {
            m_jniPinger = new JniPinger();
            m_jniPinger.initialize();
        } catch (final IOException ioe) {
            m_v4Error = ioe;
            throw ioe;
        } catch (final RuntimeException rte) {
            m_v4Error = rte;
            throw rte;
        }
    }

