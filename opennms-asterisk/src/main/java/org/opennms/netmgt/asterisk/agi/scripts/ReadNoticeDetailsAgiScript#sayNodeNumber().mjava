    public void sayNodeNumber() throws AgiException {
        String nodeID = null;
        nodeID = getVariable("OPENNMS_NODE");
        
        if ((nodeID != null) && (!"".equals(nodeID))) {
            log().debug("Reading node ID to user: " + nodeID);
            streamFile("node");
            streamFile("number");
            sayDigits(nodeID);
        } else {
            log().debug("No node ID available");
        }
    }

