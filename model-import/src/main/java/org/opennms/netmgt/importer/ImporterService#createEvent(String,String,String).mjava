    private Event createEvent(String uei, String parmName, String value) {
		Event e = createEvent(uei);
		Parms parms = new Parms();
		e.setParms(parms);
		
		Parm parm = new Parm();
		parm.setParmName(parmName);
		parms.addParm(parm);
		
		Value val = new Value();
		val.setContent(value);
		parm.setValue(val);
		
		return e;
    	
    }

