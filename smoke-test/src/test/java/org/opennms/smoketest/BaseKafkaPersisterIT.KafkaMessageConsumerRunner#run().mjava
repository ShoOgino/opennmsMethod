        @Override
        public void run() {

            consumer.subscribe(Arrays.asList(topicName));
            while (!closed.get()) {
                ConsumerRecords<String, byte[]> records = consumer.poll(1000);
                for (ConsumerRecord<String, byte[]> record : records) {
                    if (record.key().equals(nodeId)) {
                        setValue(record.value());
                    }
                }
            }
            consumer.close();
        }

