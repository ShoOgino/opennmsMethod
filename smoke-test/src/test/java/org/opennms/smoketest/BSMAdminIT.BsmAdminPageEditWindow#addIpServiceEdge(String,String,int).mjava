        public BsmAdminPageEditWindow addIpServiceEdge(String ipServiceText, String mapFunctionText, int weight) throws InterruptedException {
            newEdgeWindow()
                    .selectIpService(ipServiceText)
                    .selectMapFunction(mapFunctionText)
                    .weight(weight)
                    .confirm();
            Thread.sleep(250);
            return this;
        }

