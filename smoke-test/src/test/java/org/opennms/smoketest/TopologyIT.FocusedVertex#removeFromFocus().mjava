        public void removeFromFocus() {
            try {
                ui.testCase.setImplicitWait(IMPLICIT_WAIT_SECONDS, TimeUnit.SECONDS);
                getElement().findElement(By.xpath("//a[@class='icon-remove']")).click();
                waitForTransition();
            } finally {
                ui.testCase.setImplicitWait();
            }
        }

