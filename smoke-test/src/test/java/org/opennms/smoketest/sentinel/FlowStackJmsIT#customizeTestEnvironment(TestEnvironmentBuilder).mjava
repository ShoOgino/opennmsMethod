    @Override
    protected void customizeTestEnvironment(TestEnvironmentBuilder builder) {
            builder
                    .opennms()
                    .minion()
                    .es6()
                    .sentinel();

            // Enable Netflow 5 Adapter
            builder.withSentinelEnvironment()
                    .addFile(getClass().getResource("/sentinel/features-jms.xml"), "deploy/features.xml");
    }

