    @After
    @SuppressWarnings("deprecation")
    public void shutDownSelenium() throws Exception {
        if (selenium != null) {
            try {
                findElementByLink("Log out").click();
            } catch (final SeleniumException e) {
                // don't worry about it, this is just for logging out
            }
            selenium.stop();
        }
        if (m_driver != null) {
            m_driver.quit();
        }
        Thread.sleep(3000);
    }

