        public BsmAdminPageEditWindow addReductionKeyEdge(String reductionKeyText, String mapFunctionText, int weight) throws InterruptedException {
            newEdgeWindow()
                    .reductionKey(reductionKeyText)
                    .selectMapFunction(mapFunctionText)
                    .weight(weight)
                    .confirm();
            Thread.sleep(250);
            return this;
        }

