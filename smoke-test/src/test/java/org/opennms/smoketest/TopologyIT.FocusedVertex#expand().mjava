        public void expand() throws NoSuchElementException {
            try {
                ui.testCase.setImplicitWait(1, TimeUnit.SECONDS);
                getElement().findElement(By.xpath("//a[@class='gwt-Anchor icon-plus']")).click();
                waitForTransition();
            } finally {
                ui.testCase.setImplicitWait();
            }
