    /**
     * 
     * 
     * @param vNodeSetArray
     */
    public void setNodeSet(final NodeSet[] vNodeSetArray) {
        //-- copy array
        nodeSetList.clear();
        
        for (int i = 0; i < vNodeSetArray.length; i++) {
                this.nodeSetList.add(vNodeSetArray[i]);
        }
    }

