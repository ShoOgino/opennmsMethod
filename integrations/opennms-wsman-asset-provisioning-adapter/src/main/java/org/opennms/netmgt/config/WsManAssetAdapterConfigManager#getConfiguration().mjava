    /**
     * Return the configuration object.
     *
     * @return a {@link org.opennms.netmgt.config.snmpAsset.adapter.WsManAssetAdapterConfiguration} object.
     */
    protected WsManAssetAdapterConfiguration getConfiguration() {
        getReadLock().lock();
        try {
            return m_config;
        } finally {
            getReadLock().unlock();
        }
    }

