    /**
     * <p>hasSchedule</p>
     *
     * @param ipaddress a {@link java.lang.String} object.
     * @return a boolean.
     */
    public boolean hasSchedule(final String ipaddress) {
        try {
            getReadLock().lock();
            if (hasPolicyManage(ipaddress)) {
                return (getPolicyManageWithoutTesting(ipaddress).getSchedules().size() > 0);
            }
            return false;
        } finally {
            getReadLock().unlock();
        }
    }

