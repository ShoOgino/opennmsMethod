    /** {@inheritDoc} */
    @Override
    public String getType(final String sysoid) {
        try {
            getReadLock().lock();
            if (sysoid != null) {
                for (final Mapping map: mappings()) {
                    if (sysoid.startsWith(map.getSysoidMask()))
                    return map.getType();
                }
            }
            return getConfiguration().getDefaultType();
        } finally {
            getReadLock().unlock();
        }
    }

