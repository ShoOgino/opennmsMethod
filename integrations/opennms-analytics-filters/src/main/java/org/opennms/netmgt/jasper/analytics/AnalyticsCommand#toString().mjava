    public String toString() {
        StringBuilder sb = new StringBuilder();
        sb.append(CMD_IN_RRD_QUERY_STRING);
        sb.append(":");
        sb.append(m_module);
        sb.append("=");
        sb.append(m_columnNameOrPrefix);
        for (String arg : m_arguments) {
            sb.append(":");
            sb.append(arg);
        }
        return sb.toString();
    }

