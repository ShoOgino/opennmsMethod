    /**
     * <p>hasSchedule</p>
     *
     * @param ipaddress a {@link java.lang.String} object.
     * @return a boolean.
     */
    public boolean hasSchedule(String ipaddress) {
        if (hasPolicyManage(ipaddress))   
            return (getPolicyManageWithoutTesting(ipaddress).getScheduleCount() > 0);
        return false;
    }

