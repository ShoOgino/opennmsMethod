    /**
     * <p>packages</p>
     *
     * @return a {@link java.lang.Iterable} object.
     */
    public Iterable<Package> packages() {
        List<Package> pkgs = new ArrayList<Package>();
        if (hasPolicies()) {
            for (PolicyManage pm: policies() ) {
                pkgs.add(pm.getPackage());
            }
        }
        return pkgs;
    }

