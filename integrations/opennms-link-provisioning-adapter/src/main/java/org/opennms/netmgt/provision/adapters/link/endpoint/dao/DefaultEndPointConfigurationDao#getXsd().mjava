    /**
     * <p>getXsd</p>
     *
     * @return a {@link java.lang.String} object.
     */
    @Override
    public String getXsd() {
        StringResolver resolver = new StringResolver();
        try {
            m_context.generateSchema(resolver);
        } catch (IOException e) {
            LOG.debug("Unable to generate schema", e);
            return null;
        }
        return resolver.getXml();
    }

