    /**
     * <p>ping</p>
     *
     * @return a boolean.
     */
    @Override
    public boolean ping() {
        try {
            Number result = PingerFactory.getInstance().ping(getAddress());
            if (result != null) {
                return true;
            }
        } catch (Throwable e) {
            LOG.debug("Ping failed for address {}", getAddress(), e);
        }
        return false;
    }

