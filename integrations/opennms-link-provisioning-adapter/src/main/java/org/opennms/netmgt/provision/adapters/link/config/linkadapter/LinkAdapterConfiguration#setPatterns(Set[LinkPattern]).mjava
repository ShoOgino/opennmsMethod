    /**
     * <p>setPatterns</p>
     *
     * @param patterns a {@link java.util.Set} object.
     */
    public void setPatterns(final Set<LinkPattern> patterns) {
        synchronized(m_patterns) {
            if (patterns == m_patterns) return;
            m_patterns.clear();
            m_patterns.addAll(patterns);
        }
    }

