    /** {@inheritDoc} */
    @Transactional(readOnly=true)
    @Override
    public Integer getNodeId(String endPoint) {
        if (endPoint == null){
            return null;
        }
        Collection<OnmsNode> nodes = m_nodeDao.findByLabel(endPoint);
        if(nodes.size() > 0){
            return nodes.iterator().next().getId();
        }
        return null;
    }

