    /**
     * Gets the parent.
     *
     * @param parentId the parent ID
     * @return the parent
     */
    private OnmsHwEntity getParent(int parentId) {
        for (OnmsHwEntity e : entities) {
            if (e.getEntPhysicalIndex() != null && e.getEntPhysicalIndex().intValue() == parentId) {
                return e;
            }
        }
        return null;
    }

