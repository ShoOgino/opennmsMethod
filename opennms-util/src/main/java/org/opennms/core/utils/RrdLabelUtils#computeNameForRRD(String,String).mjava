    public static String computeNameForRRD(String ifname, String ifdescr) {
        String label = null;
        if (ifname != null) {
            label = AlphaNumeric.parseAndReplace(ifname, '_');
        } else if (ifdescr != null) {
            label = AlphaNumeric.parseAndReplace(ifdescr, '_');
        }
        return label;
        
    }

