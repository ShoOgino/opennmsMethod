     /**
      * <p>countChar</p>
      *
      * @param charIn a char.
      * @param stingIn a {@link java.lang.String} object.
      * @return a int.
      */
     public static int countChar(char charIn, String stingIn) {
         
         int charCount = 0;
         int charIndex = 0;
         for (int i=0; i<stingIn.length(); i++) {
             charIndex = stingIn.indexOf(charIn, i);
             if (charIndex != -1) {
                 charCount++;
                 i = charIndex +1;
             }
         }
         return charCount;
     }

