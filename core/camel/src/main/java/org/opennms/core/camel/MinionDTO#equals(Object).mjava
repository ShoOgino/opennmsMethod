	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		final MinionDTO other = (MinionDTO)obj;
		return Objects.equals(this.m_headers, other.m_headers)
				&& Arrays.equals(this.m_body, other.m_body);
	}

