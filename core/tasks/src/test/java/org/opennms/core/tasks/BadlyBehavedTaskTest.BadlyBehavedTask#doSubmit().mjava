        @Override
        public void doSubmit() {
            if (m_taskLatch != null) m_taskLatch.doSubmitLatch.countDown();
            if (m_failures.contains(Failure.DOSUBMIT)) {
                LOG.warn(m_name + " behaving badly! doSubmit()");
                throw new IllegalStateException();
            } else {
                super.doSubmit();
            }
        }

