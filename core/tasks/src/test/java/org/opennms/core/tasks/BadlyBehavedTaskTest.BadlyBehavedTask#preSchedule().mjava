        @Override
        public void preSchedule() {
            if (m_taskLatch != null) m_taskLatch.preScheduleLatch.countDown();
            if (m_failures.contains(Failure.PRESCHEDULE)) {
                LOG.warn(m_name + " behaving badly! preSchedule()");
                throw new IllegalStateException();
            } else {
                super.preSchedule();
            }
        }

