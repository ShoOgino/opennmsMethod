    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (!(o instanceof ContextKey)) {
            return false;
        }
        final ContextKey that = (ContextKey) o;
        return Objects.equals(this.context, that.context) &&
                Objects.equals(this.key, that.key);
    }

