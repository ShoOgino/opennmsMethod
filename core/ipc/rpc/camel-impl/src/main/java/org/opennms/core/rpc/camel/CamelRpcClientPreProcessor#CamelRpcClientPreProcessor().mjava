    public CamelRpcClientPreProcessor() {
        long camelJmsRequestTimeout = PropertiesUtils.getProperty(System.getProperties(), CAMEL_JMS_REQUEST_TIMEOUT_PROPERTY, 0L);

        if (camelJmsRequestTimeout < 0L) {
            LOG.error("Invalid value {} for property {} - must greater than or equal to zero!", camelJmsRequestTimeout, CAMEL_JMS_REQUEST_TIMEOUT_PROPERTY);
            camelJmsRequestTimeout = 0L;
        }

        LOG.debug("Value {} set for property {}", camelJmsRequestTimeout, CAMEL_JMS_REQUEST_TIMEOUT_PROPERTY);

        CAMEL_JMS_REQUEST_TIMEOUT = camelJmsRequestTimeout;
    }

