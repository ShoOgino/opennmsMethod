    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        final EchoResponse other = (EchoResponse) obj;
        return Objects.equals(this.message, other.message) &&
                Objects.equals(this.error, other.error);
    }

