        @Override
        public int compareTo(Delayed other) {
            long myDelay = getDelay(TimeUnit.MILLISECONDS);
            long otherDelay = other.getDelay(TimeUnit.MILLISECONDS);
            if (myDelay < otherDelay)
                return -1;
            if (myDelay == otherDelay)
                return 0;
            return 1;
        }

