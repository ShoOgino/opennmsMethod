        @Override
        public boolean equals(Object obj) {
            if (this == obj) {
                return true;
            } else if (obj == null) {
                return false;
            } else if (!(obj instanceof UDPPacket)) {
                return false;
            }
            UDPPacket other = (UDPPacket) obj;
            return Objects.equals(this.bytes, other.bytes) &&
                    Objects.equals(this.source, other.source);
        }

