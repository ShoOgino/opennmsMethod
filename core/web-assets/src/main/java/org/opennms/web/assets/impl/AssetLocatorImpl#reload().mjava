    @Override
    public void reload() {
        final Map<String,List<AssetResource>> minified = loadAssets(true);
        final Map<String,List<AssetResource>> unminified = loadAssets(false);

        if (minified != null) {
            m_minified = minified;
        }
        if (unminified != null) {
            m_unminified = unminified;
        }
    }

