    @Override
    public void reload() {
        if (providers.isEmpty()) {
            object = null;
        } else if (providers.size() == 1) {
            object = providers.iterator().next().load();
        } else {
            object = providers.stream()
                    .map(ConfigurationProviderState::load)
                    .filter(Objects::nonNull)
                    .reduce(folder)
                    .orElse(null);
        }
        LOG.debug("reloaded - conf object: {}", object);
    }

