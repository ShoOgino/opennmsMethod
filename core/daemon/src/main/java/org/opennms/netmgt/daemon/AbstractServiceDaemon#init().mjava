    /**
     * <p>init</p>
     */
    final public void init() {
        final String prefix = ThreadCategory.getPrefix();
        try {
            
            ThreadCategory.setPrefix(getName());
            log().info(getName()+" initializing.");

            onInit();

            log().info(getName()+" initialization complete.");
        } finally {
            ThreadCategory.setPrefix(prefix);
        }
    }

