    /**
     * <p>resume</p>
     */
    @Override
    final public void resume() {
        final String prefix = getPrefix();
        try {
            setPrefix(getName());
            if (!isPaused()) return;

            LOG.info("{} resuming.", getName());

            setStatus(RESUME_PENDING);
            onResume();
            setStatus(RUNNING);

            LOG.info("{} resumed.", getName());
        } finally {
            setPrefix(prefix);
        }
    }

