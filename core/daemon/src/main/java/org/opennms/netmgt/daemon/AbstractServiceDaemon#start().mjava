    /**
     * <p>start</p>
     */
    @Override
    final public synchronized void start() {
        final String prefix = getPrefix();
        try {
            setPrefix(getName());
            LOG.info("{} starting.", getName());

            setStatus(STARTING);
            onStart();
            setStatus(RUNNING);

            LOG.info("{} started.", getName());
        } finally {
            setPrefix(prefix);
        }

    }

