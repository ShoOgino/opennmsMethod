    /**
     * <p>start</p>
     */
    @Override
    final public synchronized void start() {
        
        Logging.withPrefix(getName(), new Runnable() {

            @Override
            public void run() {
                LOG.info("{} starting.", getName());

                setStatus(STARTING);
                onStart();
                setStatus(RUNNING);

                LOG.info("{} started.", getName());
            }
            
        });
        
    }

