    @Override
    public List<String> unmarshal(final String[] value) throws Exception {
        if (value == null) return null;
        final List<String> ret = new ArrayList<>();
        for (final String v : value) {
            if (v == null) {
                ret.add(null);
            } else {
                ret.add(v.trim());
            }
        }
        return ret;
    }

