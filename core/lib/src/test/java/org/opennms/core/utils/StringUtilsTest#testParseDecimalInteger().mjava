    @Test
    public void testParseDecimalInteger() {
        for (String value : new String[] {
            "1234",
            "-444",
            "-0",
            "0",
            "0000",
            String.valueOf(Integer.MAX_VALUE),
            String.valueOf(Integer.MIN_VALUE)
        }) {
            assertEquals(Integer.parseInt(value), StringUtils.parseDecimalInt(value));
        }
    }

