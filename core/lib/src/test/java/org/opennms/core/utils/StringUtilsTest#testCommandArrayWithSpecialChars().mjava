    @Test
    public void testCommandArrayWithSpecialChars() {
        String arg = "The quick fox !@#$%^&*()-+[]{}|;:<>?,./";
        String[] actual = StringUtils.createCommandArray(arg);
        assertArrayEquals(new String[]{
                "The", "quick", "fox", "!@#$%^&*()-+[]{}|;:<>?,./"
                }, actual);
    }

