    @Test
    public void testGetCopy() {
        
        String key = "key";
        String value1 = "value1";
        String value2 = "value2";
        
        assertNull(map.get(key));

        map.add(key, value1);
        map.add(key, value2);

        Set<String> copy = map.getCopy(key);
        
        assertNotSame(copy, map.get(key));
        assertEquals(copy, map.get(key));
        
        
    }

