    /* (non-Javadoc)
     * @see org.opennms.upgrade.api.OnmsUpgrade#postExecute()
     */
    @Override
    public void postExecute() throws OnmsUpgradeException {
        File zip = getBackupFile();
        if (zip.exists()) {
            log("Removing backup %s\n", zip);
            zip.delete();
        }
    }

