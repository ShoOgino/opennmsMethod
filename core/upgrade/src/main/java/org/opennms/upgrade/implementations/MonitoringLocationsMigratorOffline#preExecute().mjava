    @Override
    public void preExecute() throws OnmsUpgradeException {
        if (monitoringLocationsConfig == null) return;

        try {
            log("Backing up %s\n", configFile);
            zipFile(configFile);
        } catch (Exception e) {
            throw new OnmsUpgradeException("Can't backup " + configFile + " because " + e.getMessage());
        }
    }

