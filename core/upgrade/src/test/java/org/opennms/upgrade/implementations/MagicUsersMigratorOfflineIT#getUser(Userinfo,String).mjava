    /**
     * Gets the user.
     *
     * @param userInfo the user info
     * @param username the user name
     * @return the user
     */
    private User getUser(Userinfo userInfo, String username) {
        for (final User user : userInfo.getUsers().getUsers()) {
            if (user.getUserId().equals(username)) {
                return user;
            }
        }
        return null;
    }

