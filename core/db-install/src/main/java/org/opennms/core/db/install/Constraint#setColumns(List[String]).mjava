    /**
     * <p>setColumns</p>
     *
     * @param columns a {@link java.util.List} object.
     */
    public final void setColumns(List<String> columns) {
    	m_columns = new ArrayList<String>(columns.size());
    	for (String i : columns) {
    		m_columns.add(i.toLowerCase());
    	}
    }

