        @Override
        public String toHexString() {
            final byte[] data = getBytes();
            final StringBuilder b = new StringBuilder();
            for (int i = 0; i < data.length; ++i) {
                final int x = (int) data[i] & 0xff;
                if (x < 16) b.append("0");
                b.append(Integer.toString(x, 16).toLowerCase());
            }
            return b.toString();
        }

