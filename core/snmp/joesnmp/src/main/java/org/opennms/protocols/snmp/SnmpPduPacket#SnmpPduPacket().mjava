    /**
     * Default class constructor. Initialzies all primitive members to zero, and
     * allocates a new array list for the variables.
     * 
     */
    protected SnmpPduPacket() {
        m_command = 0;
        m_requestId = 0;
        m_errStatus = 0;
        m_errIndex = 0;
        m_variables = new ArrayList<SnmpVarBind>();
    }

