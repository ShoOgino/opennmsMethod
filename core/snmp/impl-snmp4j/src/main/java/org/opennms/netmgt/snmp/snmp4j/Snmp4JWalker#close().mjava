    @Override
    public void close() {
        if (m_session != null) {
            try {
                m_session.close();
            } catch (IOException e) {
                LOG.error("{}: Unexpected Error occured closing SNMP session for: {}", getName(), m_agentConfig, e);
            } finally {
                Snmp4JStrategy.reapSession(m_session);
            }
            m_session = null;
        }
    }

