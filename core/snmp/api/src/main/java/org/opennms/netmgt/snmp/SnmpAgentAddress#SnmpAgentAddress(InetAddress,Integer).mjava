    public SnmpAgentAddress(final InetAddress agentAddress, final Integer agentPort) {
        Assert.notNull(agentPort);
        m_address = agentAddress;
        m_port = agentPort;
        
        m_hashCode = new HashCodeBuilder(7, 15)
            .append(m_address)
            .append(m_port)
            .toHashCode();
    }

