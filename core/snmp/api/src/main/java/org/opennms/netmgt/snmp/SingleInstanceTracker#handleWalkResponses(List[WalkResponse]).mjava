    @Override
    public void handleWalkResponses(List<WalkResponse> responses) {
        // Store the result
        responses.stream()
            .flatMap(res -> res.getResults().stream())
            .filter(res -> m_oid.equals(SnmpObjId.get(res.getBase(), res.getInstance())))
            .forEach(res -> storeResult(res));
        setFinished(true);
    }

