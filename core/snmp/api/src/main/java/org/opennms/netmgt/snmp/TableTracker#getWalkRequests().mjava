    @Override
    public List<WalkRequest> getWalkRequests() {
        return m_columnTrackers.stream()
                .map(c -> {
                    WalkRequest walkRequest = new WalkRequest(c.getBase());
                    walkRequest.setMaxRepetitions(c.getMaxRepetitions());
                    return walkRequest;
                })
                .collect(Collectors.toList());
    }

