    @ElementCollection
    @JoinTable(name="monitoringLocationsTags", joinColumns = @JoinColumn(name="monitoringLocationId"))
    @Column(name="tag")
    public List<Tag> getTags() {
        if (m_tags == null) {
            return null;
        } else {
            return Collections.unmodifiableList(m_tags);
        }
    }

