    /**
     *
     *
     * @param vIncludeMbeanArray
     */
    public void setIncludeMbean(
            final String[] vIncludeMbeanArray) {
        //-- copy array
        _includeMbeanList.clear();

        for (int i = 0; i < vIncludeMbeanArray.length; i++) {
                this._includeMbeanList.add(vIncludeMbeanArray[i]);
        }
    }

