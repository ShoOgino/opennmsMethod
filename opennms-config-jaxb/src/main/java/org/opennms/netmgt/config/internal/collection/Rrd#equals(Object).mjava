    @Override
    public boolean equals(final Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (!(obj instanceof Rrd)) {
            return false;
        }
        final Rrd other = (Rrd) obj;
        if (!Arrays.equals(m_rras, other.m_rras)) {
            return false;
        }
        if (m_step != other.m_step) {
            return false;
        }
        return true;
    }

