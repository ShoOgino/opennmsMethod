    /**
     *
     *
     * @param vCompMemberArray
     */
    public void setCompMember(
            final CompMember[] vCompMemberArray) {
        //-- copy array
        _compMemberList.clear();

        for (int i = 0; i < vCompMemberArray.length; i++) {
                this._compMemberList.add(vCompMemberArray[i]);
        }
    }

