    @Override
    public boolean equals(final Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (!(obj instanceof Expression)) {
            return false;
        }
        final Expression other = (Expression) obj;
        if (m_template == null) {
            if (other.m_template != null) {
                return false;
            }
        } else if (!m_template.equals(other.m_template)) {
            return false;
        }
        return true;
    }

