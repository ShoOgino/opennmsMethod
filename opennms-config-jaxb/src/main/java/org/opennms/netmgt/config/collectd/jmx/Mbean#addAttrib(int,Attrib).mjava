    /**
     *
     *
     * @param index
     * @param vAttrib
     * @throws IndexOutOfBoundsException if the index
     * given is outside the bounds of the collection
     */
    public void addAttrib(
            final int index,
            final Attrib vAttrib)
    throws IndexOutOfBoundsException {
        this._attribList.add(index, vAttrib);
    }

