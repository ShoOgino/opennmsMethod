    @Override
    public boolean equals(final Object obj) {
        if (this == obj)
            return true;

        if (obj instanceof Rrd) {
            Rrd temp = (Rrd) obj;
            boolean equals = Objects.equals(this._step, temp._step)
                    && Objects.equals(this._rraList, temp._rraList);
            return equals;
        }
        return false;
    }

