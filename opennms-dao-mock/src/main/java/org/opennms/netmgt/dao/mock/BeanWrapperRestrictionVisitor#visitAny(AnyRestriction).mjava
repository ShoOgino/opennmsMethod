    @Override public void visitAny(final AnyRestriction restriction) {
        boolean matched = false;
        for (final Restriction r : restriction.getRestrictions()) {
            try {
                r.visit(this);
                matched = true;
                break;
            } catch (final Exception e) {
            }
        }
        if (!matched) {
            fail(restriction);
        }
    }

