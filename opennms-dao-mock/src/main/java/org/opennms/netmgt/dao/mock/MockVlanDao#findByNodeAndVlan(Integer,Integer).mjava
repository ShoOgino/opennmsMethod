    @Override
    public OnmsVlan findByNodeAndVlan(final Integer nodeId, final Integer vlanId) {
        for (final OnmsVlan vlan : findAll()) {
            if (vlan.getNode() != null && vlan.getNode().getId() != nodeId) continue;
            if (vlan.getVlanId() == vlanId) {
                return vlan;
            }
        }
        return null;
    }

