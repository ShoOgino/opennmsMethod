    public void testUnionIPAddressRangeAddRangeFollowing() {
        IPAddressSet set = new IPAddressSet(m_small);
        set = set.union(m_smaller);
        set = set.union(new IPAddressRange(m_oneDotB, m_oneDotD));
        
