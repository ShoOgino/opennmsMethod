    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        BridgePortWithMacs other = (BridgePortWithMacs) obj;
        if (m_port == null) {
            if (other.m_port != null)
                return false;
        } else if (!m_port.equals(other.m_port))
            return false;
        return true;
    }

