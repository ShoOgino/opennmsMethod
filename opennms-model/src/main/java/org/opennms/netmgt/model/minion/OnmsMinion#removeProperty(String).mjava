    public OnmsMinionProperty removeProperty(final String key) {
        OnmsMinionProperty removed = null;
        for (final OnmsMinionProperty prop : getMinionProperties()) {
            if (prop.getKey().equals(key)) {
                removed = prop;
                break;
            }
        }

        if (removed != null) {
            m_properties.remove(removed);
        }
        return removed;
    }

