    public BridgePortWithMacs getForwarder(BridgePort port) {
        Set<BridgePortWithMacs> links = new HashSet<BridgePortWithMacs>();
        m_forwarding.stream().filter( bp -> 
            bp.equals(port)).forEach(bpmi -> {
                links.add(bpmi);
            });
        return links.iterator().next();
    }

