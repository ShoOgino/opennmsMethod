    /**
     * <p>toString</p>
     *
     * @return a {@link java.lang.String} object.
     */
    @Override
    public String toString() {
        return MoreObjects.toStringHelper(this)
            .add("id", getId())
            .add("name", getName())
            .add("description", getDescription())
            .add("authorizedGroups", getAuthorizedGroups())
            .toString();
    }

