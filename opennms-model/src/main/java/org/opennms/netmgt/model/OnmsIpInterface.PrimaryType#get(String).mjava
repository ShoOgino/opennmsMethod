        public static PrimaryType get(String code) {
            if (code == null) {
                return NO_COLLECT;
            }
            code = code.trim();
            if (code.length() < 1) {
                return NO_COLLECT;
            } else if (code.length() > 1) {
                throw new IllegalArgumentException("Cannot convert string "+code+" to a collType");
            } else {
                return get(code.charAt(0));
            }
        }

