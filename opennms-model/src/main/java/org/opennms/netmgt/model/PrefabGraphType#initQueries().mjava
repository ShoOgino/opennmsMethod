    private void initQueries() {
        Collection<PrefabGraph> values = m_reportMap.values();
        Iterator<PrefabGraph> iter = values.iterator();

        PrefabGraph[] graphs = new PrefabGraph[values.size()];

        for (int i = 0; i < graphs.length; i++) {
            graphs[i] = iter.next();
        }

        m_queries = graphs;
    }

