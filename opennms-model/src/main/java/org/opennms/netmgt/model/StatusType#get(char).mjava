    public static StatusType get(char code) {
        switch (code) {
            case 'A':
                return ACTIVE;
            case 'N':
                return INACTIVE;
            case 'D':
                return DELETED;
            case 'U':
                return UNKNOWN;
            case 'B':
                return BAD;
            case 'G':
                return GOOD;
            default:
                throw new IllegalArgumentException("Cannot create statusType from code " + code);
        }
    }

