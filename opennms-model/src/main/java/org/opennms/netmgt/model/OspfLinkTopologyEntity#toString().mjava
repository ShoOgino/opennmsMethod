    @Override
    public String toString() {
        return MoreObjects.toStringHelper(this)
                .add("id", id)
                .add("nodeId", nodeId)
                .add("ospfIpAddr", ospfIpAddr)
                .add("ospfRemIpAddr", ospfRemIpAddr)
                .add("ospfIfIndex", ospfIfIndex)
                .toString();
    }

