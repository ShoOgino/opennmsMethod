        public void removeMacs(Set<String> otherMacs) {
        	Set<String> curmacs = new HashSet<String>();
    		for (String mac: getMacs()) {
    			if (otherMacs.contains(mac)) 
    				continue;
    			curmacs.add(mac);
    		}
    		role = BridgePortRole.BACKBONE;
    		macs = curmacs;
        }

