    /**
     * Gets the attribute.
     *
     * @param typeName the type name
     * @return the attribute
     */
    public OnmsHwEntityAttribute getAttribute(String typeName) {
        for (OnmsHwEntityAttribute attr : m_hwAttributes) {
            if (attr.getTypeName().equals(typeName)) {
                return attr;
            }
        }
        return null;
    }

