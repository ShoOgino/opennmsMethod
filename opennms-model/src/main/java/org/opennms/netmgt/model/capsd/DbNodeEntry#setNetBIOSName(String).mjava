    /**
     * Sets the current NetBIOS name.
     * 
     * @param name
     *            The new NetBIOS name.
     */
    public void setNetBIOSName(String name) {
        if (name != null) {
            m_nbName = name.toUpperCase();
        } else {
            m_nbName = null;
        }
        m_changed |= CHANGED_NETBIOS_NAME;
    }

