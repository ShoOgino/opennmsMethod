    /**
     * Copy constructor.
     *
     * @param pkg
     */
    public ScanReport(final ScanReport pkg) {
        m_id = pkg.getId();
        m_locale = pkg.getLocale();
        m_location = pkg.getLocation();
        m_timestamp = pkg.getTimestamp();
        m_properties = pkg.getProperties();
        m_scanReportPollResults = pkg.getPollResults();
    }

