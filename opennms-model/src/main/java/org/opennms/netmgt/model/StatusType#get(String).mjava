    public static StatusType get(String code) {
        if (code == null)
            return UNKNOWN;
        code = code.trim();
        if (code.length() < 1)
            return UNKNOWN;
        else if (code.length() > 1)
            throw new IllegalArgumentException("Cannot convert string " + code + " to a StatusType");
        else
            return get(code.charAt(0));
    }

