    /**
     * <p>getId</p>
     *
     * @return a {@link java.lang.String} object.
     */
    public ResourceId getId() {
        if (this.getParent() != null) {
            return getParent().getId().resolve(getResourceType().getName(), getName());
        } else {
            return ResourceId.get(getResourceType().getName(), getName());
        }
    }

