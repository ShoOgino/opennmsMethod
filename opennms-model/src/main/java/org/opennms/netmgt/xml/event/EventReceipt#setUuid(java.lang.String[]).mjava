    /**
     * 
     * 
     * @param vUuidArray
     */
    public void setUuid(
            final java.lang.String[] vUuidArray) {
        //-- copy array
        _uuidList.clear();
        
        for (int i = 0; i < vUuidArray.length; i++) {
                this._uuidList.add(vUuidArray[i]);
        }
    }

