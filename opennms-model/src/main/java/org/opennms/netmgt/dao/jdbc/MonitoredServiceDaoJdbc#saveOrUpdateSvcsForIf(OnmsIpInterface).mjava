    public void saveOrUpdateSvcsForIf(OnmsIpInterface iface) {
        Set svcs = iface.getMonitoredServices();
        if (!isDirty(svcs)) return;
        if (svcs instanceof JdbcSet) {
            updateSetMembers((JdbcSet)svcs);
        } else {
            removeAndAddSet(iface);
        }
    }

