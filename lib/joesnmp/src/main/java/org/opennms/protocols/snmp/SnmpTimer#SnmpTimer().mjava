    /**
     * Creates an SnmpTime object and it's internal thread that is used to
     * schedule the execution of the runnables.
     * 
     */
    SnmpTimer() {
        m_exit = false;
        m_sync = new Object();
        m_list = new LinkedList<TimeoutElement>();
        m_thread = new Thread(new Scheduler(), "SnmpTimer");

        m_thread.start();
    }

