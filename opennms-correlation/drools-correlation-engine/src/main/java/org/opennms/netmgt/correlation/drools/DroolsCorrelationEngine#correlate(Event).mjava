    /**
     * {@inheritDoc}
     */
    @Override
    public synchronized void correlate(final Event e) {
        LOG.debug("Begin correlation for Event {} uei: {}", e.getDbid(), e.getUei());
        kieSession.insert(e);
        kieSession.fireAllRules();
        LOG.debug("End correlation for Event {} uei: {}", e.getDbid(), e.getUei());
    }

