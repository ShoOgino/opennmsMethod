		@Override
		public void onEvent(final Event e) {
		    if (e.getUei().equals(EventConstants.RELOAD_DAEMON_CONFIG_UEI)) {
		        m_eventIpcManager.removeEventListener(this);
		        handleReloadEvent(e);
		        registerEventListeners();
		        return;
		    }
		    m_engine.correlate(e);
		}

