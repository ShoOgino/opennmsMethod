    /** {@inheritDoc} */
    public RequisitionDef getDef(String defName) {
        final List<RequisitionDef> defs = getDefs();
        if (defs != null) {
            for (RequisitionDef def : defs) {
                if (def.getImportName().equals(defName)) {
                    return def;
                }
            }
        }
        return null;
    }

