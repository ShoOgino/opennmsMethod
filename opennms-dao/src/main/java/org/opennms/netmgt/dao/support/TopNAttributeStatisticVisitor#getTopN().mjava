    /**
     * @return top attribute statistics (up to getCount() number)
     */
    public SortedSet<AttributeStatistic> getTopN() {
        SortedSet<AttributeStatistic> top = new TreeSet<AttributeStatistic>();
        
        for (AttributeStatistic stat : m_topN) {
            top.add(stat);
            
            if (top.size() >= m_count) {
                break;
            }
        }
        
        return top;
    }

