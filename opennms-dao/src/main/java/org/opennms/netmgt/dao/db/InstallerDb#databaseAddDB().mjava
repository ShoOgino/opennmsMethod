    public void databaseAddDB() throws Exception {
        assertUserSet();

        Statement st = getAdminConnection().createStatement();
        st.execute("CREATE DATABASE " + m_databaseName
                   + " WITH ENCODING='UNICODE'");
        st.execute("GRANT ALL ON DATABASE " + m_databaseName
                   + " TO " + m_user);
        
        st.close();
    }

