    public void populateDatabase() {
        if (POPULATE_DATABASE_IN_SEPARATE_TRANSACTION) {
            m_transTemplate.execute(new TransactionCallback<Object>() {
                public Object doInTransaction(final TransactionStatus status) {
                    doPopulateDatabase();
                    return null;
                }
            });
        } else {
            doPopulateDatabase();
        }
    }

