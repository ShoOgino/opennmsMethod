        @Override
        public PrefabGraphTypeDao reload(PrefabGraphTypeDao object,
                Resource resource) {
            try {
                return createPrefabGraphType(object.getName(), resource);
            } catch (Throwable e) {
                log().error("Could not reload configuration '" + resource
                                    + "'; nested exception: " + e, e);
                return null;
            }
        }

