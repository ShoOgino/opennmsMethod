    @Override
    public String toString() {
        StringBuilder buf = new StringBuilder();
        ConfigRange[] ranges = toArray();

        buf.append("[");
        boolean first = true;
        for(ConfigRange r : ranges) {
            if (first) {
                first = false;
            } else {
                buf.append(", ");
            }
            buf.append(r);
        }
        buf.append("]");
        
        return buf.toString();
    }

