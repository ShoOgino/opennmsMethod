    /**
     */
    public Object clone() {
        try {
            super.clone();
        } catch (CloneNotSupportedException e) {
            return null;
        }

        Global newGlobal = new Global();

        for (int i = 0; i < m_securityList.size(); i++) {
            newGlobal.addSecurity((String) m_securityList.get(i));
        }

        return newGlobal;
    }

