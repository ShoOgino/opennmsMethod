    /**
     */
    public Object clone() {
        try {
            super.clone();
        } catch (CloneNotSupportedException e) {
            return null;
        }

        Forward newForward = new Forward();

        newForward.setForward(m_forward);
        newForward.setState(m_state);
        newForward.setMechanism(m_mechanism);

        return newForward;
    }

