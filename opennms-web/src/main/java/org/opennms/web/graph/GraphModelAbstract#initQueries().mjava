    private void initQueries() {
        Collection values = m_reportMap.values();
        Iterator iter = values.iterator();

        PrefabGraph[] graphs = new PrefabGraph[values.size()];

        for (int i = 0; i < graphs.length; i++) {
            graphs[i] = (PrefabGraph) iter.next();
        }

        m_queries = graphs;
    }

