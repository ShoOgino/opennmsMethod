        @Override
        public void onSuccess(SurveillanceData data) {
            setData(data);
            
            
            if (!data.isComplete()) {
                final AsyncCallback<SurveillanceData> cb = this;
                Timer timer = new Timer() {
                    @Override
                    public void run() {
                        m_surveillanceService.getSurveillanceData(cb);
                    }
                };
                timer.schedule(2000);
            } else {
                complete();
            }
        }

