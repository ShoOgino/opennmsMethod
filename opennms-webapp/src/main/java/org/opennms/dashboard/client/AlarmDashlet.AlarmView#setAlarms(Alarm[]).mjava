        public void setAlarms(Alarm[] alarms) {
            int rows = Math.min(m_rows, alarms.length);
            
            for(int i = 1; i <= rows; i++) {
                setRow(i, alarms[i-1]);
            }
            
            for(int i = rows+1; i <= m_rows; i++) {
                clearRow(i);
            }

        }

