    /**
     * <p>onLoad</p>
     */
    @Override
    protected void onLoad() {
        if (m_loader == null) {
            m_loader = new DashletLoader();
        }
        m_titleWidget = new DashletTitle(m_title, m_loader);

        m_panel.add(m_titleWidget);
        m_panel.add(m_view);

        if(m_hasFooter){
            m_footer = new DashletFooter();
            m_panel.add(m_footer);
        }

        m_view.onDashLoad();
        
    }

