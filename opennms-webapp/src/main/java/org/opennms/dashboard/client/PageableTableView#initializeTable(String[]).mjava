    /**
     * <p>initializeTable</p>
     *
     * @param headings an array of {@link java.lang.String} objects.
     */
    protected void initializeTable(String[] headings) {
	    
	    setHeadings(headings);
	    
	    for(int i = 1; i <= getPageSize(); i++) {  
	        clearRow(i);
	    }
	}

