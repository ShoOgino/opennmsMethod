    public Alarm[] getAlarmsForSet(SurveillanceSet set) {
        try { Thread.sleep(2000); } catch (InterruptedException e) {}
        
        int alarmCount = m_random.nextInt(30);
        
        Alarm[] alarms = new Alarm[alarmCount];
        for(int i = 0; i < alarmCount; i++) {
            alarms[i] = newAlarm();
        }
        
        return alarms;
        
    }

