    static public void init(){
        
        try {
            log().debug("Setting Up RWS client");
            RWSClientApi.init();

            RWSConfigFactory.init();
            
            rwsCfgFactory = RWSConfigFactory.getInstance();
            burl = rwsCfgFactory.getUrls();
            _URL = burl[0].getServer_url();
            log().debug("RWS Url " + _URL);            
        }
        catch (Exception e) {
            
        }
    }

