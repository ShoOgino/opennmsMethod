    public Element(int mapId, int id, String type, String label,
            String iconName, int x, int y) {
        this.mapId = mapId;
        this.id = id;
        try {
            this.setType(type);
        } catch (MapsException me) {
//        	throw new MapsException();
        }
        this.label = label;
        this.iconName = iconName;
        this.x = x;
        this.y = y;
    }

