	public synchronized void saveMaps(Map[] m) throws MapsException {
		try {
			log.debug("saving maps");
			startSession();

			for (int i = 0, n = m.length; i < n; i++) {
                            saveMapInSession(m[i]);
			}
		}  catch (Exception e) {
                    log.error("Error while saving maps ");
                    rollback();
		}finally{
                    endSession();
                }
	}

