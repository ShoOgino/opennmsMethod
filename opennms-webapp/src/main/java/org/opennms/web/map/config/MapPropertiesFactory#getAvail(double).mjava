    public Avail getAvail(double avail) {
    	if (avail < 0) avail = -1; 
    	Avail rightAv= null;
    	int bestfound = -1;
    	Iterator ite = availsMap.values().iterator();
    	while (ite.hasNext()) {
    		Avail av = (Avail) ite.next();
    		if (avail > av.getMin() && avail >=bestfound) {
    			rightAv=av;
    			bestfound=av.getMin();
    		}
    	}
    	return rightAv;
    }

