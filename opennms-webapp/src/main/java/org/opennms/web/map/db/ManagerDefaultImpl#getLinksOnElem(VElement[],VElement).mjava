 	public List<VLink> getLinksOnElem(VElement[] elems,VElement elem) throws MapsException {

    	List<VLink> links = new ArrayList<VLink>();
        
    	
    	VLink[] vlinks = getLinksOnElement(elems, elem);
    	if (vlinks != null) {
            for (VLink vlink : vlinks) {
            	links.add(vlink);
            	
            }
        }
        return links;
    }    

