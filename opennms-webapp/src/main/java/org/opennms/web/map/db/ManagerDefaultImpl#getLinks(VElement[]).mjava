    public List<VLink> getLinks(VElement[] elems) throws MapsException {

    	List<VLink> links = new ArrayList<VLink>();
        
    	
    	VLink[] vlinks = getLinkArray(elems);
    	if (vlinks != null) {
            for (VLink vlink : vlinks) {
            	links.add(vlink);
            	
            }
        }
        return links;
    }    

