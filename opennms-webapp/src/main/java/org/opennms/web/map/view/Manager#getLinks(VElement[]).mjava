    public List getLinks(VElement[] elems) throws MapsException {

    	List links = new ArrayList();
        
    	
    	VLink[] vlinks = m_implManager.getLinks(elems);
    	if (vlinks != null) for (int i=0;i<vlinks.length;i++) {
    		links.add(vlinks[i]);
    		
    	}
        return links;
    }    

