    private double getNodeAvailability(Set nodes,java.util.Map availsMap) throws SQLException{
    	Iterator ite = nodes.iterator();
    	double avail = 0.0;
    	while (ite.hasNext()) {
			avail+=((Double)availsMap.get((Integer)ite.next())).doubleValue();
		}
		avail = avail/nodes.size();
		return avail;
    }

