	public void finalize() throws MapsException {
		log.debug("finalizing...");
		try {
			if (m_dbConnection != null && !m_dbConnection.isClosed())
				m_dbConnection.close();
			m_dbConnection = null;
		} catch (Exception e) {
			log.error("Exception while finalizing");
			throw new MapsException(e);
		}
	}

