	private Vector rs2MapMenuVector(ResultSet rs) throws SQLException {
		Vector mapVec = null;
		boolean firstTime = true;
		while (rs.next()) {
			if (firstTime) {
				mapVec = new Vector();
				firstTime = false;
			}

			MapMenu currMap = new MapMenu(rs.getInt("mapId"), rs
					.getString("mapName"), rs.getString("mapOwner"));
			mapVec.add(currMap);
		}
		return mapVec;
	}

