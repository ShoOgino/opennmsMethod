	/**
	 * <p>finalize</p>
	 *
	 * @throws java.lang.Throwable if any.
	 */
        @Override
	protected void finalize() throws Throwable {
		LOG.debug("Finalizing...closing db connections");
		super.finalize();
		if(opennmsConn!=null){
			Vault.releaseDbConnection(opennmsConn);
		}
		if(externalConn!=null && !externalConn.isClosed()){
			externalConn.close();
		}
	}

