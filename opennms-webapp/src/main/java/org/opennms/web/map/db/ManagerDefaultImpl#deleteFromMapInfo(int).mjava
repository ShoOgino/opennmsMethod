    private void deleteFromMapInfo(int mapId) {
        List<VMapInfo> mapinfolist = new ArrayList<VMapInfo>();
        for (VMapInfo vmapinfo: mapInfo) {
            if (vmapinfo.getId() != mapId )
                mapinfolist.add(vmapinfo);
        }
        mapInfo.clear();
        mapInfo.addAll(mapinfolist);
    }

