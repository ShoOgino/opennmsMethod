 	public List getLinksOnElem(VElement[] elems,VElement elem) throws MapsException {

    	List links = new ArrayList();
        
    	
    	VLink[] vlinks = m_implManager.getLinksOnElement(elems, elem);
    	if (vlinks != null) for (int i=0;i<vlinks.length;i++) {
    		links.add(vlinks[i]);
    		
    	}
        return links;
    }    

