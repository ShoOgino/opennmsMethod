    private void modifyRole(Role role) {
        Schedule sched = role.getSchedule(m_schedIndex);
        Time time = sched.getTime(m_timeIndex);
        sched.setName(m_user);
        sched.setType(m_type);
        time.setDay(m_day);
        time.setBegins(m_begins);
        time.setEnds(m_ends);
    }

