    private String getFilterDescriptions() {
        String desc = " (";
        
        for(int i = 0; i < m_filters.length; i++){
              desc += m_filters[i].getDescription();
              if(i < m_filters.length - 1){
                  desc += " " + m_conditionType + " "; 
              }
        }
        desc += ") ";
        return desc;
    }

