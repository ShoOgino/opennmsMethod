    private String getPortString(Integer ifindex, String ifName, String ifAlias) {
        if (ifindex == null && ifName == null && ifAlias == null)
            return null;
        String port = "";
        if (ifName != null) 
            port += ifName;
        if (ifindex != null )
            port += "(ifindex:" + ifindex + ")";
        if (ifAlias != null) {
            port += "(ifalias: " + ifAlias + ")";
        }
        return port;
    }

