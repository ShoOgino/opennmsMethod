	private String getPortString(Integer ifindex, String portName) {
	    if (ifindex != null && portName != null) {
            return portName + "(ifindex:" + ifindex + ")";
        }
	    if (portName != null ) {
            return portName;
        }
	    if (ifindex != null) {
            return "(ifindex:" + ifindex + ")";
        }
	    return "(no interface found in db)";
	}

