    private boolean hasUser(final String userName) {
        if (userName != null) {
            try {
                return m_userDao.hasUser(userName);
            } catch (MarshalException e) {
                Log.error("could not load user", e); //ignore
            } catch (ValidationException e) {
                Log.error("could not load user", e); //ignore
            } catch (IOException e) {
                Log.error("could not load user", e); //ignore
            }
        }
        return false;
    }

