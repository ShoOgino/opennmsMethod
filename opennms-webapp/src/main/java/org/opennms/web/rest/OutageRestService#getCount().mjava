    /**
     * <p>getCount</p>
     *
     * @return a {@link java.lang.String} object.
     */
    @GET
    @Produces(MediaType.TEXT_PLAIN)
    @Path("count")
    @Transactional
    public String getCount() {
        getReadLock().lock();
        try {
            return Integer.toString(m_outageDao.countAll());
        } finally {
            getReadLock().unlock();
        }
    }

