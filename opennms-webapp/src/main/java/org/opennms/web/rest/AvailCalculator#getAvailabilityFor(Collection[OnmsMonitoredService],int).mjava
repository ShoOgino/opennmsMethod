    public double getAvailabilityFor(Collection<OnmsMonitoredService> svcs, int i) {
        
        double sum = 0.0;
        for(OnmsMonitoredService svc : svcs) {
            sum += getServiceAvailCalculator(svc).getAvailability(i);
        }
        
        return svcs.size() == 0 ? 1.0 : sum / svcs.size();
    }

