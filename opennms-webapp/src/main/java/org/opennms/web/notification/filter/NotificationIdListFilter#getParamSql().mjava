    public String getParamSql() {
        StringBuilder buf = new StringBuilder(m_notificationIds.length*3 + 20);
        
        buf.append(" NOTIFICATIONS.NOTIFYID IN ");
        
        buf.append('(');
        for(int i = 0; i < m_notificationIds.length; i++){
            if(i != 0){
                buf.append(", ");
            }
            buf.append('?');
        }
        
        buf.append(')');
        
        return buf.toString();
    }

