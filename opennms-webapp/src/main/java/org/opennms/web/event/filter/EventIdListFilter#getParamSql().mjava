    public String getParamSql() {
        StringBuilder buf = new StringBuilder(m_eventIds.length*3 + 20);
        
        buf.append(" EVENTS.EVENTID IN ");
        
        buf.append('(');
        for(int i = 0; i < m_eventIds.length; i++) {
            if (i != 0) {
                buf.append(", ");
            }
            buf.append('?');
        }
        
        buf.append(')');
        
        return buf.toString();
    }

