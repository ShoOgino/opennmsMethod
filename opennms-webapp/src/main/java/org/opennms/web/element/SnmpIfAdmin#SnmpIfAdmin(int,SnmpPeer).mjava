    /**
     * Construct a SnmpIfAdmin object from a SnmpPeer object
     * 
     * @param snmpPeer
     * @param conn
     * @throws SocketException
     * @throws Exception
     */
    public SnmpIfAdmin(int nodeid, SnmpPeer snmpPeer) throws SocketException {
        m_snmpPeer = snmpPeer;
        this.nodeid = nodeid;
        openSession();
    }

