    public String getParamSql() {
        StringBuilder buf = new StringBuilder(m_values.length*3 + 20);
        
        buf.append(" " + m_fieldName + " IN ");
        
        buf.append('(');
        for(int i = 0; i < m_values.length; i++) {
            if (i != 0) {
                buf.append(", ");
            }
            buf.append('?');
        }
        
        buf.append(')');
        return buf.toString();
    }

