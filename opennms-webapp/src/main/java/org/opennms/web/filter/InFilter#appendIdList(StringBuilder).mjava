    private void appendIdList(StringBuilder buf) {
        buf.append("(");
        for(int i = 0; i < m_values.length; i++) {
            if (i != 0) {
                buf.append(", ");
            }
            buf.append(m_sqlType.formatValue(m_values[i]));
        }
        
        buf.append(")");
        
    }

