    private String getFilterSql(){
        String sql = " (";
        
        for(int i = 0; i < m_filters.length; i++){
            sql += " " + m_filters[i].getSql();
            if(i < m_filters.length - 1){
                sql += " " + m_conditionType;
            }
        }
        sql += ") ";
        return sql;
    }

