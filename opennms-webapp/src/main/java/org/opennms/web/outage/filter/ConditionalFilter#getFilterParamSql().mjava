    private String getFilterParamSql() {
        String param = " (";
        
        for(int i = 0; i < m_filters.length; i++){
            param += " " + m_filters[i].getParamSql();
            if(i < m_filters.length - 1){
                param += " " + m_conditionType;
            }
        }
        param += ") ";
        return param;
    }

