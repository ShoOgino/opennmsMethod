    @SuppressWarnings("unused")
    private static boolean isColumnValid(String column) {
        Assert.notNull(column, "argument column cannot be null");

        for (String[] assetColumn : s_columns) {
            if (column.equals(assetColumn[1])) {
                return true;
            }
        }

        return false;
    }

