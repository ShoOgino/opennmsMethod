        public EditModel(OnmsCategory category, List<OnmsNode> nodes) {
            m_category = category;
            m_nodes = nodes;
            
            for (OnmsNode node : m_category.getMemberNodes()) {
                m_nodes.remove(node);
            }
            
            m_sortedMemberNodes =
                new ArrayList<OnmsNode>(m_category.getMemberNodes());
            Collections.sort(m_sortedMemberNodes);
        }

