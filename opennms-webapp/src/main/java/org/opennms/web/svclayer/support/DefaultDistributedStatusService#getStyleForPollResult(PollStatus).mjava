    /**
     * TODO: Use the enum for these string values
     */
    private String getStyleForPollResult(PollStatus status) {
        if (status.isAvailable()) {
            return "Normal";
        } else if (status.isUnresponsive()) {
            return "Warning";
        } else if (status.isUnknown()) {
            return "Indeterminate";
        } else {
            return "Critical";
        }
    }

