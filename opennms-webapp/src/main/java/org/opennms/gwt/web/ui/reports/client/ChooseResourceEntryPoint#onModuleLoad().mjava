    public void onModuleLoad() {
        
        RootPanel panel = RootPanel.get("opennms:chooseResource");
        if(panel != null) {
            m_dataList = getDataList();
            m_view = new ChooseResourceViewImpl();
            m_view.setPresenter(this);
            m_view.setDataList(m_dataList);
            panel.add(m_view);
        }
    }

