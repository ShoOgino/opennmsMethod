	public static void main( String[] args ) {
        MibLoader loader = new MibLoader();
        Mib mib = null;
        File file;
        URL url;
        
        // Check command-line arguments
        parseCli(args);
        /*
        if (args.length < 2) {
        	printHelp("You must specify at least a MIB file or URL");
        	System.exit(1);
        }
