    /**
     * Convenience method of determining a "close" state of a ticket.
     * @param newState
     * @return true if canceled or closed state
     */
    private boolean isClosingState(State newState) {
        switch(newState) {
        case CANCELLED:
        case CLOSED:
            return true;
        case OPEN:
        default:    
            return false;
        }
        
    }

