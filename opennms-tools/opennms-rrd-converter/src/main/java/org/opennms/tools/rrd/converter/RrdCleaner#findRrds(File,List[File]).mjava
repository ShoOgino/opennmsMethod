    public void findRrds(final File directory, final List<File> files) {
        for (final File f : directory.listFiles()) {
            if (f.isDirectory()) {
                findRrds(f, files);
            } else {
                if (f.getName().endsWith(".finished")) {
                    files.add(f);
                }
                if ((f.getName().endsWith(".rrd") || f.getName().endsWith(".jrb")) && getNumOfDsNames(f) == 1) {
                    files.add(f);
                }
            }
        }
    }

