    private Double getLastValue(final String dsName) {
        Double lastValue = m_lastValue.get(dsName);
        if (lastValue == null) {
            lastValue = 0.0D;
        }
        if (LogUtils.isTraceEnabled(this)) LogUtils.tracef(this, "getLastValue(%s) = %f", dsName, lastValue);
        return lastValue;
    }

