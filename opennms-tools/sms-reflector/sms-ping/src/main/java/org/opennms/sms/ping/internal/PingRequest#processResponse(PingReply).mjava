    /**
     * <p>processResponse</p>
     *
     * @param reply a {@link org.opennms.sms.ping.internal.PingReply} object.
     * @return a boolean.
     */
    @Override
    public boolean processResponse(PingReply reply) {
        try {
            setResponseTimestamp(reply.getReceiveTimestamp());
            processResponse(reply.getPacket());
            return true;
        } finally {
            m_processed = true;
        }
    }

