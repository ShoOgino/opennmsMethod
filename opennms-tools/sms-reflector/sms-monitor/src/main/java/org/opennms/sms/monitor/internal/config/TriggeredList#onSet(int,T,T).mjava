    /**
     * <p>onSet</p>
     *
     * @param index a int.
     * @param oldElem a T object.
     * @param newElem a T object.
     */
    protected void onSet(int index, T oldElem, T newElem) {
        if (oldElem != null) {
            onRemove(index, oldElem);
        }
        if (newElem != null) {
            onAdd(index, newElem);
        }
    }

