	@Override
	public void run() {
		try {
			getBuilder().send(m_host, m_port, m_community);
			int totalTrapsSent = m_trapCounter.getAndIncrement();
			if (totalTrapsSent >= m_trapLimit) {
				throw new IllegalStateException("Reached traps to send limit.");
			}

		} catch (Exception e) {
			e.printStackTrace();
		}
	}

