     * 
     * @param vCategoryArray
     */
    public void setCategory(
            final org.opennms.client.schema.Category[] vCategoryArray) {
        //-- copy array
        _categoryList.clear();

        for (int i = 0; i < vCategoryArray.length; i++) {
                this._categoryList.add(vCategoryArray[i]);
        }
    }

