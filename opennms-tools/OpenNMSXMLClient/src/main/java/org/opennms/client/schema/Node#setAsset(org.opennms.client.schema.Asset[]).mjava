    /**
     * 
     * 
     * @param vAssetArray
     */
    public void setAsset(
            final org.opennms.client.schema.Asset[] vAssetArray) {
        //-- copy array
        _assetList.clear();

        for (int i = 0; i < vAssetArray.length; i++) {
                this._assetList.add(vAssetArray[i]);
        }
    }

