    public void append(OVObjectId oid) {
        if (oid.isNull()) {
            return;
        }

        ObjectIDByReference pOid = new ObjectIDByReference(m_oid);
        IntByReference pLen = new IntByReference(m_len);
        
        ovsnmp().OVsnmpOidAppend(pOid, pLen, oid.m_oid, oid.m_len);
        
        m_oid = pOid.getValue();
        m_len = pLen.getValue();
        
        
    }

