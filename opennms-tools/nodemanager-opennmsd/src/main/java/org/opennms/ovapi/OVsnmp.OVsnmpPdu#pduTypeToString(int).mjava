        public static String pduTypeToString(int type) {
            switch(type) {
            case GET_REQ_MSG:
                return "GET_REQ_MSG";
            case GETNEXT_REQ_MSG:
                return "GETNEXT_REQ_MSG";
            case GET_RSP_MSG:
                return "GET_RSP_MSG";
            case SET_REQ_MSG:
                return "SET_REQ_MSG";
            case V1TRAP_REQ_MSG:
                return "V1TRAP_REQ_MSG";
            case GETBULK_REQ_MSG:
                return "GETBULK_REQ_MSG";
            case INFORM_REQ_MSG:
                return "INFORM_REQ_MSG";
            case V2TRAP_REQ_MSG:
                return "V2TRAP_REQ_MSG";
            case REPORT_MSG:
                return "REPORT_MSG";
            default:    
                return "UNKNOWN";   
            }
        }

