	public double[] replaceValues(double[] values, List<Integer> indices) {
		Set<Integer> indexSet = new HashSet<>();
		for (int i : indices) {
			indexSet.add(i);
		}
		
		for (int i : indices) {
			int newIndex = walkBackwards(i, indexSet);
			if (newIndex >= 0) {
				values[i] = newIndex;
			} else {
				values[i] = Double.NaN;
			}
		}
		return null;
	}

