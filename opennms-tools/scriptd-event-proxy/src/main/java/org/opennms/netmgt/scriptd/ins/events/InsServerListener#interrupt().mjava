	@Override
	/**
	 * Stops the listener
	 */
	public void interrupt() {
        ThreadCategory log = getLog();
		try {
			listener.close();
		} catch (IOException e) {
		    log.error("Gor Error closing listener: " + e.getLocalizedMessage());
		}
		super.interrupt();
        log.info("InsServerListener Interrupted!");
	}

