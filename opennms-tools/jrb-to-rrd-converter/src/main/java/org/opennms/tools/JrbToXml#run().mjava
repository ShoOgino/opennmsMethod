    /**
     * <p>Run conversion</p>
     *
     * Run for each file in queue
     */
    public void run() {
        while (!queueClosed || !m_queue.isEmpty()) {
            while (!m_queue.isEmpty()) {
                try {
                    String path = m_queue.poll();

                    convertToXml(path);
                    convertToRrd(path);

                    m_jrbToRrdConverter.increaseConvertedFiles();
                } catch (RrdException e) {
                    e.printStackTrace();
                } catch (IOException e) {
                    e.printStackTrace();
                }
            }
        }
    }

