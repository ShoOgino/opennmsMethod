    /**
     * <p>Release RRD database reference</p>
     *
     * @param rrdDb
     * @throws IOException
     * @throws RrdException
     */
    private void releaseRrdDbReference(RrdDb rrdDb) throws IOException, RrdException {
        if (rrdDbPoolUsed) {
            RrdDbPool.getInstance().release(rrdDb);
        } else {
            rrdDb.close();
        }
    }

