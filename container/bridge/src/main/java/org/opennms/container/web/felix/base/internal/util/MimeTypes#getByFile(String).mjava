    public String getByFile(String file)
    {
        if (file == null) {
            return null;
        }

        int dot = file.lastIndexOf('.');
        if (dot < 0) {
            return null;
        }

        String ext = file.substring(dot + 1);
        if (ext.length() < 1) {
            return null;
        }

        return getByExtension(ext);
    }

