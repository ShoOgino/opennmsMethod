    @GET
    @Produces(MediaType.TEXT_PLAIN)
    @Path("{minionId}/{key}")
    @Transactional
    public String getMinionProperty(@PathParam("minionId") final String minionId, @PathParam("key") final String key) {
        readLock();
        try {
            return m_minionDao.findById(minionId).getProperties().get(key);
        } finally {
            readUnlock();
        }
    }

