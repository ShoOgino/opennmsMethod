    private ZoneId extractUserTimeZoneId(HttpSession session){
        ZoneId zoneId = null;
        if(session != null){
            zoneId = (ZoneId) session.getAttribute(CentralizedDateTimeFormat.SESSION_PROPERTY_TIMEZONE_ID);
        }
        if(zoneId == null){
            zoneId = ZoneId.systemDefault();
        }
        return zoneId;
    }

