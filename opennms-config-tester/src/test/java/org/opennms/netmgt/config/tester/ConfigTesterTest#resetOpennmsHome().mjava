    @After
    public void resetOpennmsHome() {
        if (opennmsHomeOriginal == null) {
            System.clearProperty(OPENNMS_HOME_NAME);
        } else {
            System.setProperty(OPENNMS_HOME_NAME, opennmsHomeOriginal);
        }
        if(opennmsHomeDir != null){
            opennmsHomeDir.delete();
        }
    }

