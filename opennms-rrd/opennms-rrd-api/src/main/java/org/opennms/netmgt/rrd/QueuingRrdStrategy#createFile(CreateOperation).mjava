    /*
     * (non-Javadoc)
     *
     * @see RrdStrategy#createFile(java.lang.Object)
     */
    /**
     * <p>createFile</p>
     *
     * @param op a {@link Operation} object.
     * @throws java.lang.Exception if any.
     */
    @Override
    public void createFile(CreateOperation op) throws Exception {
        if (m_queueCreates) {
            addOperation(op);
        } else {
            m_delegate.createFile(op.getData());
        }
    }

