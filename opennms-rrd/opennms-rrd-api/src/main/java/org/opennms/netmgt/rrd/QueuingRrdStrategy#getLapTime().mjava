    /**
     * <p>getLapTime</p>
     *
     * @return a {@link java.lang.String} object.
     */
    public String getLapTime() {
        long newLap = System.currentTimeMillis();
        double seconds = (newLap - lastLap) / 1000.0;
        lastLap = newLap;
        return "[" + seconds + " sec]";
    }

