    /**
     * <p>getLapTime</p>
     *
     * @return a {@link java.lang.String} object.
     */
    private String getLapTime() {
        final long newLap = System.currentTimeMillis();
        final double seconds = (newLap - lastLap) / 1000.0;
        lastLap = newLap;
        return "[" + seconds + " sec]";
    }

