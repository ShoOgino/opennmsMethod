        assertEquals("graph printLines - DEF item 2", "1.600000e+07", printLines[2]);
        assertEquals("graph printLines - DEF item 3", "9.896721e+09", printLines[3]);
        assertEquals("graph printLines - DEF item 4", "9.574000e+03", printLines[4]);

