    @Override
    public Component getWallboardComponent() {
        if (m_gridLayout == null) {
            m_gridLayout = new GridLayout();
            m_gridLayout.setCaption(getName());
            m_gridLayout.setSizeFull();
            m_gridLayout.setColumns(1);
            m_gridLayout.setRows(1);
        }

        return m_gridLayout;
    }

