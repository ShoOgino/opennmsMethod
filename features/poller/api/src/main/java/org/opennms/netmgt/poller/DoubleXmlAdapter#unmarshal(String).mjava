    @Override
    public Double unmarshal(final String v) throws Exception {
        if (v == null || "".equals(v)) {
            return null;
        }
        if ("NaN".equals(v)) {
            return Double.NaN;
        }
        try {
            return Double.valueOf(v);
        } catch (final NumberFormatException e) {
            LOG.warn("Unable to convert unhandled value '{}' to a Double.  Returning null instead.", v);
            return null;
        }
    }

