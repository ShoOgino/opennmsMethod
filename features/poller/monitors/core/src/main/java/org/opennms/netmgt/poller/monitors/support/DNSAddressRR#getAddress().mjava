    /**
     * <P>
     * Returns the address from the address record as a byte array.
     * </P>
     *
     * @return The address as a byte array.
     */
    public byte[] getAddress() {
        byte[] ip = new byte[4];
        for (int j = 0; j < 4; j++)
            ip[j] = (byte) (ipAddress[j]);
        return ip;
    }

