    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        PollerResponseDTO other = (PollerResponseDTO) obj;
        return Objects.equals(this.pollStatus, other.pollStatus) &&
                Objects.equals(this.error, other.error);
    }

