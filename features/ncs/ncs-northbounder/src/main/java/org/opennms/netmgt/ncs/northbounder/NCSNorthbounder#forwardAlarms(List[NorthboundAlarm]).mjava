    @Override
    public void forwardAlarms(List<NorthboundAlarm> alarms) throws NorthbounderException {
    	
    	if (!m_config.isEnabled()) return;
    	
        LogUtils.infof(this, "Forwarding %d alarms", alarms.size());
  
        HttpEntity entity = createEntity(alarms);
        
        postAlarms(entity);
    }

