    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        SystemDefinition other = (SystemDefinition) obj;
        return Objects.equals(this.rule, other.rule) &&
                Objects.equals(this.includeGroup, other.includeGroup) &&
                Objects.equals(this.name, other.name);
    }

