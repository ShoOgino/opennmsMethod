    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        Rrd other = (Rrd) obj;
        return Objects.equals(this.rra, other.rra) &&
                Objects.equals(this.step, other.step);
    }

