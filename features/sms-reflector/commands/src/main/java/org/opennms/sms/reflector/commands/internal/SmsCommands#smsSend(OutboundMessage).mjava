    /**
     * <p>smsSend</p>
     *
     * @param msg a {@link org.smslib.OutboundMessage} object.
     */
    public void smsSend(final OutboundMessage msg){
        try{
            m_service.sendMessage(msg);
        }catch(final Exception e){
            LogUtils.debugf(this, e, "error sending message (%s)", msg);
        }
    }

