    /**
     * <p>stopService</p>
     */
    public void stopService(){
        if (m_service != null){
            try {
                m_service.stopService();
            } catch (Exception e) {
                ThreadCategory.getInstance(SmsCommands.class).debug("Exception Stopping Service Occurred", e);
                //intp.printStackTrace(e);
            }
        }
    }

