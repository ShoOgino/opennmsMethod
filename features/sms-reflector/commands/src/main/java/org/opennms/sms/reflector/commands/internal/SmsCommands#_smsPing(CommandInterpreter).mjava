    public Object _smsPing(CommandInterpreter intp) {
        try {
            Long latency = SmsPinger.ping(intp.nextArgument());

            if(latency == null){
                intp.println("Ping Timedout");
            }else{
                intp.println("Ping roundtrip time: " + latency);
            }

        } catch (Exception e) {
            intp.printStackTrace(e);
        }
        return null;
    }

