    public String toString() {
        return new ToStringBuilder(this)
            .append("ID", m_id)
            .append("Queue", m_queue)
            .append("Created", m_created)
            .append("Requestors", StringUtils.join(m_requestors, ", "))
            .append("Status", m_status)
            .append("Subject", m_subject)
            .append("Text", m_text)
            .toString();
    }

