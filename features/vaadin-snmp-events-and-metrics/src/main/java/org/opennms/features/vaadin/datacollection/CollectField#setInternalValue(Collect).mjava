    /* (non-Javadoc)
     * @see com.vaadin.ui.AbstractField#setInternalValue(java.lang.Object)
     */
    @Override
    protected void setInternalValue(Collect value) {
        listField.removeAllItems();
        for (String group : value.getIncludeGroups()) {
            listField.addItem(group);
        }
    }

