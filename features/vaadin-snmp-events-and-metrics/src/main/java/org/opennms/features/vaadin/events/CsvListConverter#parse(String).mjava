    /* (non-Javadoc)
     * @see org.vaadin.addon.customfield.PropertyConverter#parse(java.lang.Object)
     */
    @Override
    public ArrayList<String> parse(String fieldValue) {
        ArrayList<String> list = new ArrayList<String>();
        if (fieldValue != null) {
            for (String s : fieldValue.split(",")) {
                list.add(s);
            }
        }
        return list;
    }

