    /* (non-Javadoc)
     * @see com.vaadin.ui.AbstractField#getValue()
     */
    @Override
    public ArrayList<Varbind> getValue() {
        ArrayList<Varbind> beans = new ArrayList<Varbind>();
        for (Object itemId: container.getItemIds()) {
            beans.add(container.getItem(itemId).getBean());
        }
        return beans;
    }

