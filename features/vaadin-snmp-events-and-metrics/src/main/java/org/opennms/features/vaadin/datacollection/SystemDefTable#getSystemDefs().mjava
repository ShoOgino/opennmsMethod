    /**
     * Gets the systemDefs.
     *
     * @return the systemDefs
     */
    public List<SystemDef> getSystemDefs() {
        List<SystemDef> systemDefs = new ArrayList<SystemDef>();
        for (String itemId : getContainer().getItemIds()) {
            systemDefs.add(getContainer().getItem(itemId).getBean());
        }
        return systemDefs;
    }

