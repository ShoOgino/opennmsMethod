    /* (non-Javadoc)
     * @see com.vaadin.ui.AbstractField#getValue()
     */
    @Override
    public Rrd getValue() {
        Rrd dto = new Rrd();
        dto.setStep(new Integer((String) step.getValue()));
        for (Object itemId: container.getItemIds()) {
            dto.addRra(container.getItem(itemId).getBean().getRra());
        }
        return dto;
    }

