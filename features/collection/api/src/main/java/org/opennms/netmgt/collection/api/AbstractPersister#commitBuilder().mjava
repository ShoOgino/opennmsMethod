    /**
     * <p>commitBuilder</p>
     */
    public void commitBuilder() {
        if (isPersistDisabled()) {
            LOG.debug("Persist disabled for {}", m_builder.getName());
            return;
        }
        try {
            m_builder.commit();
        } catch (PersistException e) {
            LOG.error("Unable to persist data for {}", m_builder.getName(), e);
        }
        m_builder = null;
    }

