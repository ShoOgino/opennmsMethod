    /**
     * <p>isValidInterfaceResource</p>
     *
     * @return a boolean.
     */
    public boolean isValidInterfaceResource() {
        try {
            if(m_ifindex == null) {
                return false;
            } else if(Integer.parseInt(m_ifindex) < 0) {
                return false;
            }
        } catch(Throwable e) {
            return false;
        }
        return true;
    }

