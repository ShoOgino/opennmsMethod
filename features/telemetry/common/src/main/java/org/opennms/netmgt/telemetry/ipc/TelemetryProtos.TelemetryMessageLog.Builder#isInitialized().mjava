      public final boolean isInitialized() {
        if (!hasLocation()) {
          
          return false;
        }
        if (!hasSystemId()) {
          
          return false;
        }
        for (int i = 0; i < getMessageCount(); i++) {
          if (!getMessage(i).isInitialized()) {
            
            return false;
          }
        }
        return true;
      }

