    private boolean isKarafOk() {
        Class serviceClass = ReportStoreService.class;
        try {
            final ReportStoreService service = SERVICE_LOOKUP.lookup(serviceClass, null);
            return service != null;
        } catch (Exception e) {
            LOG.warn("Could not lookup {}, will fail.", serviceClass.getName(), e);
            return false;
        }
    }

