    private static void buildKeysRecursive(Node n, String prefix, Map<String,String> m) {
        n.value.ifPresent(v -> {
            m.put(prefix, v);
        });

        // recurse to the leaves
        for(Map.Entry<String, Node> e: n.children.entrySet()) {
            buildKeysRecursive(e.getValue(), prefix + "." + e.getKey(), m);
        }
    }

