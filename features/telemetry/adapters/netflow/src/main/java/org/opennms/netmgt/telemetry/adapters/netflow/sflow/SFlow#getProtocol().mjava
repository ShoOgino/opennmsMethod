    @Override
    public Integer getProtocol() {
        return first(get(document, "flows", "0:3", "protocol"),
                     get(document, "flows", "0:4", "protocol"),
                     get(document, "flows", "0:1", "ipv4", "protocol"),
                     get(document, "flows", "0:1", "ipv6", "protocol"))
                .map(v -> v.asInt32().getValue())
                .orElse(null);
    }

