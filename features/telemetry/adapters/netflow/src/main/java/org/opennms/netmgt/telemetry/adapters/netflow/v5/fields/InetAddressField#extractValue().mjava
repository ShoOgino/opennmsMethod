    @Override
    protected String extractValue() {
        byte[] bytes = getFieldBytes();
        if (bytes == null ||bytes.length != 4) {
            throw new IllegalStateException("Could not parse field to InetAddress");
        }
        try {
            return InetAddress.getByAddress(bytes).toString().replaceFirst("/", "");
        } catch (UnknownHostException e) {
            throw new IllegalArgumentException("Cannot parse field to InetAddress", e);
        }
    }

