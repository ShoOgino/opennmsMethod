    @Override
    public Integer getTos() {
        return first(get(document, "flows", "0:3", "tos"),
                     get(document, "flows", "0:4", "tos"),
                     get(document, "flows", "0:1", "ipv4", "tos"),
                     get(document, "flows", "0:1", "ipv6", "tos"))
                .map(v -> v.asInt32().getValue())
                .orElse(null);
    }

