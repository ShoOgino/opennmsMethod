    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (o == null || getClass() != o.getClass()) return false;
        Rrd rrd = (Rrd) o;
        return Objects.equals(step, rrd.step) &&
                Objects.equals(rras, rrd.rras) &&
                Objects.equals(baseDir, rrd.baseDir);
    }

