    @Test
    public void testOutOfOrder() {
        final SequenceNumberTracker tracker = new SequenceNumberTracker(32);
        for (int x = 0; x <= tracker.getPatience() * 2; x += 2) {
            assertTrue("x=" + (x + 2), tracker.verify(x + 2));
            assertTrue("x=" + (x + 1), tracker.verify(x + 1));
        }
    }

