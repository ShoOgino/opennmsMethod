    private void saveOrUpdateAsnInfo(BmpAsnInfo bmpAsnInfo) {
        if (bmpAsnInfo != null) {
            try {
                bmpAsnInfoDao.saveOrUpdate(bmpAsnInfo);
            } catch (Exception e) {
                LOG.error("Exception while persisting BMP ASN Info  {}", bmpAsnInfo, e);
            }
        }
    }

