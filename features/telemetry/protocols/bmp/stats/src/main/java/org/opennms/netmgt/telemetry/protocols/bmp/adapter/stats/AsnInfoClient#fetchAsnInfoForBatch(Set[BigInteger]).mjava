    private Set<BmpAsnInfo> fetchAsnInfoForBatch(Set<BigInteger> asnSet) {
        Set<BmpAsnInfo> bmpAsnInfos = new HashSet<>();
        asnSet.forEach(asn -> {
            BmpAsnInfo bmpAsnInfo = fetchAndBuildAsnInfo(asn);
            if (bmpAsnInfo != null) {
                bmpAsnInfos.add(bmpAsnInfo);
            }
        });
        return bmpAsnInfos;
    }

