        /**
         * @param value The numeric wire value of the corresponding enum entry.
         * @return The enum associated with the given numeric wire value.
         */
        public static Origin forNumber(int value) {
          switch (value) {
            case 0: return IGP;
            case 1: return EGP;
            case 2: return INCOMPLETE;
            default: return null;
          }
        }

