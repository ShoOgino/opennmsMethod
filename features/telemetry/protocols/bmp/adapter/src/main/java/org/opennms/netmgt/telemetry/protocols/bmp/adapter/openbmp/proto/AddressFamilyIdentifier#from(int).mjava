    public static AddressFamilyIdentifier from(final int code) {
        if (code == 0 || code == 65535) {
            return RESERVED;
        }
        if ((code >= 31 && code <= 16383) || (code >= 16398 && code <= 65534)) {
            return UNASSIGNED;
        }
        return Arrays.stream(values()).filter(e -> e.code == code).findFirst().get();
    }

