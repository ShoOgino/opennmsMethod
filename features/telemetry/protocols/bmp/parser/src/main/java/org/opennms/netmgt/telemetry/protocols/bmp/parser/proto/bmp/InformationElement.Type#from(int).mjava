        private static Type from(final int type) {
            switch (type) {
                case 0:
                    return STRING;
                case 1:
                    return SYS_DESCR;
                case 2:
                    return SYS_NAME;
                case 3:
                    return VRF_TABLE_NAME;
                case 4:
                    return ADMIN_LABEL;
                case 65531:
                    return BGP_ID;
                default:
                    BmpParser.LOG.warn("Unknown Information Element Type: {}", type);
                    return UNKNOWN;
            }
        }

