        private static Type from(final int type) {
            switch (type) {
                case 0:
                    return STRING;
                case 1:
                    return SYS_DESCR;
                case 2:
                    return SYS_NAME;
                default:
                    BmpParser.LOG.warn("Unknown Information Element Type: {}", type);
                    return UNKNOWN;
            }
        }

