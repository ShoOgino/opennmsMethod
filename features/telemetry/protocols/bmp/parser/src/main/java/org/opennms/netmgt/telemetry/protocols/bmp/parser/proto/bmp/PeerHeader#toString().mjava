    @Override
    public String toString() {
        return MoreObjects.toStringHelper(this)
                .add("type", this.type)
                .add("config", this.flags)
                .add("distinguisher", this.distinguisher)
                .add("address", this.address)
                .add("as", this.as)
                .add("id", this.id)
                .add("timestamp", this.timestamp)
                .toString();
    }

