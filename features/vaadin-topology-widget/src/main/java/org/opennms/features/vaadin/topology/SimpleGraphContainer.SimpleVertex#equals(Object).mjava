		@Override
		public boolean equals(Object obj) {
			if (this == obj)
				return true;
			if (obj == null)
				return false;
			if (getClass() != obj.getClass())
				return false;
			SimpleVertex other = (SimpleVertex) obj;
			if (m_id == null) {
				if (other.m_id != null)
					return false;
			} else if (!m_id.equals(other.m_id))
				return false;
			return true;
		}

