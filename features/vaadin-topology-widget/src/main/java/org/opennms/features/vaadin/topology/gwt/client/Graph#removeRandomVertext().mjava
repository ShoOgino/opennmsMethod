    public void removeRandomVertext() {
        int random = (int) ((Math.random() * (m_vertices.size() - 2)) + 1);
        Vertex vert = m_vertices.remove(random);
        
        Iterator<Edge> it = m_edges.iterator();
        while(it.hasNext()) {
            Edge edge = it.next();
            if(edge.getSource() == vert || edge.getTarget() == vert) {
                it.remove();
            }
        }
        
    }

