    public void updateFromUIDL(UIDL uidl, ApplicationConnection client) {
        
        if(client.updateComponent(this, uidl, true)) {
            
            return;
        }
        
        m_client = client;
        m_paintableId = uidl.getId();
        
        drawCircles(uidl.getIntArrayVariable("dataArray"));
    }

