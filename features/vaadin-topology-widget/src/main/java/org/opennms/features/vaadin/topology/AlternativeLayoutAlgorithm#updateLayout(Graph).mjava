    public void updateLayout(Graph graph) {
        Vertex vertex = graph.getVertices().get(0);
        //Set<Vertex> neighbors = getNeighbors(graph, vertex);
        List<Vertex> vertices = graph.getVertices();
        int i = 0;
        for(Vertex v : vertices) {
            
            v.setX(50 + (50*i));
            v.setY(50);
            i++;
        }
    }

