	public Action[] getActions() {
		if(m_actions == null) {
			return new Action[] {};
		}
		final Action[] actions = new Action[m_actions.length];
		for(int i = 0; i < actions.length; i++) {
			String actionKey = m_actions[i];
			GraphAction a = new GraphAction(this);
			a.setCaption(actionKey);
			
			actions[i] = a;
			
		}
		
		return actions;
	}

