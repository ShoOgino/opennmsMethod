    public void removeVertex(Vertex target) {
        if (m_vertices.size() <= 0) return;
        
        Iterator<Vertex> it = m_vertices.iterator();
        while(it.hasNext()) {
            Vertex vertex = it.next();
            if(vertex == target) {
                it.remove();
                
            }
        }
        removeEdges(target);
    }

