    public void removeVertex(Vertex target) {
        if (getVertices().size() <= 0) return;
        
        Iterator<Vertex> it = getVertices().iterator();
        while(it.hasNext()) {
            Vertex vertex = it.next();
            if(vertex == target) {
                it.remove();
                
            }
        }
        removeEdges(target);
    }

