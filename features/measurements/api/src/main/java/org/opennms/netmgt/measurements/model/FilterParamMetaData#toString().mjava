    @Override
    public String toString() {
       return com.google.common.base.Objects.toStringHelper(this)
                 .add("Name", this.name)
                 .add("Type", this.type)
                 .add("Description", this.description)
                 .add("Default", this.defaultValue)
                 .add("Required", this.required)
                 .toString();
    }

