        LOG.debug("using RRD base directory: {}", s_rrdDirectory.getAbsolutePath());
        System.setProperty("rrd.base.dir", s_rrdDirectory.getAbsolutePath());
        System.setProperty("rrd.binary", findRrdtool());

        assumeRrdtoolExists("jrrd");
    }

