    public String getIconKey() {
        if (m_node.getSysObjectId() == null) {
            return "linkd.system";
        }
        if (m_node.getSysObjectId().startsWith(".")) {
            return "linkd.system.snmp" + m_node.getSysObjectId();
        }
        return "linkd.system.snmp." + m_node.getSysObjectId();
    }

