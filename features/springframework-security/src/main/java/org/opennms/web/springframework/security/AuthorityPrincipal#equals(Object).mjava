    @Override
    public boolean equals(final Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (obj instanceof AuthorityPrincipal) {
            final AuthorityPrincipal other = (AuthorityPrincipal) obj;
            return Objects.equals(m_name, other.m_name);
        }
        return false;
    }

