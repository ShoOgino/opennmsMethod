    public Credentials(String username, String password, Map<String, String> attributes) {
        m_username = username;
        m_password = password;
        if (attributes == null) {
            m_attributes = ImmutableMap.copyOf(Collections.emptyMap());
        } else {
            m_attributes = ImmutableMap.copyOf(Objects.requireNonNull(attributes));
        }
    }

