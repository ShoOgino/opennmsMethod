        @Override
        public String toString() {
            return new StringJoiner(", ", NewtsLikeSampleAggregatorBuilder.class.getSimpleName() + "[", "]")
                    .add("resource=" + resource)
                    .add("start=" + start)
                    .add("end=" + end)
                    .add("currentSources=" + currentSources)
