    public static Iterator<Results.Row<org.opennms.newts.api.Sample>> samplesToNewtsRowIterator(List<Sample> allSamples, final List<Source> listOfSources) {
        return allSamples
                .stream()
                .map(NewtsConverterUtils::sampleToRow)
                .collect(Collectors.toList())
                .iterator();
    }

