    @Test
    public void testSortByAverageUnsuccessfulCollectionTime() {

        Collector c = new Collector();
        c.setSortColumn(SortColumn.AVGUNSUCCESSCOLLECTTIME);
        c.addLog("2010-03-13 02:21:30,000 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: begin: example1/0/1.1.1.1/SNMP");
        c.addLog("2010-03-13 02:21:35,000 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: error: example1/0/1.1.1.1/SNMP");
        c.addLog("2010-03-13 02:21:40,000 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: end: example1/0/1.1.1.1/SNMP");
        c.addLog("2010-03-13 02:22:20,976 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: begin: example1/0/2.2.2.2/SNMP");
        c.addLog("2010-03-13 02:22:25,976 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: error: example1/0/2.2.2.2/SNMP");
        c.addLog("2010-03-13 02:22:40,976 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: end: example1/0/2.2.2.2/SNMP");
        c.addLog("2010-03-13 02:23:31,976 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: begin: example1/0/2.2.2.2/SNMP");
        c.addLog("2010-03-13 02:23:35,976 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: error: example1/0/2.2.2.2/SNMP");
        c.addLog("2010-03-13 02:23:46,976 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: end: example1/0/2.2.2.2/SNMP");
        c.addLog("2010-03-13 02:24:30,000 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: begin: example1/0/3.3.3.3/SNMP");
        c.addLog("2010-03-13 02:24:50,000 DEBUG [CollectdScheduler-400 Pool-fiber51] o.o.n.c.DefaultCollectdInstrumentation: collector.collect: end: example1/0/3.3.3.3/SNMP");
        List<ServiceCollector> collectors = c.getServiceCollectors();
        assertEquals(3, collectors.size());
        assertEquals(17500, collectors.get(0).getAverageErrorCollectionTime());
        assertEquals(10000, collectors.get(1).getAverageErrorCollectionTime());
        assertEquals(0,  collectors.get(2).getAverageErrorCollectionTime());

    }

