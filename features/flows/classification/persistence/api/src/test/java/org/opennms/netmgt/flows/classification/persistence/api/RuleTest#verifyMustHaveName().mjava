    @Test
    public void verifyMustHaveName() {
        Rule rule = new Rule();
        assertThat(rule.isValid(), is(false));

        rule.setPort("80");
        assertThat(rule.isValid(), is(false));

        rule.setIpAddress("10.10.10.10");
        assertThat(rule.isValid(), is(false));

        rule.setProtocol("tcp");
        assertThat(rule.isValid(), is(false));

        rule.setId(5);
        assertThat(rule.isValid(), is(false));

        rule.setName("http");
        assertThat(rule.isValid(), is(true));
    }

