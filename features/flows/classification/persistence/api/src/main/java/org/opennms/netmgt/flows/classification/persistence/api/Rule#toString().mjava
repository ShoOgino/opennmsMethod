    @Override
    public String toString() {
        return MoreObjects.toStringHelper(getClass())
            .add("name", name)
            .add("dstAddress", dstAddress)
            .add("dstPort", dstPort)
            .add("srcAddress", srcAddress)
            .add("srcPort", srcPort)
            .add("exporterFilter", exporterFilter)
            .add("protocol", protocol)
            .add("position", position)
            .add("group", group)
            .toString();
    }

