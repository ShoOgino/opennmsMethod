    public static <T> BytesInOut sum(List<TrafficSummary<T>> summaries) {
        BytesInOut bytes = new BytesInOut();
        for (TrafficSummary<?> summary : summaries) {
            bytes.bytesIn += summary.getBytesIn();
            bytes.bytesOut += summary.getBytesOut();
        }
        return bytes;
    }

