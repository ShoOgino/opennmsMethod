    @Override
    public List<CredentialsScope> getCredentials() {
        if (Files.exists(configPath)) {
            final ElasticFlowRepositoryCredentialsWrapper credentialsWrapper = JAXB.unmarshal(configPath.toFile(),
                    ElasticFlowRepositoryCredentialsWrapper.class);
            return credentialsWrapper.getCredentialsScopes();
        }
        return new ArrayList<>();
    }

