    public static NetflowVersion from(Flow.NetflowVersion version) {
        if (version == null) return null;

        switch (version) {
            case V5:
                return V5;
            case V9:
                return V9;
            case IPFIX:
                return IPFIX;
            case SFLOW:
                return SFLOW;
            default:
                throw new IllegalArgumentException("Unknown protocol version: " + version.name());
        }
    }

