    public String getSeriesFromTopNQuery(List<String> topN, long start, long end, long step, String groupByTerm) {
        return render("top_n_series.ftl", ImmutableMap.builder()
                .put("start", start)
                .put("end", end)
                .put("topN", topN)
                .put("step", step)
                .put("groupByTerm", groupByTerm)
                .build());
    }

