    private static Map<Long, Double> mergeSeries(Map<Long, Double> m1, Map<Long, Double> m2) {
        HashMap<Long, Double> res = new HashMap<>();
        res.putAll(m1);
        m2.forEach((l, d) -> {
            Double o = res.get(l);
            res.put(l, o != null ? o + d : d);
        });
        return res;
    }

