    private static <K> TrafficSummary<K> flowDoc2TrafficSummary(FlowDocument fd, K key) {
        return TrafficSummary
                .from(key)
                .withBytes(
                        fd.getDirection() == Direction.INGRESS ? fd.getBytes() : 0,
                        fd.getDirection() == Direction.EGRESS ? fd.getBytes() : 0)
                .build();
    }

