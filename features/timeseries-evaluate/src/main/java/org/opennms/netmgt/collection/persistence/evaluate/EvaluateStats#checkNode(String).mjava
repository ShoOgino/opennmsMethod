    /**
     * Checks a node.
     *
     * @param nodeId the node identifier
     */
    public void checkNode(final String nodeId) {
        if (nodeId.startsWith("fs") || nodeId.matches("\\d+")) {
            nodeMap.putIfAbsent(nodeId, true);
        } else {
            interfaceMap.putIfAbsent(nodeId, true);
        }
    }

