    @Test
    public void testAddManyPoints() {
        BoundsBuilder bldr = new BoundsBuilder();
        
        
        for(int i = 0; i <= 10; i++) {
            bldr.extend(0.0 + i, 0.0);
            bldr.extend(0.0, 0.0 - i);
        }
        
        assertEquals(new GWTBounds(0.0, -10.0, 10.0, 0.0), bldr.getBounds());
        
    }

