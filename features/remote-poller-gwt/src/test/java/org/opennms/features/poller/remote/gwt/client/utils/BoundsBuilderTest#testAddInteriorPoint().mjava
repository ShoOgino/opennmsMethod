    @Test
    public void testAddInteriorPoint() {
        BoundsBuilder bldr = new BoundsBuilder();
        
        bldr.extend(0.0, 0.0);
        bldr.extend(2.0, 2.0);
        
        assertEquals(new GWTBounds(0.0, 0.0, 2.0, 2.0), bldr.getBounds());

        bldr.extend(1.0, 1.0);
        
        // nothing happens because its in the interior
        assertEquals(new GWTBounds(0.0, 0.0, 2.0, 2.0), bldr.getBounds());
        
    }

