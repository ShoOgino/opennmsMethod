        private boolean runOnePass() {
            Iterator<Object> iterator = m_commands.iterator();
            if(!iterator.hasNext()) {
                return true;
            }

            while(iterator.hasNext()) {
                Object o = iterator.next();
                if(o instanceof Command) {
                    ((Command) o).execute();
                    iterator.remove();
                }else {
                    IncrementalCommand command = (IncrementalCommand) o;
                    if(!command.execute()) {
                        iterator.remove();
                    }
                }

            }

            return false;
        }

