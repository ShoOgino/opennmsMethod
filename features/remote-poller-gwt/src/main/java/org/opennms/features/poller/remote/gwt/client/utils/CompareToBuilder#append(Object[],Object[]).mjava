	public CompareToBuilder append(Object[] a, Object[] b) {
		if (comparison != 0) return this;
		if (a == b) return this;
		if (a == null) {
			comparison = -1;
		} else if (b == null) {
			comparison = 1;
		} else {
	        if (a.length != b.length) {
	            comparison = (a.length < b.length) ? -1 : +1;
	            return this;
	        }
	        for (int i = 0; i < a.length && comparison == 0; i++) {
	            append(a[i], b[i]);
	        }
		}
		return this;
	}

