    /**
     * <p>onUpButtonClick</p>
     *
     * @param event a {@link com.google.gwt.event.dom.client.ClickEvent} object.
     */
    @UiHandler("upButton") 
    public void onUpButtonClick(ClickEvent event) {
        m_eventBus.fireEvent(new StatusSelectionChangedEvent(Status.UP, upButton.isDown()));
    }

