        @Override
        public boolean execute() {
            if(m_locations.isEmpty()) {
                return false;
            }else {
                LocationInfo location = m_locations.remove();
                updateLocation(location);
                m_updated++;
                m_view.setStatusMessage("Updated " + m_updated + " of " + m_count);
                return !m_locations.isEmpty();
            }
            
        }

