    /**
     * <p>getAllTags</p>
     *
     * @return a {@link java.util.List} object.
     */
    public List<String> getAllTags() {
        final List<String> retval = new ArrayList<String>();
        for (final LocationInfo location : m_locations.values()) {
            retval.addAll(location.getTags());
        }
        return retval;
    }

