    /**
     * <p>extend</p>
     *
     * @param lat a double.
     * @param lng a double.
     */
    public void extend(double lat, double lng) {
        if (isEmpty()) {
            swLat = neLat = lat;
            swLng = neLng = lng;
       } else {
           swLat = Math.min(swLat, lat);
           neLat = Math.max(neLat, lat);
           
           if (!containsLongitude(lng)) {
               if (distanceEast(lng) < distanceWest(lng)) {
                   neLng = lng;
               } else {
                   swLng = lng;
               }
           }
           
       }
    }

