	private String getServiceNames(Set<Integer> serviceIds) {
	    StringBuilder buf = new StringBuilder();
	    
	    boolean first = true;
	    for(Integer serviceId : serviceIds) {
	        if (first) {
	            first = false;
	        } else {
	            buf.append(", ");
	        }
	        buf.append(getServiceName(serviceId));
	    }
	    return buf.toString();
	}

