		public String getStyle() {
			String cssClass;
			if (this.equals(ServiceStatus.UP)) {
				cssClass = "statusUp";
			} else if (this.equals(ServiceStatus.MARGINAL)) {
				cssClass = "statusMarginal";
			} else if (this.equals(ServiceStatus.DOWN)) {
				cssClass = "statusDown";
			} else {
				cssClass = "statusUnknown";
			}
			return cssClass;
		}

