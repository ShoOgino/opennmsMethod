    public void placeMarker(GWTMarkerState marker) {
        MQAPoi m = getMarker(marker.getName());
        
        if(m == null) {
        	m = createMarker(marker);
        	m_markers.put(marker.getName(), m);
        	// FIXME
        	// m_map.addShape(m);
        }else {
            updateMarker(m, marker);
        }
        
    }

