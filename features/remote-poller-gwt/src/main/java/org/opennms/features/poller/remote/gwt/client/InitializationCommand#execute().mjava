	public boolean execute() {
	    if(m_currentLoader == null || m_currentLoader.isLoaded()) {
	        m_currentLoader = m_queue.poll();
	        
	        if(m_currentLoader == null) {
	            finished();
	            return false;
	        }else {
	            m_currentLoader.load();
	        }
	        
	    }
	    
	    return true;
	    
	    
	}

