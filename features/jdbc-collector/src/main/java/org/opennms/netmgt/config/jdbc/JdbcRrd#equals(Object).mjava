    @Override
    public boolean equals(Object obj) {
        if (obj instanceof JdbcRrd) {
            JdbcRrd other = (JdbcRrd) obj;
            return new EqualsBuilder()
                .append(getStep(), other.getStep())
                .append(getJdbcRras().toArray(), other.getJdbcRras().toArray())
                .isEquals();
        }
        return false;
    }

