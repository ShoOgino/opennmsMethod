    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        Bridge other = (Bridge) obj;
        if (m_nodeId == null) {
            if (other.m_nodeId != null)
                return false;
        } else if (!m_nodeId.equals(other.m_nodeId))
            return false;
        return true;
    }

