    public static String getIconKey(NodeTopologyEntity node) {
        if (node.getSysObjectId() == null) {
            return "linkd.system";
        }
        if (node.getSysObjectId().startsWith(".")) {
            return "linkd.system.snmp" +node.getSysObjectId();
        }
        return "linkd.system.snmp." + node.getSysObjectId();
        
    }

