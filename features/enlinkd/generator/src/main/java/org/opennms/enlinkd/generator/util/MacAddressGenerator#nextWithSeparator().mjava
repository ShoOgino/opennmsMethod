    public String nextWithSeparator() {

        String s = next();
        s = s.substring(0, 2)
                + ':' + s.substring(2, 4)
                + ':' + s.substring(4, 6)
                + ':' + s.substring(6, 8)
                + ':' + s.substring(8, 10)
                + ':' + s.substring(10, 12);
        last++;
        return s;
    }

