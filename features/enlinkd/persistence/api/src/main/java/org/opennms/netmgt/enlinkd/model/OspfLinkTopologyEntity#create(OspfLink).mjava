    public static OspfLinkTopologyEntity create (OspfLink link) {
        return new OspfLinkTopologyEntity(link.getId()
                , link.getNode().getId()
                , link.getOspfIpAddr()
                , link.getOspfIpMask()
                , link.getOspfRemIpAddr()
                , link.getOspfIfIndex());
    }

