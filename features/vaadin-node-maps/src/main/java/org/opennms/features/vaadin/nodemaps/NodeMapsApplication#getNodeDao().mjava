    /**
     * Gets the OpenNMS Node DAO.
     * 
     * @return the OpenNMS Node DAO
     */
    public NodeDao getNodeDao() {
        if (nodeDao == null) {
            LogUtils.infof(this, "Initializing NodeDao");
            nodeDao = BeanUtils.getBean("daoContext", "nodeDao", NodeDao.class);
        }
        return nodeDao;
    }

