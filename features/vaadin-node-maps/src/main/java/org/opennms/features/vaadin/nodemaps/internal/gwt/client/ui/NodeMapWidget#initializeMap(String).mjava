    private void initializeMap(final String divId) {
        LOG.info("NodeMapWidget.initializeMap()");

        createMap(divId);
        // createGoogleLayer();
        addTileLayer();
        addMarkerLayer();

        // overlay controls
        addSearchControl();
        addAlarmControl();
        addZoomControl();

        m_searchControl.focusInput();
        m_componentTracker.ready(getClass());
        LOG.info("NodeMapWidget.initializeMap(): finished");
    }

