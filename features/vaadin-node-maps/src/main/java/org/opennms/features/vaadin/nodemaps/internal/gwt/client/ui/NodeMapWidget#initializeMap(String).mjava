    private void initializeMap(final String divId) {
        logger.log(Level.INFO, "NodeMapWidget.initializeMap()");

        createMap(divId);
        // createGoogleLayer();
        addTileLayer();
        addMarkerLayer();

        // overlay controls
        addSearchControl();
        addAlarmControl();
        addZoomControl();

        m_searchControl.focus();

        logger.log(Level.INFO, "NodeMapWidget.initializeMap(): finished");
    }

