    private void exportReport(ReportFormat format, JasperPrint jasperPrint, OutputStream outputStream) throws JRException {
        switch (format) {
            case PDF:
                exportReportToPdf(jasperPrint, outputStream);
                break;
                
            case CSV:
                exportReportToCsv(jasperPrint, outputStream);
                break;
    
            default:
                break;
        }
        
    }

