    public ReportParameterBuilder withDouble(String name, Double value) {
        Objects.requireNonNull(name);
        Objects.requireNonNull(value);

        final ReportDoubleParm parm = new ReportDoubleParm();
        parm.setName(name);
        parm.setValue(value);

        doubleParms.add(parm);
        return this;
    }

