    // executes query
    protected <T> void executeQuery(RowHandler<T> rowHandler) {
        final List<T> rows = genericPersistenceAccessor.executeNativeQuery(sql.toString(), parameterMap);
        if (rows != null && !rows.isEmpty()) {
            for (T eachRow : rows) {
                rowHandler.handle(eachRow);
            }
        }
    }

