    @Before
    public void before() {
        databasePopulator.populateDatabase();

        alarmDao.findAll().forEach(alarm -> alarmDao.delete(alarm));
        alarmDao.flush();

        eventDao.findAll().forEach(e -> eventDao.delete(e));
        eventDao.flush();

        outageDao.findAll().forEach(o -> outageDao.delete(o));
        outageDao.flush();
    }

