    private boolean isKarafOk() {
        Class serviceClass = KarafHealthService.class;
        try {
            final KarafHealthService service = SERVICE_LOOKUP.lookup(serviceClass, null);
            return service != null;
        } catch (Exception e) {
            LOG.error("Could not lookup {}, will fail.", serviceClass.getName(), e);
            return false;
        }
    }

