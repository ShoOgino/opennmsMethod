    private static Map<String, String> buildMap(String[][] pairs) {
        if (pairs != null) {
            Map<String, String> map = new HashMap<>();
            for(String[] eachPair : pairs) {
                map.put(Objects.requireNonNull(eachPair[0]), Objects.requireNonNull(eachPair[1]));
            }
            return map;
        }
        return null;
    }

