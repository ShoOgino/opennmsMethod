    @Override
    public boolean equals(Object obj) {
        if (obj == null) {
            return false;
        }
        if (obj == this) {
            return true;
        }
        if (getClass() != obj.getClass()) {
            return false;
        }
        final IpServiceDTO other = (IpServiceDTO) obj;
        final boolean equals = Objects.equal(id, other.id)
                && Objects.equal(location, other.location);
        return equals;
    }

