    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        StatusWithIndices other = (StatusWithIndices) obj;
        return Objects.equals(this.status, other.status)
                && Objects.equals(this.indices, other.indices);
    }

