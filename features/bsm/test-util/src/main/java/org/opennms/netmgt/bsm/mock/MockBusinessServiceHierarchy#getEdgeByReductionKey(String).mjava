    public ReadOnlyEdge getEdgeByReductionKey(String reductionKey) {
        return m_builder.m_businessServicesById.values().stream()
                .map(b -> b.getEdges())
                .flatMap(l -> l.stream())
                .filter(e -> e.getReductionKeys().contains(reductionKey))
                .findFirst().orElse(null);
    }

