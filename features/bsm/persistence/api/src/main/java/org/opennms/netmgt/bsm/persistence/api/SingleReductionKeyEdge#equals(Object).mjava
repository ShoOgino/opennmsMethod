    @Override
    public boolean equals(Object obj) {
        if (obj == null) {
            return false;
        }
        if (getClass() != obj.getClass()) {
            return false;
        }
        final SingleReductionKeyEdge other = (SingleReductionKeyEdge) obj;
        if (!super.equals(obj)) {
            return false;
        }
        return Objects.equals(reductionKey, other.reductionKey);
    }

