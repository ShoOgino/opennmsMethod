	@Override
	public List<? extends Vertex> getVertices(Collection<? extends VertexRef> references) {
		List<SimpleVertex> vertices = new ArrayList<SimpleVertex>();
		for(VertexRef ref : references) {
			SimpleVertex vertex = getSimpleVertex(ref);
			if (ref != null) {
				vertices.add(vertex);
			}
		}
		return vertices;
	}

