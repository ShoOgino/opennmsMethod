		private String matrixTransform(SVGMatrix matrix) {
	        String m = "matrix(" + matrix.getA() +
	                ", " + matrix.getB() +
	                ", " + matrix.getC() + 
	                ", " + matrix.getD() +
	                ", " + matrix.getE() + 
	                ", " + matrix.getF() + ")";
	        return D3.getTransform( m ).toString();
	    }

