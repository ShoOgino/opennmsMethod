	private void addItems(MenuBar menuBar) {
		for (MenuBar.MenuItem eachItem : menuBar.getItems()) {
			addItems(() ->
				this.addItem(
					eachItem.getText(),
					eachItem.getIcon(),
					(Command) selectedItem -> {
						if (eachItem.getCommand() != null) {
							eachItem.getCommand().menuSelected(eachItem);
						}
					}), eachItem);
		}
	}

