        public void visit(Visitor<Value> visitor) {
            if (!visitor.visitNode(this)) {
                if (!isLeaf()) {
                    for(int i = 0; i < m_nodes.length; i++) {
                        Node<Value> n = m_nodes[i];
                        if (n != null) {
                            n.visit(visitor);
                        }
                    }
                }
                
            }
        }

