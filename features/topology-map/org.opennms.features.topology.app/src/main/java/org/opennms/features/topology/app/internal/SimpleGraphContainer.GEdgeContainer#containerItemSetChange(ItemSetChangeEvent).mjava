        @Override
        public void containerItemSetChange(ItemSetChangeEvent event) {
            LoggerFactory.getLogger(getClass()).debug("containerItemSetChange()");
            m_edgeHolder.update();
            removeAllItems();
            addAll(m_edgeHolder.getElements());
            if (m_graph != null) {
            	m_graph.update();
            }
            fireItemSetChange();
        }

