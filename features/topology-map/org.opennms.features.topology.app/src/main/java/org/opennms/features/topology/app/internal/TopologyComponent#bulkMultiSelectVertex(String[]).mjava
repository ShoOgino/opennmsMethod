    /**
     * Select multiple vertices at a time
     * @param vertexKeys
     */
    private void bulkMultiSelectVertex(String[] vertexKeys) {
    	
    	List<?> itemIds = getGraph().getVertexItemIdsForKeys(Arrays.asList(vertexKeys));
    	
		m_graphContainer.selectVertices(itemIds);
        
        requestRepaint();
    }

