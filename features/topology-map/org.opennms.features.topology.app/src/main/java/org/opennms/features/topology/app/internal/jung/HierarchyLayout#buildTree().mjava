    protected void buildTree() {
        this.m_currentPoint = new Point(0, 20);
        if (root != null  && graph != null) {
            calculateDimensionX(root);
            m_currentPoint.x += this.basePositions.get(root) / 2 + this.distX;
            buildTree(root, this.m_currentPoint.x);
        }
    }

