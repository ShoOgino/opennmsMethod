    public void visit(GraphVisitor visitor) throws Exception {
    	
    	visitor.visitGraph(this);
    	
    	for(TopoVertex vertex : getVertices()) {
    		vertex.visit(visitor);
    	}
    	
    	for(TopoEdge edge : getEdges()) {
    		edge.visit(visitor);
    	}
    	
    	visitor.completeGraph(this);
    	
    }

