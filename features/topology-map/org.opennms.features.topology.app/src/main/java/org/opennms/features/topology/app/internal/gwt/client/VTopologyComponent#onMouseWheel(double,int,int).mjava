    @Override
    public void onMouseWheel(double scrollVal, int x, int y) {
        Map<String, Object> props = new HashMap<String, Object>();
        props.put("x", x);
        props.put("y", y);
        props.put("scrollVal", scrollVal);
        m_client.updateVariable(getPaintableId(), "scrollWheel", props, true);
    }

