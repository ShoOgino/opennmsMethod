	/**
	 * Very inefficient default behavior, override in subclasses.
	 */
	@Override
	public Object nextItemId(Object itemId) {
		Iterator<T> itr = m_dao.findMatching(m_criteria).iterator();
		do {
			if (itemId.equals(getId(itr.next()))) {
				if (itr.hasNext()) {
					return getId(itr.next());
				} else {
					return null;
				}
			}
		} while (itr.hasNext());
		return null;
	}

