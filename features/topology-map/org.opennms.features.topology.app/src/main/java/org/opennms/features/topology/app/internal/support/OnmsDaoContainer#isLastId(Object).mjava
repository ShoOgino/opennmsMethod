	/**
	 * Very inefficient default behavior, override in subclasses.
	 */
	@Override
	public boolean isLastId(Object itemId) {
		Object lastItemId = lastItemId();
		if (lastItemId == null) {
			if (itemId == null) {
				return true;
			} else {
				return false;
			}
		} else {
			if (itemId == null) {
				return false;
			} else {
				return lastItemId.equals(itemId);
			}
		}
		
	}

