    @Override
    public void redoLayout() {
        s_log.debug("redoLayout()");
        long graphHashOld = hash(m_graph);
        getGraph();
        long graphHashNew = hash(m_graph);
        if(m_layoutAlgorithm != null  && graphHashOld != graphHashNew) {
            m_layoutAlgorithm.updateLayout(m_graph);
            fireGraphChanged();
        }
    }

