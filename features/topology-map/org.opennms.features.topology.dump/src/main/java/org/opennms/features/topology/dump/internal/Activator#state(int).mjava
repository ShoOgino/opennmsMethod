	public String state(int state) {
		switch (state) {
		case Bundle.INSTALLED: 
			return "INSTALLED";
		case Bundle.ACTIVE:
			return "ACTIVE";
		case Bundle.RESOLVED:
			return "RESOLVED";
		case Bundle.STARTING:
			return "STARTING";
		case Bundle.STOPPING:
			return "STOPPING";
		case Bundle.UNINSTALLED:
			return "UNINSTALLED";
		default:
			return "UNKNOWN STATE";
		}
	}

