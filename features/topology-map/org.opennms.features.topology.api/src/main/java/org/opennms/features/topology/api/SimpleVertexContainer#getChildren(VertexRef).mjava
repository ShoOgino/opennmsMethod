	@Override
	public Collection<VertexRef> getChildren(VertexRef itemId) {
		if (!containsId(itemId)) return Collections.emptyList();
		Vertex v = getItem(itemId).getBean();
		if (v.isLeaf()) {
			return Collections.emptyList();
		}
		else {
			SimpleGroup g = (SimpleGroup)v;
			return g.getMembers();
		}
	}

