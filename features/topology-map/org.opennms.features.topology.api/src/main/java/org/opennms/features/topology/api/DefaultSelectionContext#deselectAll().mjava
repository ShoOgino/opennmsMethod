	@Override
	public boolean deselectAll() {
		int vertSelectionSize = m_selectedVertices.size();
		int edgeSelectionSize = m_selectedEdges.size();

		doDeselectAll();

		if(vertSelectionSize > m_selectedVertices.size() || edgeSelectionSize > m_selectedEdges.size()) {
			return true;
		} else {
			return false;
		}
	}

