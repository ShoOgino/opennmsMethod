    @Override
    public boolean equals(Object obj) {
        if (obj == null) {
            return false;
        }
        if (obj == this) {
            return true;
        }
        if (obj instanceof DefaultVertexHopCriteria) {
            return Objects.equals(vertexRef, ((DefaultVertexHopCriteria) obj).vertexRef);
        }
        return false;
    }

