	public WrappedEdge(Edge edge, WrappedVertex source, WrappedVertex target) {
		key = edge.getKey();
		label = edge.getLabel();
		styleName = edge.getStyleName();
		tooltipText = edge.getTooltipText();
		id = edge.getId();
		namespace = edge.getNamespace();

		this.source = source;
		this.target = target;
	}

