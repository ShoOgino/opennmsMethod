    private void fireEdgesRemoved(List<? extends EdgeRef> edges) {
        List<String> ids = new ArrayList<String>(edges.size());
        for(EdgeRef e : edges) {
            ids.add(e.getId());
        }
        for(EdgeListener listener : m_edgeListeners) {
            listener.edgeSetChanged(this, null, null, ids);
        }
    }

