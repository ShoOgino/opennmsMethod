	@Override
	public VertexRefJaxbEntry marshal(VertexRef input) throws Exception {
		if(input == null) {
			return null;
		}
		final VertexRefJaxbEntry entry = new VertexRefJaxbEntry();
		entry.namespace = input.getNamespace();
		entry.id = input.getId();
		entry.label = input.getLabel();
		return entry;
	}

