        @Override
        public boolean equals(Object obj) {
            if (obj == null) {
                return false;
            }
            if (obj == this) {
                return true;
            }
            if (obj instanceof WrappedVertexHopCriteria) {
                WrappedVertexHopCriteria other = (WrappedVertexHopCriteria) obj;
                return Objects.equals(criteriaList, other.criteriaList);
            }
            return false;
        }

