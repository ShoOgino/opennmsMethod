	public String processCode(Code c){
		int k = 0;
		if (c.getCharCode() != 0) k = c.getCharCode();
		else if (c.getKeyCode() != 0) k = c.getKeyCode();
		
		if (c.isCtrlDown()) {
			k = ctrlPressed(k);
			if (k == -1) return null;
		} else if (c.is1337() || c.isAltDown()) {
			k = fromKeyDownSwitch(k);
			if (k == -1) return null;
		}
		return buildCharacter(k);
	}

