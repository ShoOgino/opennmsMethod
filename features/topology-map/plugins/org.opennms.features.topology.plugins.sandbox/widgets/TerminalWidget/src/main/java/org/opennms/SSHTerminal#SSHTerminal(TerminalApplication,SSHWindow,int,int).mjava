	public SSHTerminal(TerminalApplication app, SSHWindow sshWindow, int width, int height) {
		super();
		TERM_WIDTH = width;
		TERM_HEIGHT = height;
		dumpContents = null;
		this.app = app;
		this.sshWindow = sshWindow;
		try {
			isClosed = "false";
			st = new SessionTerminal();
			st.start();
			forceUpdate = true;

		} catch (IOException e) { e.printStackTrace(); }
	}

