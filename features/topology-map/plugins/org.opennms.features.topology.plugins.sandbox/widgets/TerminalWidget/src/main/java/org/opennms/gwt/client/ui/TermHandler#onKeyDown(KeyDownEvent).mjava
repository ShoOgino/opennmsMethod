	public void onKeyDown(KeyDownEvent event) {
		
		code = new Code(event);
		if (!code.isControlKey()){
			if (code.isFunctionKey() || code.isCtrlDown() || code.isAltDown()) {
				processCode(code);
				event.getNativeEvent().stopPropagation();
				event.getNativeEvent().preventDefault();
			}
		}
	}

