  private String[] add(String d, String[]array){
    int i=0;
    while(i<array.length){
      if(d.equals(array[i])){
        if(i!=0){
          System.arraycopy(array, 0, array, 1, i);
          array[0]=d;
        }  
        return array;
      }
      i++;
    }
    String[] foo = new String[array.length+1];
    if(array.length>0){
      System.arraycopy(array, 0, foo, 1, array.length);
    }
    foo[0]=d;
    array=foo;
    return array;
  }

