  private String[] difference(String[] a, String[] b){
    java.util.Vector<String> v = new java.util.Vector<String>();
    for(int i=0; i<a.length; i++){
      String tmp = a[i].trim();
      for(int j=0; j<b.length; j++){
        if(tmp.equals(b[j])){
          tmp = null;
          break;
        }
      }
      if(tmp!=null)
        v.addElement(tmp);
    }
    return v.toArray(new String[0]);
  }

