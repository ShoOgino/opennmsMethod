  public void init(byte[] key) throws Exception{
    if(key.length>bsize){
      byte[] tmp=new byte[bsize];
      System.arraycopy(key, 0, tmp, 0, bsize);	  
      key=tmp;
    }
    SecretKeySpec skey=new SecretKeySpec(key, "HmacSHA1");
    mac=Mac.getInstance("HmacSHA1");
    mac.init(skey);
  } 

