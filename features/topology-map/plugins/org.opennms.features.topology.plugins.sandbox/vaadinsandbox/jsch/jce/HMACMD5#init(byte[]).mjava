  public void init(byte[] key) throws Exception{
    if(key.length>BSIZE){
      byte[] tmp=new byte[BSIZE];
      System.arraycopy(key, 0, tmp, 0, BSIZE);	  
      key=tmp;
    }

    SecretKeySpec skey=new SecretKeySpec(key, "HmacMD5");
    mac=Mac.getInstance("HmacMD5");
    mac.init(skey);
  } 

