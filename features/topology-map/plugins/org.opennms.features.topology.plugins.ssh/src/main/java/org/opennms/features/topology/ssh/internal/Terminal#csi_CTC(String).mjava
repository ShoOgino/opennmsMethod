	private void csi_CTC(String p) {
		String[] ps = vt100_parse_params(p, new String[] { "0" });
		for (String m : ps) {
			if ("0".equals(m)) {
				if (tab_stops.indexOf(cx) < 0) {
					tab_stops.add(cx);
					Collections.sort(tab_stops);
				}
			} else if ("2".equals(m)) {
				tab_stops.remove(Integer.valueOf(cx));
			} else if ("5".equals(m)) {
				tab_stops = new ArrayList<>();
			}
		}
	}

