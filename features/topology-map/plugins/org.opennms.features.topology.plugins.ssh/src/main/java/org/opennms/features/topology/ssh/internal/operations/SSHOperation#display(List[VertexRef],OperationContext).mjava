	public boolean display(List<VertexRef> targets, OperationContext operationContext) {
	    if (operationContext.getDisplayLocation() == DisplayLocation.MENUBAR) {
	    	return true;
	    } else if(targets != null && targets.size() > 0 && targets.get(0) != null) {
	        return true;
	    } else {
	        return false;
	    }
	    
	}

