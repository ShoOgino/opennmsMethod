	@Test
	public void testPipe_cm_ucLetters() {
		String expected = "\u001bO";
		String input = "~";
		term.write("\u001b[?1h");
		for (int i = 0; i < ucLetters.length; i++) {
			//testing cursor-mode upper-case control sequences
			assertEquals(expected + ucLetters[i], term.pipe(input + ucLetters[i]));
		}
	}

