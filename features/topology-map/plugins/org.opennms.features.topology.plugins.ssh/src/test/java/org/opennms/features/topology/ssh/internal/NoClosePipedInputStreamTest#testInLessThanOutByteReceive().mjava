    @Test
    public void testInLessThanOutByteReceive() throws IOException {
        in.connect(out);
        in.in = 1;
        in.out = 5;
        in.receive(testByte, 0, 4);

        for(int i = 0; i < testByte.length; i++) {
            assertEquals(testByte[i], in.buffer[i+1]);
        }
        assertEquals(15,in.buffer[5]);
    }

