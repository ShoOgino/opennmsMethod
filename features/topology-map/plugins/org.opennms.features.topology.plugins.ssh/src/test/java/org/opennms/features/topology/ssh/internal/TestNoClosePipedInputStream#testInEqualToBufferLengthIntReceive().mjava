    @Test
    public void testInEqualToBufferLengthIntReceive() throws IOException{

        in.in = 2000;
        in.out = 1;
        in.connect(out);
        in.receive(1);
        System.out.println("buffer: " + in.buffer.length);
        System.out.println("in.in: " + in.in);
        
        
        in.in = 1022;
        in.receive(1);
        
        System.out.println("buffer: " + in.buffer.length);
        System.out.println("in.in: " + in.in);
        
    }

