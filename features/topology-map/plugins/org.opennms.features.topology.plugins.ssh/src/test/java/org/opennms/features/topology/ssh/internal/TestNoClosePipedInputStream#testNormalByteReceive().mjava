    @Test
    public void testNormalByteReceive() throws IOException {
        in.in = 1;
        in.out  = 1;
        in.connect(out);
        in.receive(testByte, 0, 4);
        
        for(int i = 1; i < testByte.length; i++) {
            assertEquals(testByte[i], in.buffer[i]);
        }
    }

