	/**
	 * Very inefficient default behavior, override in subclasses.
	 */
	@Override
	public Object prevItemId(Object itemId) {
		if (itemId == null) {
			return null;
		}

		Iterator<T> itr = m_dao.findMatching(m_criteria).iterator();
		T previous = null;
		do {
			T current = (T)itr.next();
			if (itemId.equals(getId(current))) {
				return previous;
			}
			previous = current;
		} while (itr.hasNext());
		return null;
	}

