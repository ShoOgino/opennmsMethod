	@Override
	public Component[] getExtraComponents() {
		if (SecurityContextHolder.getContext().toString().contains(Authentication.ROLE_READONLY)) {
			return new Component[] {
				m_refreshButton
			};
		} else {
			return new Component[] {
				m_refreshButton,
				m_selectAllButton,
				m_resetButton,
				m_ackCombo,
				m_submitButton
			};
		}
	}

