    @Override
    public boolean equals(Object obj) {
        if (obj == this) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (getClass() != obj.getClass()) {
            return false;
        }
        ApplicationCriteria other = (ApplicationCriteria) obj;
        return Objects.equals(applicationId, other.applicationId);
    }

