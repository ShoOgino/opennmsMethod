        final int prime = 31;
        int result = 1;
        result = prime * result
                + ((m_foreignSource == null) ? 0 : m_foreignSource.hashCode());
        result = prime * result + ((m_id == null) ? 0 : m_id.hashCode());
        result = prime * result + ((m_name == null) ? 0 : m_name.hashCode());
