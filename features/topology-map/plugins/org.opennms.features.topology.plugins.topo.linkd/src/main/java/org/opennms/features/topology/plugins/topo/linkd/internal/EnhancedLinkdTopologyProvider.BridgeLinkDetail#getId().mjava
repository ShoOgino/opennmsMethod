        @Override
        public String getId() {
            if (getTargetIfIndex() == null) 
                return getSource().getNodeID() + ":" + getSourceIfIndex() + "|" + getTarget().getNodeID();
            return getSource().getNodeID() + ":" + getSourceIfIndex() + "|" + getTarget().getNodeID() +":" + getTargetIfIndex();
        }

