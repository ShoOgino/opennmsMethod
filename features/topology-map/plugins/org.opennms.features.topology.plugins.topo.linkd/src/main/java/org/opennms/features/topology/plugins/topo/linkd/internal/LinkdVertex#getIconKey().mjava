    @Override
    public String getIconKey() {
        if (m_sysObjectId == null) {
            return "linkd.system";
        }
        if (m_sysObjectId.startsWith(".")) {
            return "linkd.system.snmp" + m_sysObjectId;
        }
        return "linkd.system.snmp." + m_sysObjectId;
    }

