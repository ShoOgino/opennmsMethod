    @Override
    public List<VertexRef> query(SearchQuery searchQuery) {
        List<Vertex> vertices = m_vertexProvider.getVertices();
        List<VertexRef> vertRefs = Lists.newArrayList();

        for(Vertex vertex : vertices){
            if(searchQuery.matches(vertex)) {
                vertRefs.add(vertex);
            }
        }

        return vertRefs;
    }

