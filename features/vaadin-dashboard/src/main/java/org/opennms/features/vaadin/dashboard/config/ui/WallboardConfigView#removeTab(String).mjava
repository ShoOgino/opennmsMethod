    /**
     * Removes a tab identified by name
     */
    public void removeTab(String name) {
        for(int i=0; i<m_tabSheet.getComponentCount(); i++) {
            Tab tab = m_tabSheet.getTab(i);
            if (name.equals(tab.getCaption())) {
                m_tabSheet.removeTab(tab);
            }
        }
    }

