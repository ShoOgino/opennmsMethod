    /**
     * <p>onStart</p>
     */
    @Override
    protected void onStart() {
        try {
            log().debug("onStart: Starting Access Point Monitor scheduler");

            // Start the scheduler
            getScheduler().start();
        } catch (RuntimeException e) {
            log().fatal("onStart: Failed to start scheduler", e);
            throw e;
        }

    }

