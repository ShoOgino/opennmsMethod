    /**
     * @param result
     * @param operator
     * @param operand
     * @param retVal
     * @return
     */
    protected Boolean isCriteriaNull(Object result, String operator, String operand) {
        
        if (result == null)
            return Boolean.FALSE;
        if (operator == null || operand == null) {
            return Boolean.TRUE;
        } else {
            return null;
        }
    }

