    /**
     * Resumes Trapd
     */
    @Override
    protected void onResume() {
        if (m_status != PAUSED) {
            return;
        }

        m_status = RESUME_PENDING;

        LOG.debug("resume: Calling resume on trap receiver");
        m_trapReceiver.start();

        m_status = RUNNING;

        LOG.debug("resume: Trapd resumed");
    }

