    public static ImmutableLogMsg fromMutableLogMsg(Logmsg logMsg) {
        if (logMsg == null) {
            return null;
        }

        return ImmutableLogMsg.newBuilder()
                .setContent(logMsg.getContent())
                .setDest(logMsg.getDest())
                .setNotify(logMsg.hasNotify() ? logMsg.getNotify() : null)
                .build();
    }

