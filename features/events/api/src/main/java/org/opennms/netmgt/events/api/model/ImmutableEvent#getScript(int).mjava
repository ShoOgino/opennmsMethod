    @Override
    public IScript getScript(int index) {
        if (index < 0 || index >= scriptList.size()) {
            throw new IndexOutOfBoundsException("getScript: Index value '" + index + "' not in range [0.." +
                    (scriptList.size() - 1) + "]");
        }

        return scriptList.get(index);
    }

