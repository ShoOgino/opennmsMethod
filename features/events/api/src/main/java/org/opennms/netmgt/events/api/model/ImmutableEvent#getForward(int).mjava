    @Override
    public IForward getForward(int index) {
        if (index < 0 || index >= forwardList.size()) {
            throw new IndexOutOfBoundsException("getForward: Index value '" + index + "' not in range [0.." +
                    (forwardList.size() - 1) + "]");
        }

        return forwardList.get(index);
    }

