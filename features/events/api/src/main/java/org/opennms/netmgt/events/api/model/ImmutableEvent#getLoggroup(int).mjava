    @Override
    public String getLoggroup(int index) {
        if (index < 0 || index >= logGroupList.size()) {
            throw new IndexOutOfBoundsException("getLoggroup: Index value '" + index + "' not in range [0.." +
                    (logGroupList.size() - 1) + "]");
        }

        return logGroupList.get(index);
    }

