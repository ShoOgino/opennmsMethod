    public static Logmsg copyFrom(ILogMsg source) {
        if (source == null) {
            return null;
        }

        Logmsg logmsg = new Logmsg();
        logmsg.setContent(source.getContent());
        logmsg.setDest(source.getDest());
        logmsg.setNotify(source.hasNotify() ? source.getNotify() : null);
        return logmsg;
    }

