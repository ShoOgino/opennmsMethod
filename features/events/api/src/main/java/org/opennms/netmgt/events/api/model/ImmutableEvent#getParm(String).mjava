    @Override
    public IParm getParm(String key) {
        if (parms == null) {
            return null;
        }
        if (key == null) {
            throw new IllegalArgumentException("Parameter key cannot be null!");
        }

        return parms.stream().filter(p -> Objects.equals(key, p.getParmName()))
                .findFirst().orElse(null);
    }

