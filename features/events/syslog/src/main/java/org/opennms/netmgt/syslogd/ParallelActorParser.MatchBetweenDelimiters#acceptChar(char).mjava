		@Override
		public AcceptResult acceptChar(char c) {
			if (foundStart) {
				if (c == m_endDelimiter) {
					return AcceptResult.COMPLETE_AFTER_CONSUMING;
				} else {
					accumulate(c);
					return AcceptResult.CONTINUE;
				}
			} else {
				if (c == m_startDelimiter) {
					foundStart = true;
					return AcceptResult.CONTINUE;
				} else {
					return AcceptResult.CANCEL;
				}
			}
		}

