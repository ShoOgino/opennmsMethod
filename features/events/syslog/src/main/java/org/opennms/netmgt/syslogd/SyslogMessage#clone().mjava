    @Override
    public SyslogMessage clone() {
        return new SyslogMessage(
            m_facility.getFacilityNumber(),
            m_severity.getSeverityNumber(),
            m_version,
            m_date,
            m_year,
            m_month,
            m_dayOfMonth,
            m_hourOfDay,
            m_minute,
            m_second,
            m_millisecond,
            m_timeZone,
            m_hostname,
            m_processName,
            m_processId,
            m_messageId,
            m_message
        );
    }

