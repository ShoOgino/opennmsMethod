		@Override
		public Integer getValue(SyslogParserState state) {
			// Trim the leading zeros from this value
			String value = accumulatedValue(state);
			while (value.startsWith("0")) {
				value = value.substring(1);
			}

			if ("".equals(value)) {
				return 0;
			} else {
				return Integer.parseInt(value);
			}
		}

