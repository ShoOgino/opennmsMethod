		@Override
		public AcceptResult acceptChar(SyslogParserState state, char c) {
			if (state.flag) {
				if (c == m_endDelimiter) {
					return AcceptResult.COMPLETE_AFTER_CONSUMING;
				} else {
					accumulate(state, c);
					return AcceptResult.CONTINUE;
				}
			} else {
				if (c == m_startDelimiter) {
					state.flag = true;
					return AcceptResult.CONTINUE;
				} else {
					return AcceptResult.CANCEL;
				}
			}
		}

