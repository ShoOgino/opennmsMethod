    /**
     * 
     * 
     * @param vNotificationArray
     */
    public void setNotification(final Notification[] vNotificationArray) {
        //-- copy array
        notificationList.clear();
        
        for (int i = 0; i < vNotificationArray.length; i++) {
                this.notificationList.add(vNotificationArray[i]);
        }
    }

