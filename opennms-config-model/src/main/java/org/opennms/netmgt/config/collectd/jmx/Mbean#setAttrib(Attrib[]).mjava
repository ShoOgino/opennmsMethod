    /**
     * 
     * 
     * @param vAttribArray
     */
    public void setAttrib(
            final Attrib[] vAttribArray) {
        //-- copy array
        _attribList.clear();
        
        for (int i = 0; i < vAttribArray.length; i++) {
                this._attribList.add(vAttribArray[i]);
        }
    }

