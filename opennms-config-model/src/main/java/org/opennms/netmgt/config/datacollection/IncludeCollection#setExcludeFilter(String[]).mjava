    /**
     * 
     * 
     * @param excludeFilters
     */
    public void setExcludeFilter(final String[] excludeFilters) {
        m_excludeFilters.clear();
        for (int i = 0; i < excludeFilters.length; i++) {
                m_excludeFilters.add(excludeFilters[i].intern());
        }
    }

