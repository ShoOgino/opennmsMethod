    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (super.equals(obj)==false) {
            return false;
        }

        if (obj instanceof Expression) {
            final Expression that = (Expression)obj;
            return Objects.equals(this.m_expression, that.m_expression);
        }
        return false;
    }

