    /**
     *
     *
     * @param vAutoEventArray
     */
    public void setAutoEvent(final AutoEvent[] vAutoEventArray) {
        // -- copy array
        _autoEventList.clear();

        for (int i = 0; i < vAutoEventArray.length; i++) {
            this._autoEventList.add(vAutoEventArray[i]);
        }
    }

