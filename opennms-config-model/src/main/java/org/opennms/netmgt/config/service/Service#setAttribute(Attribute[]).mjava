    /**
     * 
     * 
     * @param vAttributeArray
     */
    public void setAttribute(
            final Attribute[] vAttributeArray) {
        //-- copy array
        _attributeList.clear();
        
        for (int i = 0; i < vAttributeArray.length; i++) {
                this._attributeList.add(vAttributeArray[i]);
        }
    }

