     * 
     * @param vEventScriptArray
     */
    public void setEventScript(final EventScript[] vEventScriptArray) {
        //-- copy array
        eventScriptList.clear();
        
        for (int i = 0; i < vEventScriptArray.length; i++) {
                this.eventScriptList.add(vEventScriptArray[i]);
        }
    }

