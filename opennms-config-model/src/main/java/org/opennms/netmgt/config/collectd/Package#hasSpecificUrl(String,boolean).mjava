    public boolean hasSpecificUrl(String iface, boolean hasSpecific) {
        if (hasSpecific) {
            return true;
        } else {
            for (String includeURL : getIncludeUrls()) {
                IncludeURL url = new IncludeURL(includeURL);
                if (url.interfaceInUrl(iface)) {
                    return true;
                }
            }
        }
        return false;
    }

