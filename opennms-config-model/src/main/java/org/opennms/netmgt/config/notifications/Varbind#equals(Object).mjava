    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (obj instanceof Varbind) {
            final Varbind that = (Varbind)obj;
            return Objects.equals(this.m_vbname, that.m_vbname)
                    && Objects.equals(this.m_vbvalue, that.m_vbvalue);
        }
        return false;
    }

