    /**
     * <p>hasIncludeRange</p>
     *
     * @param addr a long.
     * @return a boolean.
     */
    public boolean hasIncludeRange(String addr) {
        if (getIncludeRanges().size() == 0 && getSpecifics().size() == 0) {
            return true;
        }

        for (final IncludeRange rng : getIncludeRanges()) {
            if (isInetAddressInRange(addr, rng.getBegin(), rng.getEnd())) {
                return true;
            }
        }
        return false;
    }

