    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (obj instanceof Mapping) {
            final Mapping that = (Mapping)obj;
            return Objects.equals(this.m_sysoidMask, that.m_sysoidMask)
                    && Objects.equals(this.m_sysdescrMatch, that.m_sysdescrMatch)
                    && Objects.equals(this.m_type, that.m_type);
        }
        return false;
    }

