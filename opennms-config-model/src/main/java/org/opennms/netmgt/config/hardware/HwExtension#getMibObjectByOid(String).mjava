    /**
     * Gets the MIB object by OID.
     *
     * @param oid the OID
     * @return the MIB object by OID
     */
    public MibObj getMibObjectByOid(String oid) {
        for (MibObj obj : mibObjects) {
            if (obj.getOid().toString().equals(oid)) {
                return obj;
            }
        }
        return null;
    }

