    /**
     * 
     * 
     * @param vCompAttribArray
     */
    public void setCompAttrib(
            final CompAttrib[] vCompAttribArray) {
        //-- copy array
        _compAttribList.clear();
        
        for (int i = 0; i < vCompAttribArray.length; i++) {
                this._compAttribList.add(vCompAttribArray[i]);
        }
    }

