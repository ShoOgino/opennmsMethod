    /**
     * Overrides the Object.equals method.
     * 
     * @param obj
     * @return true if the objects are equal.
     */
    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }
        
        if (obj instanceof Uei) {
            Uei temp = (Uei)obj;
            boolean equals = Objects.equals(temp.name, name);
            return equals;
        }
        return false;
    }

