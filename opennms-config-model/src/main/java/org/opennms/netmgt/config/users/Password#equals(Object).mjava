    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }
        
        if (obj instanceof Password) {
            final Password temp = (Password)obj;
            return Objects.equals(temp.m_password, m_password)
                && Objects.equals(temp.m_salt, m_salt);
        }
        return false;
    }

