    public void replaceCategoryInGroup(final String groupname, final Category cat) {
        m_categoryGroups.forEach(cg -> {
            if (cg.getName().equals(groupname)) {
                cg.getCategories().replaceAll(c -> {
                    if (c.getLabel().equals(cat.getLabel())) {
                        return cat;
                    }
                    return c;
                });
            }
        });        
    }

