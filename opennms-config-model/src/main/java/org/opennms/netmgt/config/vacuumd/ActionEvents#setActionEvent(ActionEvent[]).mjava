    /**
     *
     *
     * @param vActionEventArray
     */
    public void setActionEvent(final ActionEvent[] vActionEventArray) {
        // -- copy array
        _actionEventList.clear();

        for (int i = 0; i < vActionEventArray.length; i++) {
            this._actionEventList.add(vActionEventArray[i]);
        }
    }

