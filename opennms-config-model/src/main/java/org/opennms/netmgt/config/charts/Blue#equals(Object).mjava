    /**
     * Overrides the Object.equals method.
     * 
     * @param obj
     * @return true if the objects are equal.
     */
    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }
        
        if (obj instanceof Blue) {
            Blue temp = (Blue)obj;
            boolean equals = Objects.equals(temp.rgbColor, rgbColor);
            return equals;
        }
        return false;
    }

