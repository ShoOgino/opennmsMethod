    @Override()
    public boolean equals(final Object obj) {
        if (this == obj)
            return true;

        if (obj instanceof Value) {
            final Value that = (Value) obj;
            return Objects.equals(this.m_content, that.m_content) &&
                    Objects.equals(this.m_type, that.m_type);
        }
        return false;
    }

