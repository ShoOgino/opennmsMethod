     * @param vStopScriptArray
     */
    public void setStopScript(final StopScript[] vStopScriptArray) {
        //-- copy array
        stopScriptList.clear();
        
        for (int i = 0; i < vStopScriptArray.length; i++) {
                this.stopScriptList.add(vStopScriptArray[i]);
        }
    }

