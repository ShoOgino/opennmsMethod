    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (obj instanceof View) {
            final View that = (View)obj;
            return Objects.equals(this.m_viewName, that.m_viewName)
                    && Objects.equals(this.m_sections, that.m_sections);
        }
        return false;
    }

