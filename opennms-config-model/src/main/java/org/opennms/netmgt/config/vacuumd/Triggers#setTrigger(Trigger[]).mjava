    /**
     *
     *
     * @param vTriggerArray
     */
    public void setTrigger(final Trigger[] vTriggerArray) {
        // -- copy array
        _triggerList.clear();

        for (int i = 0; i < vTriggerArray.length; i++) {
            this._triggerList.add(vTriggerArray[i]);
        }
    }

