     * @param vColumnDefArray
     */
    public void setColumnDef(final ColumnDef[] vColumnDefArray) {
        //-- copy array
        columnDefList.clear();
        
        for (int i = 0; i < vColumnDefArray.length; i++) {
                this.columnDefList.add(vColumnDefArray[i]);
        }
    }

