    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (obj instanceof IncludeRange) {
            final IncludeRange that = (IncludeRange)obj;
            return Objects.equals(this.m_begin, that.m_begin) &&
                    Objects.equals(this.m_end, that.m_end);
        }
        return false;
    }

