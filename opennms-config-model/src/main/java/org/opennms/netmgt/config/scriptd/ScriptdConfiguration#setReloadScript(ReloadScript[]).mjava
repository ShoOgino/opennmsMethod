    /**
     * 
     * 
     * @param vReloadScriptArray
     */
    public void setReloadScript(final ReloadScript[] vReloadScriptArray) {
        //-- copy array
        reloadScriptList.clear();
        
        for (int i = 0; i < vReloadScriptArray.length; i++) {
                this.reloadScriptList.add(vReloadScriptArray[i]);
        }
    }

