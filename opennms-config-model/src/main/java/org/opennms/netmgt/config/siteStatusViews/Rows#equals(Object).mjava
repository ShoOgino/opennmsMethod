    /**
     * Overrides the Object.equals method.
     * 
     * @param obj
     * @return true if the objects are equal.
     */
    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }
        
        if (obj instanceof Rows) {
            Rows temp = (Rows)obj;
            boolean equals = Objects.equals(temp.rowDefList, rowDefList);
            return equals;
        }
        return false;
    }

