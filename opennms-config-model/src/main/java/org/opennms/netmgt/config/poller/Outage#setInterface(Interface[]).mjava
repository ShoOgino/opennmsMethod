     * @param vInterfaceArray
     */
    public void setInterface(final Interface[] vInterfaceArray) {
        //-- copy array
        _interfaceList.clear();

        for (int i = 0; i < vInterfaceArray.length; i++) {
            this._interfaceList.add(vInterfaceArray[i]);
        }
