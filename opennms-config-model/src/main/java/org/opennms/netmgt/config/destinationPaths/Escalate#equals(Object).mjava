    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (obj instanceof Escalate) {
            final Escalate temp = (Escalate)obj;
            return Objects.equals(temp.m_delay, m_delay)
                    && Objects.equals(temp.m_targets, m_targets);
        }
        return false;
    }

