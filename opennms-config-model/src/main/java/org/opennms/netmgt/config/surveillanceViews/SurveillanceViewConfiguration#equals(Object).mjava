    /**
     * Overrides the Object.equals method.
     * 
     * @param obj
     * @return true if the objects are equal.
     */
    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }
        
        if (obj instanceof SurveillanceViewConfiguration) {
            SurveillanceViewConfiguration temp = (SurveillanceViewConfiguration)obj;
            boolean equals = Objects.equals(temp.defaultView, defaultView)
                && Objects.equals(temp.views, views);
            return equals;
        }
        return false;
    }

