    /**
     *
     *
     * @param vAutomationArray
     */
    public void setAutomation(final Automation[] vAutomationArray) {
        // -- copy array
        _automationList.clear();

        for (int i = 0; i < vAutomationArray.length; i++) {
            this._automationList.add(vAutomationArray[i]);
        }
    }

