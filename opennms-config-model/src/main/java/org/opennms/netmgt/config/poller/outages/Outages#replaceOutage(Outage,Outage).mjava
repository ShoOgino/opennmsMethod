    public boolean replaceOutage(final Outage oldOutage, final Outage newOutage) {
        String match = null;

        for (final Map.Entry<String,Outage> entry : m_outages.entrySet()) {
            if (entry.getValue().equals(oldOutage)) {
                match = entry.getKey();
                break;
            }
        }

        if (match != null) {
            m_outages.put(match, newOutage);
            return true;
        }

        return false;
    }

