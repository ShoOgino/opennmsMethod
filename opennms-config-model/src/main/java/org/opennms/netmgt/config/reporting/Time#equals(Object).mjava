    @Override
    public boolean equals(final Object obj) {
        if ( this == obj ) {
            return true;
        }

        if (obj instanceof Time) {
            final Time that = (Time)obj;
            return Objects.equals(this.m_hours, that.m_hours)
                    && Objects.equals(this.m_minutes, that.m_minutes);
        }
        return false;
    }

