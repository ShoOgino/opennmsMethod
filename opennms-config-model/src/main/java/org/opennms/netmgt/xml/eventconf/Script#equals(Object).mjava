    @Override
    public boolean equals(final Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj instanceof Script) {
            final Script that = (Script) obj;
            return Objects.equals(this.m_content, that.m_content) &&
                    Objects.equals(this.m_language, that.m_language);
        }
        return false;
    }

