    public static String toHexString(byte[] data) {
        final StringBuffer b = new StringBuffer();
        for (int i = 0; i < data.length; ++i) {
            final int x = (int) data[i] & 0xff;
            if (x < 16) b.append("0");
            b.append(Integer.toString(x, 16).toLowerCase());
        }
        return b.toString();
    }

