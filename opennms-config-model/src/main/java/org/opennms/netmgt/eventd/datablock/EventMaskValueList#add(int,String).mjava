    /**
     * Override to re-evaluate hashcode
     *
     * @see java.util.ArrayList#add(int, Object)
     * @param index a int.
     * @param o a {@link java.lang.String} object.
     */
    @Override
    public void add(int index, String o) {
        super.add(index, o);
        evaluateHashCode();
    }

