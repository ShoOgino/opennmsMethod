        @Override
        public void run() {
            while (m_counter.get() < 100) {
                try {
                    m_lock.withTreeLock(new RecursiveCallable(m_lock, m_counter, m_invocations, m_recursionDepth - 1), 5);
                } catch (LockUnavailable e) {
                    //LOG.debug(e.getMessage());
                }
            }
        }

