	public void testStop() throws Exception {
		
		setRegistered();

		anticipateAfterPropertiesSet();
		
		anticipateStop();

		replayMocks();

		m_frontEnd.afterPropertiesSet();

		assertTrue(m_frontEnd.isStarted());

		m_frontEnd.stop();

		assertFalse(m_frontEnd.isStarted());

		verifyMocks();
	}

