    protected boolean runSql(String db, String sql) throws Exception {
        String psql = System.getProperty("psql.command", "psql") ;
        System.err.println("psql.command = " + psql);
        String[] cmd = {
                psql,
                db,
                "-U",
                "opennms",
                "-c",
                sql,
        };
        return execute(cmd);
    }

