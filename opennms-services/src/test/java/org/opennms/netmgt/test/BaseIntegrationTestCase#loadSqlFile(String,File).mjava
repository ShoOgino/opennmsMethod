    protected boolean loadSqlFile(String db, File sqlFile) throws Exception {
        String psql = System.getProperty("psql.command", "psql") ;
        System.err.println("psql.command = " + psql);
        String[] cmd = {
                psql,
                db,
                "-U",
                "opennms",
                "-f",
                sqlFile.getAbsolutePath()
        };
        return execute(cmd);
    }

