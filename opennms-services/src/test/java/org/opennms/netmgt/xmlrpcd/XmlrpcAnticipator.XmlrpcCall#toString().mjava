        public String toString() {
            StringBuffer b = new StringBuffer();
            b.append("Method: " + m_method + "\n");
            for (Iterator i = m_vector.iterator(); i.hasNext(); ) {
                Object o = i.next();
                b.append("Parameter (" + o.getClass().getName() + ") "+ o + "\n");
            }
            return b.toString();
        }

