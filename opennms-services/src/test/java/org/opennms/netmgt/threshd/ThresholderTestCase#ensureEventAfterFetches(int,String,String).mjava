	private void ensureEventAfterFetches(int count, String dsName, String uei) {
	    if (uei != null) {
	        Event event = MockEventUtil.createServiceEvent("Test", uei, m_network.getService(1, "192.168.1.1", "ICMP"), null);
	        Parms parms = new Parms();
	        Parm parm = new Parm();
	        parm.setParmName("ds");
	        Value val = new Value();
	        val.setContent(dsName);
	        parm.setValue(val);
	        parms.addParm(parm);
	        event.setParms(parms);
	        m_anticipator.anticipateEvent(event);
	    }
	    for(int i = 0; i < count; i++) {
	        m_thresholder.check(m_iface, m_proxy, m_parameters);
	    }
	    verifyAnticipated(1000);
	}

