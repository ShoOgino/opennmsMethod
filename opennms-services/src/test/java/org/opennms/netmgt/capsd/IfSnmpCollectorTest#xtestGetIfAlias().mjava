    public final void xtestGetIfAlias() {
        if(m_run) {

            if(!m_hasRun)
                m_ifSnmpc.run();
            
            if (m_ifSnmpc.hasIfXTable()) {
                String ifAlias = m_ifSnmpc.getIfAlias(1);
                assertNotNull(ifAlias);
            }
        }
    }

