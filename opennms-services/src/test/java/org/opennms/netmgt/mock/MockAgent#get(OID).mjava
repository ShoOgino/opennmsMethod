    /**
     * @param oid
     * @return
     */
    public VariableBinding get(OID oid) {
        for (OID agentKey : m_subAgents.keySet()) {
            if (oid.startsWith(agentKey)) {
                SubAgent subAgent = m_subAgents.get(agentKey);
                return subAgent.get(oid);
            }
        }
        return null;
    }

