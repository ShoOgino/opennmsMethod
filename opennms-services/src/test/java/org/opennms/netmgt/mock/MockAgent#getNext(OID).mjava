    /**
     * @param oid
     * @return
     */
    public VariableBinding getNext(OID oid) {
        VariableBinding result = null;
        for (Iterator it = m_subAgents.values().iterator(); it.hasNext();) {
            SubAgent subAgent = (SubAgent) it.next();
            result = subAgent.getNext(oid);
            if (result != null)
                return result;
        }
        
        return result;
    }

