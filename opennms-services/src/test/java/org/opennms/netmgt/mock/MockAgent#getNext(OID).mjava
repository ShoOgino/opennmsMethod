    /**
     * @param oid
     * @return
     */
    public VariableBinding getNext(OID oid) {
        VariableBinding result = null;
        for (SubAgent subAgent : m_subAgents.values()) {
            result = subAgent.getNext(oid);
            if (result != null) {
                return result;
            }
        }
        
        return result;
    }

