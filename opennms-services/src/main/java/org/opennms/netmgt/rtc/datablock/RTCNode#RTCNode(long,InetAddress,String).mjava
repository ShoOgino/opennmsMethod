    /**
     * Constructor.
     *
     * @param nodeid
     *            the node id
     * @param inetAddress
     *            the IP address
     * @param svcName
     *            the service
     */
    public RTCNode(long nodeid, InetAddress inetAddress, String svcName) {
        m_nodeID = nodeid;

        m_ip = inetAddress;

        m_svcName = svcName;

        m_svcTimesList = new RTCNodeSvcTimesList();
        m_categories = new ArrayList<String>();
    }

