    /**
     * Whether vlan discovery is enabled in linkd-config (default: true)
     */
    public boolean isVlanDiscoveryEnabled() {
        getReadLock().lock();
        try {
            if (m_config.hasEnableVlanDiscovery()) return m_config.getEnableVlanDiscovery();
        } finally {
            getReadLock().unlock();
        }
        return true;
    }

