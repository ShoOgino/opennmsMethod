    /**
     * {@inheritDoc}
     *
     * <p>
     * Return if nodeid is part of specified outage
     * </p>
     */
    public synchronized boolean isNodeIdInOutage(long lnodeid, String outName) {
        Outage out = getOutage(outName);
        if (out == null)
            return false;

        return isNodeIdInOutage(lnodeid, out);
    }

