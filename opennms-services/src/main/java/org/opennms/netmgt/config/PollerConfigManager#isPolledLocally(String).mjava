    /** {@inheritDoc} */
    public boolean isPolledLocally(final String ipaddr) {
        getReadLock().lock();
        try {
            for(final Package pkg : packages()) {
                if (!pkg.getRemote() && isInterfaceInPackage(ipaddr, pkg)) {
                    return true;
                }
            }
        } finally {
            getReadLock().unlock();
        }
        return false;
    }

