    /**
     * 
     */
    public synchronized StandbyUrl getNextStandbyUrl() {
        StandbyUrl standbyUrl = null;
        if (hasStandbyUrl()) {
            if (cursor == m_config.getStandbyUrlCount())   
                cursor = 0;
            standbyUrl = m_config.getStandbyUrl(cursor++);
        }
        
        return standbyUrl;
    }

