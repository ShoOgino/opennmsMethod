    public synchronized void replaceOutage(Outage oldOutage, Outage newOutage) {
        int count = m_config.getOutageCount();
        for (int i = 0; i < count; i++) {
            if (m_config.getOutage(i).equals(oldOutage)) {
                m_config.setOutage(i, newOutage);
                return;
            }
        }
    }

