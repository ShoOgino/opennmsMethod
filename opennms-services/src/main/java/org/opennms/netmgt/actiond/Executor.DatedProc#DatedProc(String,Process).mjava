        /**
         * Constructs a new dated process.
         * 
         * @param cmd
         *            The command used to start the process.
         * @param p
         *            The running process.
         */
        DatedProc(String cmd, Process p) {
            m_cmd = cmd;
            m_proc = p;
            m_started = System.currentTimeMillis();
        }

