    private int getNextId() throws SQLException {
        int id;
        // Get the next id from sequence specified in
        ResultSet rs = null;
        try {
            rs = m_getNextIdStmt.executeQuery();
            rs.next();
            id = rs.getInt(1);
        } catch (SQLException e) {
            throw e;
        } finally {
            rs.close();
        }
        rs = null;
        return id;
    }

