    final public void init() {
        String prefix = ThreadCategory.getPrefix();
        try {
            
            if (!isStartPending()) {
                return;
            }
            
            ThreadCategory.setPrefix(getName());
            log().debug(getName()+" initializing.");

            onInit();

            log().debug(getName()+" initialization complete.");
        } finally {
            ThreadCategory.setPrefix(prefix);
        }
    }

