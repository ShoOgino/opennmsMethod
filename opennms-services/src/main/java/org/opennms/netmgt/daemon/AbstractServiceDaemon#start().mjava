	final public synchronized void start() {
		String prefix = ThreadCategory.getPrefix();
		try {
			ThreadCategory.setPrefix(getName());
			log().debug(getName()+" starting.");

			setStatus(STARTING);

			onStart();

			setStatus(RUNNING);

			log().debug(getName()+" started.");
		} finally {
			ThreadCategory.setPrefix(prefix);
		}
	
	}

