    protected void onStop() {
		try {
            if (m_eventReader != null) {
                m_eventReader.close();
            }
        } catch (Exception e) {
        }

        try {
            if (m_eventWriter != null) {
                m_eventWriter.stop();
            }
        } catch (Exception e) {
        }

        m_eventWriter = null;
        m_eventReader = null;
        m_generator = null;

        try {
            if (m_manager != null) {
                m_manager.stop();
	    }
        } catch (Exception e) {
        }
        m_manager = null;
	}

