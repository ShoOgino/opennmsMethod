    protected void onPause() {
		if (log().isDebugEnabled())
            log().debug("Calling pause on processor");

        for (int i = 0; i < m_processors.size(); i++) {
            EventQueueProcessor proc = m_processors.get(i);
            proc.pause();
        }

        if (log().isDebugEnabled())
            log().debug("Processor paused");
	}

