    /**
     * <p>onStart</p>
     */
    protected void onStart() {
        LogUtils.debugf(this, "start: Initializing the xmlrpcd config factory");
        
        for (final EventQueueProcessor proc : m_processors) {
        	proc.start();
        }

        LogUtils.debugf(this, "start: xmlrpcd ready to process events");
    }

