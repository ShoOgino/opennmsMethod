    protected void onResume() {
		if (log().isDebugEnabled())
            log().debug("Calling resume on processor");

        for (int i = 0; i < m_processors.size(); i++) {
            EventQueueProcessor proc = m_processors.get(i);
            proc.resume();
        }

        if (log().isDebugEnabled())
            log().debug("Processor resumed");
	}

