    public void delete() {
        Runnable r = new Runnable() {
            public void run() {
                Collection members = getMembers();
                for (Iterator it = members.iterator(); it.hasNext();) {
                    PollableElement member = (PollableElement) it.next();
                    member.delete();
                }
                PollableContainer.super.delete();
            }
        };
        withTreeLock(r);
        
    }

