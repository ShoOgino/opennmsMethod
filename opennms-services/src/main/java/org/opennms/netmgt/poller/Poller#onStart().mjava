    /**
     * <p>onStart</p>
     */
    @Override
    protected void onStart() {
		// get the category logger
        // start the scheduler
        //
        try {
            if (log().isDebugEnabled())
                log().debug("start: Starting poller scheduler");

            getScheduler().start();
        } catch (RuntimeException e) {
            log().error("start: Failed to start scheduler", e);
            throw e;
        }
	}

