        @Override
        public void checkIn() {
            MonitorStatus status = doCheckIn();
            switch (status) {
            case CONFIG_CHANGED:
                onConfigChanged();
                break;
            case DELETED:
                onDeleted();
                break;
            case DISCONNECTED:
                onDisconnected();
                break;
            case PAUSED:
                onPaused();
                break;
            case STARTED:
                onStarted();
                break;

            }
        }

