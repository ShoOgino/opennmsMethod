        @Override
        public void checkIn() {
            try {
                MonitorStatus status = doCheckIn();
                switch (status) {
                case CONFIG_CHANGED:
                    onConfigChanged();
                    break;
                case DELETED:
                    onDeleted();
                    break;
                case DISCONNECTED:
                    onDisconnected();
                    break;
                case PAUSED:
                    onPaused();
                    break;
                case STARTED:
                    onStarted();
                    break;

                }
            } catch (Exception e) {
                log().fatal("Unexpected exception occurred loading the configs", e);
                setState(new FatalExceptionOccurred());
            }
        }

