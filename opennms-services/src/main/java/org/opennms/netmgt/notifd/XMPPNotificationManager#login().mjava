	private void login() {
		try {
			if (xmpp.isConnected()) {
				xmpp.login(xmppUser, xmppPassword, XMPP_RESOURCE);
				rooms = new HashMap();
				log().debug("XMPP Manager logged in");
			} else
				log().debug("XMPP Manager unable to login: Not connected to XMPP server");
		} catch (Exception e) {
			log().fatal("XMPP Manager unable to login: ", e);
		}
	}

