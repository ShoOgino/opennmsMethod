    /**
     * 
     */
    public void processQueue() {
        Category log = ThreadCategory.getInstance(getClass());

        try {
            Long now = new Long(System.currentTimeMillis());
            SortedMap readyNotices = m_noticeQueue.headMap(now);

            Iterator i = readyNotices.values().iterator();
            while (i.hasNext()) {
                Object o = i.next();
                if (o instanceof NotificationTask) {
                    NotificationTask task = (NotificationTask) o;
                    task.start();
                } else if (o instanceof List) {
                    List list = (List) o;
                    for (int j = 0; j < list.size(); j++) {
                        NotificationTask task = (NotificationTask) list.get(j);
                        task.start();
                    }
                }
            }
            readyNotices.clear();

            log.debug("current state of tree: ");
            log.debug("\n" + m_noticeQueue);
        } catch (Exception e) {
            log.error(e.getMessage(), e);
        }
    }

