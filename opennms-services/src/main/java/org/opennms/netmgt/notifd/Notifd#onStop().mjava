    protected void onStop() {
		try {
            Iterator i = m_queueHandlers.keySet().iterator();
            while (i.hasNext()) {
                NotifdQueueHandler curHandler = (NotifdQueueHandler) m_queueHandlers.get(i.next());
                curHandler.stop();
            }
        } catch (Exception e) {
        }

        if (m_eventReader != null)
            m_eventReader.close();

        m_eventReader = null;
	}

