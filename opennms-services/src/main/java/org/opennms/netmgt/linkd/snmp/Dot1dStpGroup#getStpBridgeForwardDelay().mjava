    /**
     * <p>getStpBridgeForwardDelay</p>
     *
     * @return a int.
     */
    public int getStpBridgeForwardDelay(){
    	Integer stpBridgeForwardDelay = m_store.getInt32(STP_BRDG_FORW_DELAY);
    	if (stpBridgeForwardDelay == null ) {
            return -1;
        }
    	return stpBridgeForwardDelay;
    }

