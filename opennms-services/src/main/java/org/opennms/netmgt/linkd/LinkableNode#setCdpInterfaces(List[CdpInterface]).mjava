    /**
     * <p>
     * setCdpInterfaces
     * </p>
     * 
     * @param cdpinterfaces
     *            The m_cdpinterfaces to set.
     */
    public void setCdpInterfaces(List<CdpInterface> cdpinterfaces) {
        if (cdpinterfaces == null || cdpinterfaces.isEmpty())
            return;
        m_hascdpinterfaces = true;
        m_cdpinterfaces = cdpinterfaces;
    }

