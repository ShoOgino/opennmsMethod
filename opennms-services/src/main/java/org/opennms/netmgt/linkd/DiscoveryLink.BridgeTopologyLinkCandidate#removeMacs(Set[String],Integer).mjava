        public void removeMacs(Set<String> otherMacs, Integer targettomerge) {
        	targets.add(targettomerge);
        	Set<String> curmacs = new HashSet<String>();
    		for (String mac: getMacs()) {
    			if (otherMacs.contains(mac)) 
    				continue;
    			curmacs.add(mac);
    		}
    		macs = curmacs;
        }

