    /** {@inheritDoc} */
    @Override
    public void storeResult(SnmpResult res) {
            if (!hasIndex) {
                    m_isisCircIndex = res.getInstance().getSubIdAt(res.getInstance().length()-2);
                    m_isisISAdjIndex = res.getInstance().getLastSubId();
                    hasIndex = true;
            }
            super.storeResult(res);
    }

