	/**
	 * 
	 * @param nodeid
	 * @return true if found
	 */

	boolean isRouterNode(int nodeid) {

		Iterator ite = routerNodes.iterator();
		while (ite.hasNext()) {
			LinkableNode curNode = (LinkableNode) ite.next();
			if (nodeid == curNode.getNodeId())
				return true;
		}
		return false;
	}

