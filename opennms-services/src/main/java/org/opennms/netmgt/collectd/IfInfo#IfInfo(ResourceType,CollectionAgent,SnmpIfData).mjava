    public IfInfo(ResourceType def, CollectionAgent agent, SnmpIfData snmpIfData) {
        super(def, agent);
        m_nodeId = snmpIfData.getNodeId();
        m_collType = snmpIfData.getCollectionType();
        m_ifIndex = snmpIfData.getIfIndex();
