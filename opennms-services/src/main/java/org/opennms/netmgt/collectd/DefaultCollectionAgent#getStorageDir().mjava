    /* (non-Javadoc)
     * @see org.opennms.netmgt.collectd.CollectionAgent#getStorageDir()
     */
    /**
     * <p>getStorageDir</p>
     *
     * @return a {@link java.io.File} object.
     */
    @Override
    public File getStorageDir() {
        if (m_storageDir == null) {
            m_storageDir = m_agentService.getStorageDir();
        }
        return m_storageDir;
    }

