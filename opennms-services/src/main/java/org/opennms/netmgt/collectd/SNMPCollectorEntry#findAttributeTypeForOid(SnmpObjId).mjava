    private AttributeType findAttributeTypeForOid(SnmpObjId base) {
        for (Iterator it = m_attrList.iterator(); it.hasNext();) {
            AttributeType attrType = (AttributeType)it.next();
            if (base.equals(attrType.getSnmpObjId()))
                return attrType;
        }
        return null;
    }

