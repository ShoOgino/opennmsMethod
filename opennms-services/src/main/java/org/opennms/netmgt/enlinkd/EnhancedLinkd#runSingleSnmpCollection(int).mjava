    public boolean runSingleSnmpCollection(final int nodeId) {
        boolean allready = true;
        final Node node = m_queryMgr.getSnmpNode(nodeId);

        for (final NodeDiscovery snmpColl : getSnmpCollections(node)) {
            if (!snmpColl.isReady()) {
                allready = false;
                continue;
            }
            snmpColl.runDiscovery();
        }
        return allready;
    }

