	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result
				+ (int) (m_initial_sleep_time ^ (m_initial_sleep_time >>> 32));
		result = prime * result + ((m_node == null) ? 0 : m_node.hashCode());
		result = prime * result
				+ (int) (m_poll_interval ^ (m_poll_interval >>> 32));
		return result;
	}

