    @Override
    public boolean equals(Object obj) {
            if (this == obj)
                    return true;
            if (obj == null)
                    return false;
            if (getClass() != obj.getClass())
                    return false;
            Discovery other = (Discovery) obj;
            if (m_initial_sleep_time != other.m_initial_sleep_time)
                    return false;
            if (m_poll_interval != other.m_poll_interval)
                    return false;
            return true;
    }

