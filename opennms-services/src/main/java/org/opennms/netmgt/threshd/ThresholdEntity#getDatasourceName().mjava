    /**
     * Get datasource name
     */
    public String getDatasourceName() {
        if (getThresholdEvaluatorStates().size() > 0) {
            return getThresholdEvaluatorStates().get(0).getThresholdConfig().getDsName();
        } else {
            throw new IllegalStateException("No thresholds have been added.");
        }
    }

