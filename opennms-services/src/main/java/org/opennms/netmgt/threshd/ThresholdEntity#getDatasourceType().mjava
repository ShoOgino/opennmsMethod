    /**
     * Get datasource type
     */
    public String getDatasourceType() {
        if (getThresholdEvaluatorStates().size() > 0) {
            return getThresholdEvaluatorStates().get(0).getThresholdConfig().getDsType();
        } else {
            throw new IllegalStateException("No thresholds have been added.");
        }
    }

