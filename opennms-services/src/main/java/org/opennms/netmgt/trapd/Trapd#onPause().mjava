    /**
     * Pauses Trapd
     */
    public void onPause() {
        if (m_status != RUNNING) {
            return;
        }

        m_status = PAUSE_PENDING;

        log().debug("pause: Calling pause on processor");

        m_processor.pause();

        log().debug("pause: Processor paused");

        m_status = PAUSED;

        log().debug("pause: Trapd paused");
    }

