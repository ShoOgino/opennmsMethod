    /**
     * Pauses Trapd
     */
    @Override
    public void onPause() {
        if (m_status != RUNNING) {
            return;
        }

        m_status = PAUSE_PENDING;

        LogUtils.debugf(this, "pause: Calling pause on processor");

        m_processor.pause();
        m_status = PAUSED;

        LogUtils.debugf(this, "pause: Trapd paused");
    }

