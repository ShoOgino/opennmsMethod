    /**
     * Resumes Trapd
     */
    @Override
    public void onResume() {
        if (m_status != PAUSED) {
            return;
        }

        m_status = RESUME_PENDING;

        LOG.debug("resume: Calling resume on processor");

        m_status = RUNNING;

        LOG.debug("resume: Trapd resumed");
    }

