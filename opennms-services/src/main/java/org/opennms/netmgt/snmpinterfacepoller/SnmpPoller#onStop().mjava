    /**
     * <p>onStop</p>
     */
    protected void onStop() {
        log().debug("onStop: updating snmp interfaces poll status to 'N'");

        updatesnmpInterface();
        
        if(getScheduler()!=null) {
            log().debug("onStop: stopping scheduler");
            getScheduler().stop();
        }

        setScheduler(null);
    }

