    public static SnmpInterfaceStatus statusFromMibValue(int mibValue) {
        switch(mibValue) {
            case 1:
                return UP;
            case 2:
                return DOWN;
            case 3:
                return TESTING;
            case 4:
                return UNKNOWN;
            case 5:
                return DORMANT;
            case 6:
                return NOT_PRESENT;
            case 7:
                return LOWER_LAYER_DOWN;
            default:
                return INVALID;
        }
    }

