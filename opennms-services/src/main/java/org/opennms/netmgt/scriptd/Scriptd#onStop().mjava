    /**
     * <p>onStop</p>
     */
    @Override
    protected void onStop() {
        try {
            if (m_executor != null) {
                m_executor.stop();
            }
        } catch (Throwable e) {
            LOG.warn("Unexpected throwable when stopping Scriptd", e);
        }

        m_executor = null;

        LOG.info("Scriptd stopped");
    }

