    public static String convertToJsSafeString(String str){
        
        String retStr  = str
        .replace("\\", "\\\\")
        .replace("\"", "\\\"")
        .replace("\t", "\\t")
        .replace("\r", "\\r")
        .replace("\n", "\\n")
        .replace("\b", "\\b");
        
        return retStr;
    }

