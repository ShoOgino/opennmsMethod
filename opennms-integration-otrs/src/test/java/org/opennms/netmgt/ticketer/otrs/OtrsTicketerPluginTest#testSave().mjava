	public void testSave() {
	    
	    Ticket retrievedTicket = null;
		
		try {
            m_ticketer.saveOrUpdate(m_ticket);
            retrievedTicket = m_ticketer.get(m_ticket.getId());
            
        } catch (PluginException e) {
            e.printStackTrace();
        }
		
		assertTicketEquals(m_ticket, retrievedTicket);
		
	}

