    /** {@inheritDoc} */
    public String getVlanClassName(final String sysoid) {
        getReadLock().lock();
        try {
            for (final String oidMask : m_oidMask2VlanclassName.keySet()) {
                if (sysoid.startsWith(oidMask)) {
                    return m_oidMask2VlanclassName.get(oidMask);
                }
            }
        } finally {
            getReadLock().unlock();
        }
        return null;
    }

