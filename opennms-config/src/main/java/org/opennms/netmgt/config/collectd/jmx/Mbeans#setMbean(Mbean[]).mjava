     * 
     * @param vMbeanArray
     */
    public void setMbean(
            final Mbean[] vMbeanArray) {
        //-- copy array
        _mbeanList.clear();
        
        for (int i = 0; i < vMbeanArray.length; i++) {
                this._mbeanList.add(vMbeanArray[i]);
        }
    }

