    /** {@inheritDoc} */
    public void addMonitor(final String svcName, final String className) {
        getWriteLock().lock();
        try {
            final Monitor monitor = new Monitor();
            monitor.setService(svcName);
            monitor.setClassName(className);
            m_config.addMonitor(monitor);
        } finally {
            getWriteLock().unlock();
        }
    }

