    /**
     * <p>getGroups</p>
     *
     * @return a {@link java.util.Map} object.
     */
    @Override
    public Map<String, Group> getGroups() {
        try {
            return m_groupManager.getGroups();
        } catch (Throwable e) {
            throw CONFIG_OBJECT_EXCEPTION_TRANSLATOR.translate("getting groups", e);
        }
    }

