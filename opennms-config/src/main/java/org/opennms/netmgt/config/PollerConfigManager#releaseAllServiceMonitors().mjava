	/**
	 * <p>releaseAllServiceMonitors</p>
	 */
	public void releaseAllServiceMonitors() {
	    getWriteLock().lock();
	    try {
    		Iterator<ServiceMonitor> iter = getServiceMonitors().values().iterator();
    	    while (iter.hasNext()) {
    	        ServiceMonitor sm = iter.next();
    	        sm.release();
    	    }
	    } finally {
	        getWriteLock().unlock();
	    }
	}

