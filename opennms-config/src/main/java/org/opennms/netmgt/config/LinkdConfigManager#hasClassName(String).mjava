    /** {@inheritDoc} */
    public boolean hasClassName(final String sysoid) {
        getReadLock().lock();
        try {
            for (final String oidMask : m_oidMask2VlanclassName.keySet()) {
                if (sysoid.startsWith(oidMask)) {
                    return true;
                }
            }
        } finally {
            getReadLock().unlock();
        }
        return false;
    }

