    /**
     * Whether autodiscovery is enabled in linkd-config (default: false)
     */
    @Override
    public boolean isAutoDiscoveryEnabled() {
        try {
            getReadLock().lock();
            if (m_config.hasAutoDiscovery()) return m_config.getAutoDiscovery();
        } finally {
            getReadLock().unlock();
        }
        return false;
    }

