     * @param vEventArray
     */
    public void setEvent(
            final org.opennms.netmgt.xml.eventconf.Event[] vEventArray) {
        //-- copy array
        _eventList.clear();
        
        for (int i = 0; i < vEventArray.length; i++) {
                this._eventList.add(vEventArray[i]);
        }
    }

