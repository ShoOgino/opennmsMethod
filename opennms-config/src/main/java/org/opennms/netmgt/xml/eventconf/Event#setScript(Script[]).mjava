    /**
     * 
     * 
     * @param vScriptArray
     */
    public void setScript(
            final Script[] vScriptArray) {
        //-- copy array
        m_scriptList.clear();
        
        for (int i = 0; i < vScriptArray.length; i++) {
                this.m_scriptList.add(vScriptArray[i]);
        }
    }

