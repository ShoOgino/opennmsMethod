    /**
     * 
     * 
     * @param vFilterArray
     */
    public void setFilter(
            final org.opennms.netmgt.xml.eventconf.Filter[] vFilterArray) {
        //-- copy array
        _filterList.clear();
        
        for (int i = 0; i < vFilterArray.length; i++) {
                this._filterList.add(vFilterArray[i]);
        }
    }

