    /**
     * 
     * 
     * @param vOperactionArray
     */
    public void setOperaction(
            final Operaction[] vOperactionArray) {
        //-- copy array
        m_operactionList.clear();
        
        for (int i = 0; i < vOperactionArray.length; i++) {
                this.m_operactionList.add(vOperactionArray[i]);
        }
    }

