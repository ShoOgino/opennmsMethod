    /**
     * 
     * 
     * @param vScriptArray
     */
    public void setScript(
            final org.opennms.netmgt.xml.eventconf.Script[] vScriptArray) {
        //-- copy array
        _scriptList.clear();
        
        for (int i = 0; i < vScriptArray.length; i++) {
                this._scriptList.add(vScriptArray[i]);
        }
    }

