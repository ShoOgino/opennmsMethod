    /**
     * 
     * 
     * @param vEventArray
     */
    public void setEvent(
            final Event[] vEventArray) {
        //-- copy array
        m_eventList.clear();
        
        for (int i = 0; i < vEventArray.length; i++) {
                this.m_eventList.add(vEventArray[i]);
        }
    }

