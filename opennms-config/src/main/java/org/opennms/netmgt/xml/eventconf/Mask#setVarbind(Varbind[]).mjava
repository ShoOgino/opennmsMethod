     * 
     * @param vVarbindArray
     */
    public void setVarbind(
            final Varbind[] vVarbindArray) {
        //-- copy array
        m_varbindList.clear();
        
        for (int i = 0; i < vVarbindArray.length; i++) {
                this.m_varbindList.add(vVarbindArray[i]);
        }
    }

