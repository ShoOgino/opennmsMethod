        public StringResource(final String text) throws IOException {
            m_tempFile = File.createTempFile("StringResource", ".txt");
            FileWriter fw = null;
            try {
                fw = new FileWriter(m_tempFile);
                fw.write(text);
                fw.flush();
                fw.close();
            } finally {
                IOUtils.closeQuietly(fw);
            }
        }

