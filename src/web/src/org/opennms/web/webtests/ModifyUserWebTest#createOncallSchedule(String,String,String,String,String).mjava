    private OncallSchedule createOncallSchedule(String role, String type, String day, String begins, String ends) {
        OncallSchedule sched = new OncallSchedule();
        sched.setName(role);
        sched.setType(type);
        Time time = new Time();
        if (day != null)
            time.setDay(day);
        time.setBegins(begins);
        time.setEnds(ends);
        sched.addTime(time);
        return sched;
    }

