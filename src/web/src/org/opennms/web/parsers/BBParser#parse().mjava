	/**
	 * Parse the already set 'm_inpSource' 
	 *
	 * @exception throws java.io.IOException if parse fails for any reason
	 */
	public synchronized void parse() throws IOException
	{
		if (m_inpSource == null)
		{
			throw new IOException("No Input stream to parse!");
		}

		parse(m_inpSource);
	}

