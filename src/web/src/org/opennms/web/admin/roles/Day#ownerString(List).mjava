    private String ownerString(List owners) {
        boolean first = true;
        StringBuffer buf = new StringBuffer();
        for (Iterator it = owners.iterator(); it.hasNext();) {
            Owner owner = (Owner) it.next();
            if (first)
                first = false;
            else 
                buf.append(", ");
            buf.append(owner.getUser());
        }
        return buf.toString();
    }

