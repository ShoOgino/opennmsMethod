	/**
	*/
	public List getEventUEIs()
	{
		if (m_lastModified != m_eventConfFile.lastModified())
		{
			try
			{
				reloadXML();
			}
			catch (Exception e)
			{
				return null;
			}
		}
		
		List eventUEIs = new ArrayList();
		
		for (int i = 0; i < m_events.size(); i++)
		{
			Event curEvent = (Event)m_events.get(i);
			eventUEIs.add( curEvent.getUei());
		}
		
		return eventUEIs;
	}

