    /**
     */
    public Object clone() {
        try {
            super.clone();
        } catch (CloneNotSupportedException e) {
            return null;
        }

        OperatorAction newAction = new OperatorAction();

        newAction.setOperatorAction(m_operatorAction);
        newAction.setMenuText(m_menuText);
        newAction.setState(m_state);

        return newAction;
    }

