    /**
     * constructor
     */

    public MapNode() {
        this.nodeID = -1;
        this.nodeParent = -1;
        this.x = -1;
        this.y = -1;

        this.RTC = 0.0;

        this.hostname = new String();
        this.iconName = new String();
        this.ipAddress = new String();
        this.status = new String();
    }

