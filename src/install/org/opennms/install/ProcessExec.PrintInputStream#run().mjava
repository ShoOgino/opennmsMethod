	public void run() {
	    try {
		BufferedReader in =
		    new BufferedReader(new InputStreamReader(m_inputStream));
		String line;

		while ((line = in.readLine()) != null) {
		    m_printStream.println(line);
		}
	
		m_inputStream.close();
	    } catch (IOException e) {
		e.printStackTrace();
		try {
		    m_inputStream.close();
		} catch (IOException e2) { } // do nothing
	    }
	}

