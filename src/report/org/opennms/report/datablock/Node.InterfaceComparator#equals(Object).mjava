		public boolean equals(Object o)
		{
                        boolean rc = false;
                        if(o != null)
                        {
                               	if(o == this)
                                      	rc = true;
                               	else if(o instanceof Interface)
                                       	rc = m_intfname.equals(((Interface)o).getName());
                               	else if(o instanceof String)
                               		rc = m_intfname.equals(o);
                        }
                        return rc;
		}

