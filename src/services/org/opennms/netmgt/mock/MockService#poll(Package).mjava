    // test
    public PollStatus poll(Package pkg) {
        m_pollCount++;
        m_pollingPkgNames.add(pkg.getName());
        
        Iterator it = m_triggers.iterator();
        while (it.hasNext()) {
            PollAnticipator trigger = (PollAnticipator) it.next();
            trigger.poll(this);
        }

        return getPollStatus();

    }

