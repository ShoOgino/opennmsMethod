    private void doExecute(Object values[]) throws SQLException {
        
        Connection conn = m_db.getConnection();
         try {
             PreparedStatement stmt = conn.prepareStatement(m_sql);
             for(int i = 0; i < values.length; i++) {
                 stmt.setObject(i+1, values[i]);
             }
             executeStmt(stmt);
         } finally {
             conn.close();
         }
    }

