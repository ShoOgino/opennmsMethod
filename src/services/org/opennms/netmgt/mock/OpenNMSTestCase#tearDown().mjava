    protected void tearDown() throws Exception {
        if(m_runSupers) {
            if (isStartEventd()) m_eventd.stop();
            m_db.drop();
        }
        assertTrue("Unexpected WARN or ERROR msgs in Log!", MockUtil.noWarningsOrHigherLogged());
        MockUtil.println("------------ End Test "+getName()+" --------------------------");

        super.tearDown();
    }

