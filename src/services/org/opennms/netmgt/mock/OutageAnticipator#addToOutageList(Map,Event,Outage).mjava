    /**
     * @param outageMap
     * @param outageEvent
     * @param svc
     */
    protected void addToOutageList(Map outageMap, Event outageEvent, Outage outage) {
        EventWrapper w = new EventWrapper(outageEvent);
        List list = (List)outageMap.get(w);
        if (list == null) {
            list = new LinkedList();
            outageMap.put(w, list);
        }
        list.add(outage);
    }

