    /**
     * 
     */
    public List getEvents(String uei) {
        List events = new ArrayList();

        for (int i = 0; i < m_events.size(); i++) {
            Event curEvent = (Event) m_events.get(i);

            if (curEvent.getUei().equals(uei)) {
                events.add(curEvent);
            }
        }

        if (events.size() > 0)
            return events;
        else
            return null;
    }

