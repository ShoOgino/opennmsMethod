    private String toString(List users) {
        StringBuffer buf = new StringBuffer("[");
        boolean first = true;
        for (Iterator it = users.iterator(); it.hasNext();) {
            User user = (User) it.next();
            if (first)
                first = false;
            else
                buf.append(" ");
            buf.append(user.getUserId());
                
                    
        }
        buf.append("]");
        return buf.toString();
    }

