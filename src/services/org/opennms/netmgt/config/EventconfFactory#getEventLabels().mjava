        /**
        */
        public Map getEventLabels()
        {
                TreeMap eventLabels = new TreeMap();
                
                for (int i = 0; i < m_events.size(); i++)
		{
			Event curEvent = (Event)m_events.get(i);
			eventLabels.put( curEvent.getUei(), curEvent.getEventLabel());
		}
                
                return eventLabels;
        }

