	/**
	 * Singleton static call to get the only instance that should exist for the EventconfFactory
	 * @return the single eventconf factory instance
	 */
	static synchronized public EventconfFactory getInstance()
	{
		if (!initialized)
			return null;
		
		if (instance == null)
		{
			instance = new EventconfFactory();
		}
		
		return instance;
	}

