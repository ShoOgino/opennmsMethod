    /**
     */
    public List getUserNames() throws IOException, MarshalException, ValidationException {
        updateFromFile();

        try {
            reload();
        } catch (Exception e) {
            return null;
        }

        List userNames = new ArrayList();

        Iterator i = m_users.keySet().iterator();

        while (i.hasNext()) {
            userNames.add((String) i.next());
        }

        return userNames;
    }

