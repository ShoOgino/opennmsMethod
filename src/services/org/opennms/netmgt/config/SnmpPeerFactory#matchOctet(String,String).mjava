    public static boolean matchOctet(String hostOctet, String patternOctet) {
        
        String patternList[] = patternOctet.split(",", 0);
        for (int i = 0; i < patternList.length; i++) {
            if (matchRange(hostOctet, patternList[i]))
                return true;
        }
        return false;
    }

