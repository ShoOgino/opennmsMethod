    public void testUnschedule() {
        m_sched.schedule();
        
        assertRunAndScheduled(0, 0, 0, 1);

        m_timer.next();

        assertRunAndScheduled(0, 1000, 1, 1);

        m_timer.next();
        
        assertRunAndScheduled(1000, 1000, 2, 1);
        
        m_sched.unschedule();
        
        // jump to the expired entry
        m_timer.next();
        
        assertRunAndScheduled(2000, -1, 2, 0);
    }

