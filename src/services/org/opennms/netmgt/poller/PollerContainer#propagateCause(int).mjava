    /**
     * @param dbid
     */
    private void propagateCause(final int cause) {
        PollerVisitor causePropagator = new PollerVisitorAdaptor() {
            public void visitElement(PollerElement elem) {
                if (elem.getCause() < 0)
                    elem.setCause(cause);
            }
        };
        this.visit(causePropagator);
    }

