	public PollableNode getNode(int nodeId)
	{
		Category log = ThreadCategory.getInstance();
		
		Integer key = new Integer(nodeId);
		boolean nodeInMap = m_pollableNodes.containsKey(key);
		
		if (nodeInMap)
		{
			return (PollableNode)m_pollableNodes.get(key);
		}
		else
		{
			return null;
		}
	}

