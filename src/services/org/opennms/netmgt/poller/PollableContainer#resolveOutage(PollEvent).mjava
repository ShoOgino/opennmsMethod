    protected void resolveOutage(final PollEvent resolution) {
        if (hasOpenOutage()) {
            Iter iter = new Iter() {
                public void forEachElement(PollableElement member) {
                    member.resolveOutage(resolution);
                }
                
            };
            forEachMember(iter);
        }
        super.resolveOutage(resolution);
    }

