    /**
     * @param nodeId
     * @param poller
     * @return
     */
    public PollableNode findNode(int nodeId) {
        Integer key = new Integer(nodeId);
        if (m_pollableNodes.containsKey(key)) {
            return (PollableNode) m_pollableNodes.get(key);
        } else {
            return null;
        }
    }

