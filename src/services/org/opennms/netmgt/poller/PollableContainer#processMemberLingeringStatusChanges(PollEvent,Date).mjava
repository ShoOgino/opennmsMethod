    /**
     * @param date
     */
    private void processMemberLingeringStatusChanges(final PollEvent cause, final Date date) {
        Iter iter = new Iter() {
            public void forEachElement(PollableElement elem) {
                elem.processLingeringStatusChanges(cause, date);
            }
            
        };
        forEachMember(iter);
        
    }

