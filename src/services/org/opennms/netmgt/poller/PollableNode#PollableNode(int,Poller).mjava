    /**
     * Constructor.
     */
    public PollableNode(int nodeId, Poller poller) {
        super(PollStatus.STATUS_UNKNOWN);
        m_poller = poller;

        m_nodeId = nodeId;
        m_interfaces = Collections.synchronizedMap(new HashMap());

        m_lock = new Object();
        m_isLocked = false;

        m_statusChangedFlag = false;

        m_isDeleted = false;
    }

