    public void close() {
        try {
            m_getSvcIdStmt.close();
            m_getHostNameStmt.close();
            m_getNextIdStmt.close();
            m_insStmt.close();
        } catch (SQLException sqle) {
            ThreadCategory.getInstance(EventWriter.class).warn("SQLException while closing prepared statements", sqle);
        } finally {
            super.close();
        }
    }

