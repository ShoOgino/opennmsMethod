    public void execute(Object values[]) {
         try {
             doExecute(values);
         } catch (SQLException e) {
             String vals = "[";
             for(int i = 0; i < values.length; i++) {
                 if (i != 0)
                     vals += ", ";
                 vals += values[i];
             }
             vals += "]";
             throw new RuntimeException("Problem executing statement: "+m_sql+" with values "+vals, e);
         }
     }

