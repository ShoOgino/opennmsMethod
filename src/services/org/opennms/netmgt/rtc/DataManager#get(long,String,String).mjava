    /**
     * Get the rtcnode with this nodeid/ip/svcname.
     * 
     * @param nodeid
     *            the node id
     * @param ip
     *            the interface
     * @param svcName
     *            the service
     */
    public synchronized RTCNode get(long nodeid, String ip, String svcName) {
        RTCNodeKey key = new RTCNodeKey(nodeid, ip, svcName);
        return (RTCNode) m_map.get(key);
    }

