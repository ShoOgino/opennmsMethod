	boolean updateOS(String newos)
	{
		boolean doUpdate = false;
		if (newos != null && m_os != null)
		{
			if (!newos.equals(m_os))
				doUpdate = true;
		}
		else if (newos == null && m_os == null)
		{
			// do nothing
		}
		else 
			// one is null the other isn't, do the update
			doUpdate = true;
			
		if (doUpdate)
		{
			setOS(newos);
			return true;
		}
		else
			return false;
	}

