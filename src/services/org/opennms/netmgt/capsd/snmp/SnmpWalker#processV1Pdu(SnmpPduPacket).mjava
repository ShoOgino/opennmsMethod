    private boolean processV1Pdu(SnmpPduPacket pdu) {
        
        if (isV1Finished(pdu)) {
            return true;
        } else {
            if (log().isDebugEnabled())
                log().debug(getName()+": snmpReceivedPdu: got SNMPv1 response and still within "+getName()+", creating new entry.");
            update(pdu.toVarBindArray());
            return false;
        }
    }

