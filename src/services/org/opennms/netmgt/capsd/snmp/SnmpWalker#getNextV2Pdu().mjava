    public SnmpPduPacket getNextV2Pdu() {
        SnmpPduPacket pdu;
        pdu = new SnmpPduBulk();
        ((SnmpPduBulk) pdu).setMaxRepititions(getMaxRepeaters());
        pdu.setRequestId(SnmpPduPacket.nextSequence());
        SnmpObjectId oid = getRootOfTable();
        pdu.addVarBind(new SnmpVarBind(oid));
        return pdu;
    }

