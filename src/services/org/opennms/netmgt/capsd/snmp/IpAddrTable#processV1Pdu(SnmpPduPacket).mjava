    private boolean processV1Pdu(SnmpPduPacket pdu) {
        
        if (isV1Finished(pdu)) {
            return true;
        } else {
            if (log().isDebugEnabled())
                log().debug("snmpReceivedPdu: got SNMPv1 response and still within IpAddrTable, creating new entry.");
            processTableRow(pdu.toVarBindArray());
            return false;
        }
    }

