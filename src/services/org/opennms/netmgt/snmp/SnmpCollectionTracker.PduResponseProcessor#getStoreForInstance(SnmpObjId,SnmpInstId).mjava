        private SnmpStore getStoreForInstance(SnmpObjId baseOid, SnmpInstId inst) {
            SnmpStore instMap = (SnmpStore)m_instanceMaps.get(inst);
            if (instMap == null) {
                instMap = createStoreForInstance(baseOid, inst);
                m_instanceMaps.put(inst, instMap);
            }
            return instMap;
        }

