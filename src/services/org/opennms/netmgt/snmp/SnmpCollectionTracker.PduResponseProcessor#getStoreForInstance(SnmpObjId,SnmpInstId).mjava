        private Map getStoreForInstance(SnmpObjId baseOid, SnmpInstId inst) {
            Map instMap = (Map)m_instanceMaps.get(inst);
            if (instMap == null) {
                instMap = createStoreForInstance(baseOid, inst);
                m_instanceMaps.put(inst, instMap);
            }
            return instMap;
        }

