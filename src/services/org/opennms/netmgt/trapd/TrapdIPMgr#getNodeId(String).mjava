    /**
     * Returns the nodeid for the IP Address
     * 
     * @param addr
     *            The IP Address to query.
     * 
     * @return The node ID of the IP Address if known.
     * 
     */
    static synchronized String getNodeId(String addr) {
        if (addr == null)
            return null;
        return (String) m_knownips.get(addr);
    }

