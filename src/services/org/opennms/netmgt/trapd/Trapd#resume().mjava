    /**
     * Resumes Trapd
     */
    public void resume() {
        if (m_status != PAUSED)
            return;

        m_status = RESUME_PENDING;

        Category log = ThreadCategory.getInstance(getClass());

        if (log.isDebugEnabled())
            log.debug("Calling resume on processor");

        m_processor.resume();

        if (log.isDebugEnabled())
            log.debug("Processor resumed");

        m_status = RUNNING;

        if (log.isDebugEnabled())
            log.debug("Trapd resumed");
    }

