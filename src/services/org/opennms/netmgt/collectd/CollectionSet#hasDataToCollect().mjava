	boolean hasDataToCollect() {
        if (!getNodeInfo().getAttributeList().isEmpty()) return true;
        
        Iterator iter = getIfMap().values().iterator();
        while (iter.hasNext()) {
            IfInfo ifInfo = (IfInfo) iter.next();
            if (!ifInfo.getOidList().isEmpty()) {
                return true;
            }
        }

        return false;
	}

