    private void logExceptionOnUpdate(DataSource ds1, IllegalArgumentException e1) {
        log().warn("getRRDValue: " + e1.getMessage());
        log().warn(
        		"updateRRDs: call to getRRDValue() failed "
        				+ "for node: " + m_agent.getNodeId() + " datasource: "
        				+ ds1.getName());
    }

