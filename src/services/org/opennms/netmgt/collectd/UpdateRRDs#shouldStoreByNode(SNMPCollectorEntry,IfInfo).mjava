    private boolean shouldStoreByNode(SNMPCollectorEntry ifEntry, IfInfo ifInfo) {
        String byNode = getStoreByNodeID();
        if (!ifInfo.isScheduledForCollection()) {
            if (byNode.equals("normal")) {
                byNode = "false";
            }                    
        } else {
            if (byNode.equals("normal")) {
                byNode = "true";
            }
        }
        
        return byNode.equals("true");
    }

