    private void logSkip(SNMPCollectorEntry ifEntry, IfInfo ifInfo) {
        if (log().isDebugEnabled()) {
        	log()
        			.debug(
        					"updateRRDs: selectively storing "
        							+ "SNMP data for primary interface ("
        							+ m_agent.getIfIndex()
        							+ "), skipping ifIndex: "
        							+ Integer.toString(ifEntry.getIfIndex().intValue())
        							+ " because collType = "
        							+ ifInfo.getCollType());
        }
    }

