    private void logUpdateFailed(SNMPCollectorEntry ifEntry, DataSource ds) {
        log()
        		.warn(
        				"updateRRDs: "
        						+ "ds.performUpdate() failed for "
        						+ "node/ifindex: "
        						+ m_agent.getNodeId() + "/"
        						+ ifEntry.getIfIndex().intValue()
        						+ " datasource: "
        						+ ds.getName());
    }

