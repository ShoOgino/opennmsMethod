    private void logSkipNonPrimary(SNMPCollectorEntry ifEntry) {
        if (log().isDebugEnabled()) {
        	log()
        			.debug(
        					"updateRRDs: only storing "
        							+ "SNMP data for primary interface ("
        							+ m_agent.getIfIndex()
        							+ "), skipping ifIndex: "
        							+ Integer.toString(ifEntry.getIfIndex().intValue()));
        }
    }

