    void logExceptionOnUpdate(DataSource ds1, IllegalArgumentException e1) {
        Category log = log();
        log.warn("getRRDValue: " + e1.getMessage());
        log.warn(
        		"updateRRDs: call to getRRDValue() failed "
        				+ "for node: " + getCollectionAgent().getNodeId() + " datasource: "
        				+ ds1.getName());
    }

