	private void initSnmpPeerFactory() {
		try {
			SnmpPeerFactory.init();
		} catch (MarshalException e) {
			log().fatal("initialize: Failed to load SNMP configuration", e);
			throw new UndeclaredThrowableException(e);
		} catch (ValidationException e) {
			log().fatal("initialize: Failed to load SNMP configuration", e);
			throw new UndeclaredThrowableException(e);
		} catch (IOException e) {
			log().fatal("initialize: Failed to load SNMP configuration", e);
			throw new UndeclaredThrowableException(e);
		}
	}

