    /**
     * This simulates send a packet and waiting for a response 
     * @param pdu
     * @return
     */
    public TestPdu send(TestPdu pdu) {
        switch(pdu.getType()) {
        case TestPdu.GET_REQ:
            return sendGet(pdu);
        case TestPdu.NEXT_REQ:
            return sendNext(pdu);
        case TestPdu.BULK_REQ:
            return sendBulk(pdu);
        default:
            return sendError(pdu);
        }
        
    }

