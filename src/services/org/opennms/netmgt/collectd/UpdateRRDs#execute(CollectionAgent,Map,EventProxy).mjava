    void execute(CollectionAgent agent, Map parms, EventProxy eproxy) {
        m_agent = agent;
        m_parameters = parms;
        m_eventProxy = eproxy;
        
        /*
    	 * Write relevant collected SNMP statistics to RRD database First the
    	 * node level RRD info will be updated. Secondly the interface level RRD
    	 * info will be updated.
    	 */
    
        // Node data
        storeNodeData();
    
    	m_forceRescan = false;
        m_rescanPending = false;

        saveInterfaceData();
    
    	if (m_forceRescan) {
    		m_agent.sendForceRescanEvent(m_eventProxy);
    	}
    }

