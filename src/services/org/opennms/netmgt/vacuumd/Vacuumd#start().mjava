    /* (non-Javadoc)
     * @see org.opennms.core.fiber.Fiber#start()
     */
    public void start() {
        m_status = STARTING;
        m_startTime = System.currentTimeMillis();
        m_thread = new Thread(this, "Vacuumd-Thread");
        m_thread.start();
        m_status = RUNNING;

    }

