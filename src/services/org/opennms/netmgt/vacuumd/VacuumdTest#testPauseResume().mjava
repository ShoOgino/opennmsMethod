    public final void testPauseResume() throws InterruptedException {
        
        Thread.sleep(200);
        assertEquals(Fiber.RUNNING, m_vacuumd.getStatus());
        
        m_vacuumd.pause();
        assertEquals(PausableFiber.PAUSED, m_vacuumd.getStatus());
        
        m_vacuumd.resume();
        Thread.sleep(500);
        assertEquals(Fiber.RUNNING, m_vacuumd.getStatus());
    }

