    private void scheduleAutomation(Automation auto) {
        
        if (auto.getActive()) {
            AutomationProcessor ap = new AutomationProcessor();
            ap.setAutomation(auto);
            Schedule s = new Schedule(ap, new AutomationInterval(auto.getInterval()), m_scheduler);
            ap.setSchedule(s);
            s.schedule();
        }
    }

