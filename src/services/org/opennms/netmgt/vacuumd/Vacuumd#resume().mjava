    /* (non-Javadoc)
     * @see org.opennms.core.fiber.PausableFiber#resume()
     */
    public void resume() {
         if (m_status != PAUSED)
            return;
        
         ThreadCategory.setPrefix(LOG4J_CATEGORY);
         Category log = ThreadCategory.getInstance(getClass());
         log.info("Resuming Vacuumd");

         m_thread = new Thread(this, "Vacuumd-Thread");
        m_thread.start();
    }

