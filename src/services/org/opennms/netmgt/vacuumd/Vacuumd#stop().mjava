    /*
     * (non-Javadoc)
     * 
     * @see org.opennms.core.fiber.Fiber#stop()
     */
    public void stop() {
        ThreadCategory.setPrefix(LOG4J_CATEGORY);
        Category log = ThreadCategory.getInstance(getClass());
        log.info("Stopping Vacuumd");

        setStatus(STOP_PENDING);
        m_stopped = true;
        setStatus(STOPPED);
    }

