    /**
     * Returns the nodeid for the IP Address
     * 
     * @param addr
     *            The IP Address to query.
     * 
     * @return The node ID of the IP Address if known.
     * 
     */
    static synchronized long getNodeId(String addr) {
        if (addr == null)
            return -1;
        return longValue((Long)m_knownips.get(addr));
    }

