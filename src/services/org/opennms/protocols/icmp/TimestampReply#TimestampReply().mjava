    /**
     * Creates a new ICMP Timestamp Reply object.
     * 
     */
    public TimestampReply() {
        super(ICMPHeader.TYPE_TIMESTAMP_REPLY, (byte) 0);
        m_origStamp = (int) ((new Date()).getTime() & 0xffffffff);
        m_recvStamp = m_origStamp;
        m_xmitStamp = m_origStamp;
    }

