	/**
	 * Creates an object identifier from the pased
	 * dotted decimal object identifier string. The
	 * string is converted to the internal representation.
	 * If the conversion fails then a default (.0.0) object
	 * identifier is assigned to the object.
	 *
	 * @param strOid The dotted decimal object identifier string
	 *
	 */
	public SnmpObjectId(String strOid)
	{
		m_data = convert(strOid);
		if(m_data == null)
		{
			m_data = new int[2];
			m_data[0] = m_data[1] = 0;
		}
	}

