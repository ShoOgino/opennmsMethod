    /**
     * Sets the object to the passed object identifier
     * 
     * @param data
     *            The new object identifier
     * 
     */
    public void setIdentifiers(int[] data) {
        if (data != null) {
            m_data = new int[data.length];
            System.arraycopy(data, 0, m_data, 0, data.length);
        } else {
            m_data = new int[2];
            m_data[0] = m_data[1] = 0;
        }
    }

