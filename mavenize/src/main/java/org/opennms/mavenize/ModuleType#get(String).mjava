    public static ModuleType get(String moduleType) {
        try {
            return (ModuleType)(new Reflector()).newInstance(Configuration.get().getClass("moduleType."+moduleType+".class", ModuleType.class), new Object[] { moduleType });
        } catch (ReflectorException e) {
            throw new RuntimeException("Unable to create moduleType class for "+moduleType, e);
        }
    }

