    private Variable findValueForOID(final OID oid) throws SnmpErrorStatusException {
    	final Object val = m_vars.get(oid);
        if (val == null) {
            return null;
        } else if (val instanceof Variable) {
            return (Variable)val;
        }
        return getVariableFromValueString(oid.toString(), (String)val);
    }

