    public void save(OnmsServiceType serviceType) {
        if (serviceType.getId() != null)
            throw new IllegalArgumentException("unable to save service that already has an id"+serviceType.getId());
        
        serviceType.setId(allocateId());
        getJdbcTemplate().update("insert into service (serviceName, serviceId) values (?, ?)", new Object[] { serviceType.getName(), serviceType.getId() });

    }

