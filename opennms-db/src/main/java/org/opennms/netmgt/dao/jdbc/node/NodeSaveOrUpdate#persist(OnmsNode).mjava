    public int persist(OnmsNode node) {
        Object[] parms = new Object[] {
                node.getDistPoller().getName(), // dpName
                node.getCreateTime(),           // nodeCreateTime
                getParentId(node),             // nodeParentID
                node.getType(),                 // nodeType
                node.getSysObjectId(),          // nodeSysOid
                node.getSysName(),              // nodeSysName
                node.getSysDescription(),       // nodeSysDescription
                node.getSysLocation(),          // nodeSysLocation
                node.getSysContact(),           // nodeSysContact
                node.getLabel(),                // nodeLabel
                node.getLabelSource(),          // nodeLabelSource
                node.getNetBiosName(),          // nodeNetBiosName
                node.getNetBiosDomain(),        // nodeDomainName
                node.getOperatingSystem(),      // operationsSystem
                node.getLastCapsdPoll(),        // lastCapsdPoll
                node.getId(),                   // nodeId
        };
        return update(parms);
    }

