    /**
     * Instantiates a new Syslog destination.
     *
     * @param name the name
     * @param protocol the protocol
     * @param facility the facility
     */
    public SyslogDestination(String name, SyslogProtocol protocol, SyslogFacility facility) {
        m_destinationName = name;
        m_protocol = protocol;
        m_facility = facility;
    }

