    public void setConfig(SyslogNorthbounderConfig config) {
    	if (config == null) {
    		throw new IllegalStateException("Configuration cannot be set to null!");
    	}
    	
		synchronized (m_configLock) {
			m_config = config;
    		Syslog.shutdown();
    		Syslog.initialize();
    		createNorthboundInstances();
		}
    }

