    /**
     * Pass filter.
     *
     * @param alarm the alarm
     * @return true, if successful
     */
    public boolean passFilter(NorthboundAlarm alarm) {
        if (m_filters != null) {
            for (SyslogFilter filter : m_filters) {
                if (filter.passFilter(alarm)) {
                    return true;
                }
            }
        }
        return false;
    }

