    /**
     * Instantiates a new northbound alarm.
     *
     * @param id the ID
     * @param uei the UEI
     */
    private NorthboundAlarm(int id, String uei) {
        // I only set these for the 'special event'
        m_id = id;
        m_uei = uei;

        m_nodeId = null;
        m_ackTime = null;
        m_ackUser = null;
        m_alarmType = null;
        m_appDn = null;
        m_clearKey = null;
        m_count = null;
        m_desc = null;
        m_poller = null;
        m_firstOccurrence = null;
        m_ipAddr = null;
        m_lastOccurrence = null;
        m_logMsg = null;
        m_objectInstance = null;
        m_objectType = null;
        m_operInst = null;
        m_ossKey = null;
        m_ossState = null;
        m_alarmKey = null;
        m_service = null;
        m_severity = null;
        m_suppressed = null;
        m_suppressedUntil = null;
        m_suppressedBy = null;
        m_ticketId = null;
        m_ticketState = null;
        m_x733Type = null;
        m_x733Cause = -1;
    }

