    /**
     * Creates the alarm.
     *
     * @return the northbound alarm
     */
    private NorthboundAlarm createAlarm() {
        OnmsAlarm alarm = new OnmsAlarm();
        alarm.setId(++alarmNumber);
        alarm.setUei("uei.opennms.org/test/httpNorthBounder");

        return new NorthboundAlarm(alarm);
    }

