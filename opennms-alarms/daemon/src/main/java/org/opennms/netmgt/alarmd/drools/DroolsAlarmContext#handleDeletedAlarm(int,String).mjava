    @Override
    public void handleDeletedAlarm(int alarmId, String reductionKey) {
        if (!isStarted()) {
            LOG.debug("Ignoring deleted alarm. Drools session is stopped.");
            return;
        }
        getLock().lock();
        try {
            handleDeletedAlarmNoLock(alarmId, reductionKey);
        } finally {
            getLock().unlock();
        }
    }

