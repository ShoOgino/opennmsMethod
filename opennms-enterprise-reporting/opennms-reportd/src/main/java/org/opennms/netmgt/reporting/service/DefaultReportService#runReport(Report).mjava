    public void runReport(Report report) {
        try {
            runAndRender(report);
            
            
        } catch (JRException e) {
            LogUtils.errorf(this, "error running report: %s",e.getMessage());
            e.printStackTrace();
        }  catch (Exception e){
            LogUtils.errorf(this, "Exception: %s", e.getMessage());
        }
    }

